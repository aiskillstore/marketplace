  "content": {
    "user_title": "Automate Scientific Research Workflows",
    "value_statement": "Scientific research requires managing multiple complex stages from hypothesis generation to publication. Denario orchestrates AI agents to automate this pipeline, generating research ideas, developing methodologies, executing analyses, and producing publication-ready LaTeX papers.",
    "seo_keywords": [
      "denario",
      "scientific research automation",
      "AI research assistant",
      "multiagent system",
      "Claude research tool",
      "LaTeX paper generation",
      "research workflow automation",
      "hypothesis generation",
      "data analysis pipeline",
      "Claude Code research"
    ],
    "actual_capabilities": [
      "Generate research hypotheses from dataset descriptions using AI agents",
      "Develop structured research methodologies with statistical approaches",
      "Execute computational experiments and generate visualizations",
      "Produce publication-ready LaTeX papers formatted for academic journals",
      "Orchestrate multi-stage research pipelines from data to publication",
      "Support hybrid workflows with manual and automated stages"
    ],
    "limitations": [
      "Requires external LLM API keys (OpenAI, Google Vertex AI) for operation",
      "Does not perform live literature searches (documentation references only)",
      "Paper generation requires LaTeX installation or Docker for PDF output",
      "Computational execution depends on available Python libraries in environment"
    ],
    "use_cases": [
      {
        "target_user": "Academic Researchers",
        "title": "Accelerate Research Publications",
        "description": "Automate the journey from raw data to formatted journal submissions using structured AI-assisted workflows."
      },
      {
        "target_user": "Data Scientists",
        "title": "Systematic Analysis Pipelines",
        "description": "Transform exploratory data analysis into reproducible research pipelines with automated documentation."
      },
      {
        "target_user": "Graduate Students",
        "title": "Learn Research Methodology",
        "description": "Generate structured research approaches and see example methodologies for learning scientific writing."
      }
    ],
    "prompt_templates": [
      {
        "title": "Basic Research Setup",
        "scenario": "Start a research project",
        "prompt": "Use Denario to set up a research project in ./my_research with data description: [describe your dataset and tools available]. Then generate a research idea."
      },
      {
        "title": "Full Pipeline Execution",
        "scenario": "Complete research workflow",
        "prompt": "Run a complete Denario research pipeline: Set data description for [domain], generate idea, develop methodology, execute results, and create a paper formatted for Journal.APS."
      },
      {
        "title": "Custom Research Direction",
        "scenario": "Provide specific hypothesis",
        "prompt": "Use Denario with custom research idea: [your hypothesis]. Set data description as [describe available data]. Let Denario develop the methodology and generate results."
      },
      {
        "title": "Paper from Existing Work",
        "scenario": "Format completed analysis",
        "prompt": "Use Denario to generate a publication-ready paper from my existing research. Provide: data description, research idea, methodology file at [path], and results file at [path]. Format for APS journal."
      }
    ],
    "output_examples": [
      {
        "input": "Use Denario to analyze climate temperature data and generate a research paper. Data: global temperatures 1880-2023, tools: pandas, sklearn, matplotlib.",
        "output": [
          "Generated research idea: Quantify the rate of global temperature increase using linear regression",
          "Created methodology with time-series preprocessing and statistical testing",
          "Executed analysis yielding +0.18Â°C per decade trend (p < 0.001)",
          "Created 3 figures: temperature anomaly plot, decadal averages, acceleration analysis",
          "Generated paper.tex formatted for APS journal with abstract, methods, results, and references"
        ]
      }
    ],
    "best_practices": [
      "Provide detailed data descriptions including format, size, known characteristics, and available tools for better generated ideas",
      "Review and refine intermediate outputs (idea.md, methodology.md) before proceeding to next stages",
      "Use version control by committing after each pipeline stage to track research evolution"
    ],
    "anti_patterns": [
      "Do not skip the data description stage - detailed context is essential for quality AI-generated research",
      "Do not ignore methodology review - always validate the generated approach before executing computations",
      "Do not proceed without verifying API keys are properly configured - credential errors will halt execution"
    ],
    "faq": [
      {
        "question": "What LLM providers does Denario support?",
        "answer": "Denario supports OpenAI (GPT-4, GPT-3.5), Google Vertex AI (Gemini, PaLM), and any provider compatible with AG2 or LangGraph frameworks."
      },
      {
        "question": "Do I need LaTeX installed for paper generation?",
        "answer": "Yes for PDF output. Install texlive-full on Linux, MacTeX on macOS, or MiKTeX on Windows. Alternatively, use the Docker image with pre-installed LaTeX."
      },
      {
        "question": "Can I use my own research idea instead of generating one?",
        "answer": "Yes. Use den.set_idea(\"Your research hypothesis\") to provide a custom idea. Denario will then develop the methodology and execute from there."
      },
      {
        "question": "What journal formats are supported?",
        "answer": "Denario includes APS (American Physical Society) format. Additional journal formats may be available. Check documentation for the complete list."
      },
      {
        "question": "How does Denario handle API credentials securely?",
        "answer": "Credentials are read from environment variables or .env files. The documentation includes security best practices for credential management."
      },
      {
        "question": "Can I use Denario with pre-computed results?",
        "answer": "Yes. Use den.set_results(\"path/to/results.md\") or den.set_method(\"path/to/methodology.md\") to provide existing work. Denario will format these into a paper."
      }
    ]
