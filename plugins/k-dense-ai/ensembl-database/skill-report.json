  "content": {
    "user_title": "Query Ensembl Genome Database",
    "value_statement": "Access comprehensive vertebrate genomic data from Ensembl database. Retrieve gene information, sequences, variants, and perform comparative genomics across 250+ species. Essential for genomic research and bioinformatics workflows.",
    "seo_keywords": [
      "Ensembl database",
      "genomics research",
      "gene lookup",
      "sequence retrieval",
      "variant analysis",
      "Claude Code",
      "bioinformatics API",
      "comparative genomics",
      "VEP predictions",
      "ortholog search"
    ],
    "actual_capabilities": [
      "Look up gene information by symbol or Ensembl ID across 250+ species",
      "Retrieve genomic, transcript, and protein sequences in JSON or FASTA format",
      "Analyze genetic variants using Variant Effect Predictor (VEP)",
      "Find orthologs and paralogs for cross-species comparative genomics",
      "Map coordinates between genome assemblies (GRCh37 to GRCh38)",
      "Query regulatory features and phenotype annotations"
    ],
    "limitations": [
      "Rate limited to 15 requests per second for anonymous users",
      "Only queries Ensembl database; does not access other genomic resources",
      "No local data storage; all queries require network access to Ensembl REST API"
    ],
    "use_cases": [
      {
        "target_user": "Genomic Researchers",
        "title": "Gene Annotation Pipeline",
        "description": "Look up genes, retrieve transcripts, get protein sequences, and export results for downstream analysis."
      },
      {
        "target_user": "Clinical Geneticists",
        "title": "Variant Effect Analysis",
        "description": "Query variants by rsID, predict functional consequences with VEP, and retrieve phenotype associations."
      },
      {
        "target_user": "Evolutionary Biologists",
        "title": "Cross-Species Comparison",
        "description": "Find orthologs across species, compare gene structures, and analyze evolutionary conservation patterns."
      }
    ],
    "prompt_templates": [
      {
        "title": "Basic Gene Lookup",
        "scenario": "Find gene information",
        "prompt": "Look up the gene TP53 in human using the Ensembl database and return its Ensembl ID, chromosomal location, and description."
      },
      {
        "title": "Sequence Retrieval",
        "scenario": "Get DNA sequence",
        "prompt": "Retrieve the genomic sequence for BRCA2 gene including 500 base pairs of flanking region in FASTA format."
      },
      {
        "title": "Variant Analysis",
        "scenario": "Predict variant effects",
        "prompt": "Analyze the variant rs699 using VEP and show its predicted functional consequences and population frequencies."
      },
      {
        "title": "Comparative Genomics",
        "scenario": "Find orthologs",
        "prompt": "Find all mouse orthologs for human gene ENSG00000139618 (BRCA2) and show their Ensembl IDs and gene symbols."
      }
    ],
    "output_examples": [
      {
        "input": "Look up gene BRCA2 in human",
        "output": [
          "Gene Symbol: BRCA2",
          "Ensembl ID: ENSG00000139618",
          "Chromosome: 13",
          "Location: 32,315,147-32,400,586 (GRCh38)",
          "Description: BRCA2 DNA repair associated"
        ]
      }
    ],
    "best_practices": [
      "Use batch endpoints when querying multiple genes or variants to reduce API calls",
      "Implement rate limiting and respect Retry-After headers to avoid being blocked",
      "Cache frequently accessed data locally to minimize repeated API requests"
    ],
    "anti_patterns": [
      "Making individual requests in a loop without rate limiting",
      "Querying outdated genome assemblies without specifying the correct server",
      "Ignoring error responses and failing to handle 404 or 429 status codes"
    ],
    "faq": [
      {
        "question": "What species are supported?",
        "answer": "Ensembl covers 250+ species including human, mouse, zebrafish, fruit fly, and many vertebrates."
      },
      {
        "question": "How do I query the older GRCh37 assembly?",
        "answer": "Use the GRCh37-specific server at https://grch37.rest.ensembl.org for hg19 queries."
      },
      {
        "question": "What is the rate limit?",
        "answer": "15 requests per second for anonymous users, 55,000 per hour for registered users."
      },
      {
        "question": "Can I retrieve protein sequences?",
        "answer": "Yes, use the sequence endpoint with type=protein or cdna to retrieve protein or transcript sequences."
      },
      {
        "question": "How does VEP work?",
        "answer": "VEP predicts functional consequences of variants including impact on proteins and regulatory elements."
      },
      {
        "question": "Are results available in FASTA format?",
        "answer": "Yes, most sequence endpoints support FASTA format via content-type headers or URL extensions."
      }
    ]
  },
