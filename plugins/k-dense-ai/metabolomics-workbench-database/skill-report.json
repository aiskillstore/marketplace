  "content": {
    "user_title": "Query Metabolomics Workbench Database",
    "value_statement": "Access over 4,200 metabolomics studies from the NIH database. Search metabolites by structure, name, or mass. Retrieve experimental data, study metadata, and compound annotations for biomarker discovery research.",
    "seo_keywords": [
      "metabolomics database",
      "NIH Metabolomics Workbench",
      "mass spectrometry data",
      "metabolite search",
      "Claude Code",
      "Claude",
      "Codex",
      "biomarker discovery",
      "RefMet nomenclature",
      "metabolomics API"
    ],
    "actual_capabilities": [
      "Query metabolite structures by PubChem CID, KEGG ID, HMDB ID, InChI Key, or registry number",
      "Access study metadata and experimental data from 4,200+ metabolomics studies",
      "Standardize metabolite names using the RefMet nomenclature database",
      "Search compounds by mass-to-charge ratio (m/z) with ion adduct specifications",
      "Filter studies by analytical platform, species, sample source, and disease",
      "Retrieve gene and protein information associated with metabolic pathways"
    ],
    "limitations": [
      "No local data storage - requires internet connection to access the Metabolomics Workbench API",
      "Rate limits apply for bulk data retrieval operations",
      "Does not perform statistical analysis on retrieved metabolomics data"
    ],
    "use_cases": [
      {
        "target_user": "Metabolomics Researchers",
        "title": "Find Studies by Metabolite",
        "description": "Search for all metabolomics studies measuring a specific metabolite to compile experimental data."
      },
      {
        "target_user": "Clinical Scientists",
        "title": "Filter by Disease and Platform",
        "description": "Find disease-specific metabolomics studies using LC-MS, GC-MS, or NMR analytical methods."
      },
      {
        "target_user": "Bioinformatics Analysts",
        "title": "Identify Compounds from MS Data",
        "description": "Match mass spectrometry m/z values to known metabolites using the moverz search endpoint."
      }
    ],
    "prompt_templates": [
      {
        "title": "Simple Compound Query",
        "scenario": "Get compound information",
        "prompt": "Find information about the metabolite citrate from Metabolomics Workbench including its molecular formula, structure, and classification."
      },
      {
        "title": "Study Search",
        "scenario": "Find relevant studies",
        "prompt": "Search for metabolomics studies on diabetes in human blood samples using LC-MS positive mode analysis."
      },
      {
        "title": "Mass Spec Identification",
        "scenario": "Identify compounds from m/z",
        "prompt": "Search for compounds with m/z value of 180.06 using M+H adduct and 0.5 Da tolerance. Return the registry numbers and names of matching metabolites."
      },
      {
        "title": "Complete Workflow",
        "scenario": "Multi-step analysis",
        "prompt": "Find all metabolomics studies containing glucose measurements, retrieve the experimental data from the first study, and download the molecular structure for any compounds identified from m/z search of 179."
      }
    ],
    "output_examples": [
      {
        "input": "Find studies about cancer metabolism in human liver tissue",
        "output": [
          "Found 47 studies matching criteria: cancer + human + liver",
          "Top studies: ST001138, ST001075, ST000966",
          "ST001138: Untargeted metabolomics of human liver cancer samples (LC-MS, POSITIVE mode)",
          "Key metabolites identified: glucose, lactate, alanine, glutamate, glutamine",
          "Access at: https://www.metabolomicsworkbench.org/rest/study/study_id/ST001138/summary/json"
        ]
      }
    ],
    "best_practices": [
      "Always use RefMet name standardization before searching for metabolites to ensure consistent nomenclature.",
      "Specify correct ion adducts (M+H for positive mode ESI, M-H for negative mode) when performing m/z searches.",
      "Cache frequently accessed reference data like RefMet database to minimize API calls during large workflows."
    ],
    "anti_patterns": [
      "Making individual API calls in tight loops without delays - implement batch processing and caching.",
      "Using deprecated or non-standard metabolite identifiers without first converting to RefMet standard names.",
      "Assuming all metabolites in a study are human-annotated - verify species and source information."
    ],
    "faq": [
      {
        "question": "Does this skill require API authentication?",
        "answer": "No. The NIH Metabolomics Workbench API is free and open access without authentication for public data."
      },
      {
        "question": "How many studies are available in the database?",
        "answer": "Over 4,200 studies are available, with 3,790+ being publicly accessible through the REST API."
      },
      {
        "question": "What mass spectrometry adducts are supported?",
        "answer": "Common adducts include M+H, M+Na, M+K, M+NH4, M+2H (positive) and M-H, M+Cl, M+FA-H (negative)."
      },
      {
        "question": "Can I download molecular structure files?",
        "answer": "Yes. Use the PNG output format or MOL file format to download molecular structures for identified metabolites."
      },
      {
        "question": "What is RefMet and why should I use it?",
        "answer": "RefMet is the standardized metabolite nomenclature database. Using RefMet ensures consistent naming across all database queries."
      },
      {
        "question": "How do I cite studies retrieved through this API?",
        "answer": "Retrieve the complete study citation from the study metadata using the mwTab output format or the study summary endpoint."
      }
    ]
