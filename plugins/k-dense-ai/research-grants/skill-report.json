  "content": {
    "user_title": "Write winning research grant proposals",
    "value_statement": "Securing research funding is highly competitive with strict agency-specific requirements. This skill provides expert guidance for writing compelling proposals that meet NSF, NIH, DOE, DARPA, and NSTC standards while maximizing your chances of success.",
    "seo_keywords": [
      "research grants",
      "NSF proposals",
      "NIH R01",
      "grant writing",
      "research funding",
      "academic writing",
      "Claude Code",
      "federal grants",
      "DARPA",
      "broader impacts"
    ],
    "actual_capabilities": [
      "Generate agency-specific proposal templates for NSF, NIH, DOE, DARPA, NSTC",
      "Write compelling broader impacts and significance statements",
      "Create detailed budget justifications with proper formatting",
      "Develop specific aims pages with testable hypotheses",
      "Structure research methodologies and timeline plans"
    ],
    "limitations": [
      "Cannot access real funding databases or submission portals",
      "Requires user to provide research content and preliminary data",
      "Does not guarantee funding success - depends on research quality",
      "Must integrate with scientific-schematics skill for required visual elements"
    ],
    "use_cases": [
      {
        "target_user": "Graduate students and postdocs",
        "title": "Write first research proposal",
        "description": "Get structured guidance for writing fellowship or small grant proposals with proper formatting and compelling narratives."
      },
      {
        "target_user": "Faculty researchers",
        "title": "Maximize funding success rate",
        "description": "Create competitive R01, NSF CAREER, or multi-institutional proposals that meet all agency requirements and review criteria."
      },
      {
        "target_user": "Research administrators",
        "title": "Ensure compliance and quality",
        "description": "Review proposals for agency-specific formatting, budget alignment, and completeness before submission."
      }
    ],
    "prompt_templates": [
      {
        "title": "Basic grant help",
        "scenario": "First-time proposal writer",
        "prompt": "Help me write an NSF proposal for [your research topic]. What are the key sections I need to include?"
      },
      {
        "title": "Specific aims review",
        "scenario": "NIH R01 application",
        "prompt": "Review my NIH Specific Aims page: [paste your aims]. Are they specific, testable, and aligned with review criteria?"
      },
      {
        "title": "Broader impacts",
        "scenario": "NSF proposal section",
        "prompt": "Help me write compelling broader impacts for my [field] research that address NSF's review criteria."
      },
      {
        "title": "Agency selection",
        "scenario": "Multi-agency funding options",
        "prompt": "My research is about [topic]. Should I apply to NSF, NIH, DOE, or DARPA? Compare their priorities and requirements."
      }
    ],
    "output_examples": [
      {
        "input": "Help me write an NSF proposal for AI in climate modeling",
        "output": [
          "Project Summary: AI-driven climate models offer transformative potential...",
          "Intellectual Merit: Novel neural architecture reduces prediction errors by 35%...",
          "Broader Impacts: Open-source tools will train 500+ researchers...",
          "Research Plan: Year 1 - Data collection and model development...",
          "Budget: $450K over 3 years with 2 graduate students..."
        ]
      }
    ],
    "best_practices": [
      "Start with specific aims - they drive the entire proposal structure and must be testable",
      "Address review criteria explicitly - make it easy for reviewers to score you highly",
      "Include mandatory visual elements using scientific-schematics skill for competitiveness"
    ],
    "anti_patterns": [
      "Writing generic proposals without agency-specific tailoring",
      "Waiting until deadline week to start - good proposals need months of development",
      "Ignoring broader impacts section (for NSF) or treating it as an afterthought"
    ],
    "faq": [
      {
        "question": "Which agency should I apply to?",
        "answer": "NSF funds basic science, NIH funds biomedical research, DOE funds energy science, DARPA funds high-risk defense research. Match your research to their missions."
      },
      {
        "question": "How long should my proposal be?",
        "answer": "NSF: 15 pages, NIH R01: 12 pages for Research Strategy, DOE/DARPA: varies by program. Always check the specific solicitation."
      },
      {
        "question": "Do I need preliminary data?",
        "answer": "NIH R01s typically need substantial preliminary data. NSF may need some for high-risk proposals. DARPA wants proof-of-concept. Tailor to agency expectations."
      },
      {
        "question": "How do I handle broader impacts?",
        "answer": "NSF requires specific, measurable broader impacts activities. Don't just list generic outreach - show institutional commitment, partnerships, and assessment plans."
      },
      {
        "question": "What's the most important section?",
        "answer": "Specific Aims/Project Summary - these frame everything else. If reviewers aren't excited by page 1, they won't read further."
      },
      {
        "question": "How do I improve my success rate?",
        "answer": "Talk to program officers early, study successful proposals, get external reviews, address all review criteria explicitly, and resubmit with improvements."
      }
    ]
  },
