  "content": {
    "user_title": "Create Interactive Charts and Dashboards",
    "value_statement": "Transform static data into interactive visualizations that users can explore. Plotly enables zoom, pan, hover details, and web embedding for engaging data presentations.",
    "seo_keywords": [
      "plotly",
      "interactive charts",
      "data visualization",
      "python plotting",
      "dashboards",
      "claude code",
      "claude",
      "codex",
      "chart types",
      "plotly express"
    ],
    "actual_capabilities": [
      "Create 40+ chart types including 3D and statistical plots",
      "Generate interactive HTML exports with zoom and hover",
      "Build multi-plot dashboards with synchronized axes",
      "Export to PNG, PDF, SVG formats using Kaleido",
      "Add animations and interactive controls",
      "Style with built-in themes and custom color scales"
    ],
    "limitations": [
      "Requires separate Kaleido install for static image export",
      "Large datasets may slow browser performance",
      "Some advanced charts only available in Graph Objects API",
      "Web deployment requires additional framework like Dash"
    ],
    "use_cases": [
      {
        "target_user": "Data Scientists",
        "title": "Exploratory Data Analysis",
        "description": "Create interactive scatter plots, histograms, and heatmaps to explore relationships in scientific data with hover details and zoom."
      },
      {
        "target_user": "Business Analysts",
        "title": "Executive Dashboards",
        "description": "Build multi-chart dashboards showing KPIs, trends, and comparisons with interactive filtering and drill-down capabilities."
      },
      {
        "target_user": "Researchers",
        "title": "Publication-Ready Figures",
        "description": "Generate high-quality interactive figures for papers and presentations that readers can explore online."
      }
    ],
    "prompt_templates": [
      {
        "title": "Basic Chart Creation",
        "scenario": "Create a simple scatter plot from CSV data",
        "prompt": "Load the data.csv file and create an interactive scatter plot with x='temperature' and y='sales'. Add a trendline and customize the hover template to show both values."
      },
      {
        "title": "Multi-Plot Dashboard",
        "scenario": "Build a dashboard with multiple chart types",
        "prompt": "Create a 2x2 subplot dashboard showing: scatter plot, bar chart, histogram, and box plot from my dataframe. Use consistent styling and add titles to each subplot."
      },
      {
        "title": "3D Visualization",
        "scenario": "Visualize 3D surface data",
        "prompt": "Create a 3D surface plot from my z_data matrix. Add a colorscale, set the axis labels, and enable the camera controls for interactive rotation."
      },
      {
        "title": "Animated Time Series",
        "scenario": "Show data changes over time",
        "prompt": "Create an animated scatter plot that shows how my data changes by year. Use animation_frame='year' and add play/pause controls. Export as interactive HTML."
      }
    ],
    "output_examples": [
      {
        "input": "Create an interactive scatter plot showing the relationship between temperature and ice cream sales",
        "output": [
          "Generated interactive scatter plot with temperature on x-axis and sales on y-axis",
          "Added hover tooltips showing exact temperature and sales values",
          "Included trendline showing positive correlation",
          "Enabled zoom and pan for detailed exploration",
          "Exported as standalone HTML file ready for web embedding"
        ]
      }
    ],
    "best_practices": [
      "Use Plotly Express for quick standard charts, Graph Objects for customizations",
      "Set appropriate axis ranges and labels for clarity",
      "Choose colorblind-friendly color scales for accessibility",
      "Add descriptive titles and annotations to guide interpretation"
    ],
    "anti_patterns": [
      "Creating overly complex dashboards with too many simultaneous views",
      "Using 3D charts when 2D would communicate better",
      "Exporting large datasets as static images instead of interactive HTML",
      "Ignoring mobile responsiveness for web-embedded charts"
    ],
    "faq": [
      {
        "question": "What's the difference between Plotly Express and Graph Objects?",
        "answer": "Plotly Express is a high-level API for common charts with 1-5 lines of code. Graph Objects provides fine-grained control for custom visualizations."
      },
      {
        "question": "How do I export charts for presentations?",
        "answer": "Install kaleido with 'uv pip install kaleido' then use fig.write_image('chart.png') for static images or fig.write_html() for interactive exports."
      },
      {
        "question": "Can I use Plotly offline?",
        "answer": "Yes, Plotly works offline. Interactive charts open in your browser. For web deployment, use Dash or export as HTML."
      },
      {
        "question": "Why is my interactive chart slow with large datasets?",
        "answer": "Consider sampling data, using scattergl for WebGL acceleration, or aggregating data before plotting. Plotly works best under 100k points."
      },
      {
        "question": "How do I create subplots?",
        "answer": "Use plotly.subplots.make_subplots() to create grid layouts. Add traces with row/col parameters to position charts."
      },
      {
        "question": "Can I customize the color scheme?",
        "answer": "Yes, use built-in templates like 'plotly_dark' or create custom color sequences with px.colors.qualitative or px.colors.sequential."
      }
    ]
