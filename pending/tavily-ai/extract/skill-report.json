  "content": {
    "user_title": "Extract Web Content from URLs",
    "value_statement": "This skill extracts clean markdown or text content from specific URLs using Tavily's extraction API. Perfect for research, documentation retrieval, and content aggregation without writing custom scraping code.",
    "seo_keywords": [
      "Claude",
      "Codex",
      "Claude Code",
      "web scraping",
      "content extraction",
      "markdown extraction",
      "API extraction",
      "tavily",
      "url to markdown",
      "web content parser"
    ],
    "actual_capabilities": [
      "Extract clean markdown or text content from specific URLs",
      "Support for basic and advanced extraction (handles JavaScript-rendered pages)",
      "Query-focused extraction that reranks content by relevance",
      "Batch extraction of up to 20 URLs per request",
      "OAuth and API key authentication support"
    ],
    "limitations": [
      "Maximum 20 URLs per extraction request",
      "Requires Tavily API key or OAuth authentication",
      "Some advanced features require query parameter with chunks_per_source",
      "May not extract content from sites that block automated access"
    ],
    "use_cases": [
      {
        "title": "Research Documentation Gathering",
        "description": "Extract documentation content from multiple API reference pages to build a local knowledge base",
        "target_user": "Developers and technical writers"
      },
      {
        "title": "Competitive Analysis",
        "description": "Extract content from competitor websites, product pages, and blog posts for market research",
        "target_user": "Business analysts and product managers"
      },
      {
        "title": "Content Aggregation",
        "description": "Pull articles and content from multiple news sources or blogs into a single markdown format",
        "target_user": "Content creators and journalists"
      }
    ],
    "prompt_templates": [
      {
        "title": "Basic URL Extraction",
        "prompt": "Extract the content from this URL: https://example.com/article",
        "scenario": "When you have a single URL and want the full content"
      },
      {
        "title": "Multiple URLs Extraction",
        "prompt": "Extract content from these URLs: https://docs.example.com/api, https://docs.example.com/auth",
        "scenario": "When you need to gather content from multiple related pages"
      },
      {
        "title": "Query-Focused Extraction",
        "prompt": "Extract information about authentication from these URLs: https://example.com/docs, https://example.com/api-reference. Focus on API keys and OAuth.",
        "scenario": "When you want relevant content filtered by a specific topic"
      },
      {
        "title": "Advanced Extraction for Dynamic Pages",
        "prompt": "Extract all content from this JavaScript-heavy page using advanced extraction: https://app.example.com/dashboard",
        "scenario": "When dealing with single-page applications or JavaScript-rendered content"
      }
    ],
    "output_examples": [
      {
        "input": "Extract content from https://example.com/about",
        "output": "## About Example\n\nWelcome to Example.com...\n\nOur Mission\n\nWe strive to provide..."
      },
      {
        "input": "Extract information about pricing from https://example.com/pricing and https://example.com/plans",
        "output": "## Pricing Information\n\n### Basic Plan - $9/month\n- Feature A\n- Feature B\n\n### Pro Plan - $29/month\n- All Basic features\n- Priority support..."
      }
    ],
    "best_practices": [
      "Use the query parameter to filter content to exactly what you need, especially when extracting from large pages",
      "Start with basic extraction and only use advanced mode if content is missing or incomplete",
      "Batch URLs by topic or category to keep results organized and relevant"
    ],
    "anti_patterns": [
      "Extracting more than 20 URLs in a single request will fail",
      "Using chunks_per_source without a query parameter will return an error",
      "Not checking the failed_results field in the response may miss extraction failures"
    ],
    "faq": [
      {
        "question": "Do I need a Tavily API key?",
        "answer": "Yes, you need either a Tavily API key or an existing Tavily account for OAuth authentication. Get an API key at tavily.com or sign up for an account."
      },
      {
        "question": "How many URLs can I extract at once?",
        "answer": "You can extract up to 20 URLs per request. For larger batches, split into multiple requests."
      },
      {
        "question": "What is the difference between basic and advanced extraction?",
        "answer": "Basic extraction is faster and works for static HTML pages. Advanced extraction handles JavaScript-rendered pages, complex layouts, and structured data but takes longer."
      },
      {
        "question": "How does the query parameter work?",
        "answer": "The query parameter reranks extracted content chunks by relevance to your search terms. Use it with chunks_per_source to get the most relevant sections."
      },
      {
        "question": "Why am I getting failed_results?",
        "answer": "Failed results occur when URLs are unreachable, blocked, or timeout. Check the failed_results array in the response for specific error information."
      },
      {
        "question": "Can I extract content from password-protected pages?",
        "answer": "No, this skill cannot extract content from pages that require login or authentication beyond what's publicly accessible."
