  "content": {
    "user_title": "Create and edit DOCX with tracked changes",
    "value_statement": "DOCX reviews and edits are hard to manage in plain text. This skill edits OOXML, adds tracked changes, and preserves formatting reliably.",
    "seo_keywords": [
      "docx editing",
      "ooxml",
      "tracked changes",
      "document comments",
      "Word automation",
      "Claude",
      "Codex",
      "Claude Code",
      "docx creation",
      "document review"
    ],
    "actual_capabilities": [
      "Unpack .docx, .pptx, and .xlsx into pretty printed XML and suggest an RSID",
      "Pack an unpacked Office directory back into a file with XML whitespace cleanup",
      "Validate packed files with soffice conversion when enabled",
      "Find XML nodes by tag, attributes, line number, or text content",
      "Create tracked insertions and deletions, and revert prior insertions or deletions",
      "Add comments and replies with author metadata in OOXML"
    ],
    "limitations": [
      "Requires external tools like pandoc or soffice for conversion and validation steps",
      "Edits operate on unpacked OOXML folders, not on binary files directly",
      "Schema correctness depends on valid OOXML structure in the source files",
      "Image insertion requires manual media file handling and relationship updates"
    ],
    "use_cases": [
      {
        "target_user": "Legal reviewer",
        "title": "Track contract edits",
        "description": "Apply precise redlines and comments while preserving existing formatting and authorship."
      },
      {
        "target_user": "Technical writer",
        "title": "Revise release notes",
        "description": "Batch edits to DOCX sections with tracked changes and clean style preservation."
      },
      {
        "target_user": "Operations analyst",
        "title": "Audit document changes",
        "description": "Extract and verify text changes by mapping content to OOXML nodes."
      }
    ],
    "prompt_templates": [
      {
        "title": "Extract text",
        "scenario": "Beginner needs document text quickly",
        "prompt": "Convert this DOCX to markdown and show tracked changes. Use pandoc and tell me the command to run."
      },
      {
        "title": "Small tracked edit",
        "scenario": "Replace a phrase with tracked changes",
        "prompt": "In the DOCX, change the phrase \"30 days\" to \"45 days\" with tracked changes. Explain how to locate and edit the OOXML."
      },
      {
        "title": "Add a comment",
        "scenario": "Comment on a paragraph with a note",
        "prompt": "Add a comment on the paragraph that contains \"Data retention\". Use the Document library and show the key steps."
      },
      {
        "title": "Batch redlining",
        "scenario": "Advanced review with multiple edits",
        "prompt": "Plan three batches of tracked edits for sections 2, 3, and 5, then outline how to implement them with the Document library."
      }
    ],
    "output_examples": [
      {
        "input": "Apply tracked changes to replace \"monthly\" with \"quarterly\" and add a comment explaining the change.",
        "output": [
          "Located the target run in word/document.xml by text match",
          "Inserted tracked deletion for monthly and insertion for quarterly",
          "Added a comment spanning the change with author metadata"
        ]
      }
    ],
    "best_practices": [
      "Use the Document library for tracked changes and comments",
      "Keep unchanged text outside tracked change tags",
      "Validate packed files after edits when possible"
    ],
    "anti_patterns": [
      "Editing inside another author tracked change directly",
      "Using unicode bullets instead of list structures",
      "Skipping XML validation after large edits"
    ],
    "faq": [
      {
        "question": "Is this compatible with DOCX, PPTX, and XLSX files?",
        "answer": "Yes, unpack and pack scripts support DOCX, PPTX, and XLSX files."
      },
      {
        "question": "What are the size limits for documents?",
        "answer": "There is no explicit size limit, but very large files may slow XML parsing."
      },
      {
        "question": "Can I integrate this into an automated pipeline?",
        "answer": "Yes, the scripts and Python library are designed for CLI and batch use."
      },
      {
        "question": "Does it send my documents to external services?",
        "answer": "No, all edits happen locally on unpacked files."
      },
      {
        "question": "What should I do if packing fails validation?",
        "answer": "Review XML changes for schema issues, then repack or use force only for testing."
      },
      {
        "question": "How does this compare to basic DOCX text extractors?",
        "answer": "It supports tracked changes, comments, and precise OOXML editing, not just text extraction."
      }
    ]
