      },
      {
        "target_user": "Bioinformaticians",
        "title": "Pipeline development",
        "description": "Build automated mass spectrometry data processing pipelines with Python"
      },
      {
        "target_user": "Metabolomics scientists",
        "title": "Metabolite analysis",
        "description": "Perform untargeted metabolomics preprocessing and feature annotation"
      }
    ],
    "prompt_templates": [
