  "content": {
    "user_title": "Query public statistics with Data Commons API",
    "value_statement": "Access aggregated public statistical data from governments and organizations worldwide. Query demographic, economic, health, and environmental datasets through a unified Python interface without manual data collection.",
    "seo_keywords": [
      "datacommons client",
      "public statistics API",
      "demographic data python",
      "economic indicators dataset",
      "health statistics research",
      "census data API",
      "population data query",
      "Claude scientific skills",
      "Claude Code tools",
      "OpenAI Codex functions"
    ],
    "actual_capabilities": [
      "Query population, income, unemployment, and health statistics for any geographic entity",
      "Resolve place names and coordinates to Data Commons identifiers (DCIDs)",
      "Fetch time-series data for trend analysis and historical comparisons",
      "Explore the knowledge graph to discover available variables and entity relationships",
      "Filter data by source for consistency across multiple statistical providers"
    ],
    "limitations": [
      "Requires API key registration at datacommons.org for full access",
      "Covers primarily geographic/place entities - non-place entities have limited support",
      "Data availability varies by region and statistical variable"
    ],
    "use_cases": [
      {
        "target_user": "Data analysts and researchers",
        "title": "Aggregate economic indicators",
        "description": "Compare GDP, unemployment rates, and income levels across countries, states, or counties for research reports."
      },
      {
        "target_user": "Policy makers and journalists",
        "title": "Fact-check with public data",
        "description": "Verify claims about demographics, health trends, or environmental metrics using authoritative public sources."
      },
      {
        "target_user": "Developers building data apps",
        "title": "Enrich applications with statistics",
        "description": "Add demographic context to location-based applications without maintaining your own data pipeline."
      }
    ],
    "prompt_templates": [
      {
        "title": "Basic population query",
        "scenario": "Get current population data",
        "prompt": "Get the latest population for California, Texas, and New York using Data Commons"
      },
      {
        "title": "Historical comparison",
        "scenario": "Analyze trends over time",
        "prompt": "Show me the unemployment rate time series for the USA from 2010 to 2023"
      },
      {
        "title": "Geographic hierarchy",
        "scenario": "Query all counties in a state",
        "prompt": "Get median household income for all counties in California for 2020"
      },
      {
        "title": "Multi-variable analysis",
        "scenario": "Compare multiple statistics",
        "prompt": "Compare population, median income, and unemployment rate for the top 5 most populous US states"
      }
    ],
    "output_examples": [
      {
        "input": "Get population and unemployment rate for California and New York",
        "output": [
          "California: 39,144,585 people (2024) - Unemployment: 4.4%",
          "New York: 19,867,248 people (2024) - Unemployment: 4.1%"
        ]
      }
    ],
    "best_practices": [
      "Resolve place names to DCIDs first using the Resolve API before querying data",
      "Use Pandas integration (to_observations_as_records) for efficient data analysis",
      "Filter by facet domains (filter_facet_domains) when data consistency matters"
    ],
    "anti_patterns": [
      "Do not hardcode DCIDs - always resolve names to DCIDs for reliability",
      "Do not assume data exists - check availability before querying",
      "Do not query one entity at a time - batch queries for efficiency"
    ],
    "faq": [
      {
        "question": "Do I need an API key?",
        "answer": "Yes, register at apikeys.datacommons.org and set DC_API_KEY environment variable."
      },
      {
        "question": "What data sources are available?",
        "answer": "US Census Bureau, CDC, WHO, EPA, World Bank, and other authoritative organizations."
      },
      {
        "question": "Can I query non-US data?",
        "answer": "Yes, Data Commons includes international data for many countries and regions."
      },
      {
        "question": "How fresh is the data?",
        "answer": "Update frequency varies by source - census data yearly, some indicators quarterly."
      },
      {
        "question": "What geographic levels are supported?",
        "answer": "Countries, states, counties, cities, census tracts, and administrative regions."
      },
      {
        "question": "Is there a rate limit?",
        "answer": "Yes, rate limits apply based on API key tier. Check Data Commons documentation."
      }
    ]
