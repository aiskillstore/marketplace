  "content": {
    "user_title": "Process and manipulate PDF documents",
    "value_statement": "Extract text, tables, and metadata from PDF files. Create new PDFs, merge multiple documents, split files into pages, and fill form fields programmatically.",
    "seo_keywords": [
      "pdf manipulation",
      "extract text from pdf",
      "pdf forms",
      "merge pdf",
      "split pdf",
      "create pdf",
      "claude",
      "codex",
      "claude code"
    ],
    "actual_capabilities": [
      "Extract text and tables from PDF documents using pdfplumber",
      "Merge multiple PDFs or split documents into individual pages",
      "Fill interactive PDF forms with text, checkboxes, and radio buttons",
      "Create new PDFs with reportlab for reports and documents",
      "Add watermarks, rotate pages, and manage document metadata",
      "Perform OCR on scanned PDFs using pytesseract"
    ],
    "limitations": [
      "Cannot edit existing PDF content (text modification requires overlays)",
      "Password-protected PDFs require owner password for full access",
      "Some scanned PDFs may require manual OCR preprocessing",
      "Complex form filling requires visual analysis of field locations"
    ],
    "use_cases": [
      {
        "target_user": "Data Analysts",
        "title": "Extract data from reports",
        "description": "Pull tables and text from PDF reports for data analysis and spreadsheet imports"
      },
      {
        "target_user": "Administrative Staff",
        "title": "Automate form processing",
