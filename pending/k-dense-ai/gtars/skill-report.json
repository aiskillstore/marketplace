  "content": {
    "user_title": "Analyze genomic intervals with high-performance Rust tools",
    "value_statement": "Genomic data analysis requires handling massive BED files and complex interval operations. Gtars provides Rust-powered performance with Python bindings for efficient genomic interval analysis, overlap detection, and ML preprocessing.",
    "seo_keywords": [
      "genomic interval analysis",
      "BED file processing",
      "Rust bioinformatics",
      "Python genomics",
      "overlap detection",
      "coverage tracks",
      "Claude Code genomics",
      "genomic ML preprocessing",
      "bioinformatics tools"
    ],
    "actual_capabilities": [
      "High-performance overlap detection using IGD indexing",
      "Coverage track generation for ATAC-seq and ChIP-seq",
      "Genomic tokenization for ML model preprocessing",
      "Reference sequence management with GA4GH refget",
      "Fragment analysis for single-cell genomics",
      "Memory-efficient processing of large genomic files"
    ],
    "limitations": [
      "Requires Rust toolchain for CLI installation",
      "Specialized for genomic interval data only",
      "Python bindings may have version compatibility constraints"
    ],
    "use_cases": [
      {
        "target_user": "Computational biologist",
        "title": "Find overlapping regulatory elements",
        "description": "Identify transcription factor binding sites that overlap with gene promoters using efficient IGD indexing"
      },
      {
        "target_user": "Bioinformatics engineer",
        "title": "Generate coverage tracks from ATAC-seq",
        "description": "Create BigWig visualization tracks from fragment files for genome browser display"
      },
      {
        "target_user": "ML researcher",
        "title": "Tokenize genomic regions for deep learning",
        "description": "Convert BED files into discrete tokens for training transformer models on genomic sequences"
      }
    ],
    "prompt_templates": [
      {
        "title": "Basic overlap detection",
        "scenario": "Find peaks overlapping promoters",
        "prompt": "Use gtars to find overlaps between chip_peaks.bed and promoters.bed, then save the overlapping peaks to a new file"
      },
      {
        "title": "Coverage track generation",
        "scenario": "Create BigWig from fragments",
        "prompt": "Generate a BigWig coverage track from atac_fragments.bed with 10bp resolution using gtars"
      },
      {
        "title": "Genomic tokenization",
        "scenario": "Prepare data for ML model",
        "prompt": "Load training_peaks.bed and create a TreeTokenizer, then tokenize all regions for ML preprocessing"
      },
      {
