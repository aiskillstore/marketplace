  "content": {
    "user_title": "Search PubMed biomedical literature",
    "value_statement": "Search millions of biomedical research articles using PubMed E-utilities API. Construct advanced queries with Boolean operators, MeSH terms, and field tags for systematic reviews and literature analysis.",
    "seo_keywords": [
      "pubmed search",
      "biomedical literature",
      "e-utilities api",
      "mesh terms",
      "systematic review",
      "clinical research",
      "claude code",
      "claude",
      "codex"
    ],
    "actual_capabilities": [
      "Search PubMed database with Boolean operators and field tags",
      "Use MeSH terms for controlled vocabulary searching",
      "Access E-utilities API programmatically via REST endpoints",
      "Filter by publication type, date, and article availability",
      "Find related articles and citation networks",
      "Export results in multiple formats for reference managers"
    ],
    "limitations": [
      "Requires API key for higher rate limits (10 req/s vs 3 req/s)",
      "Full text access depends on publisher or institutional subscriptions",
      "Pre-1975 articles often lack abstracts"
    ],
    "use_cases": [
      {
        "target_user": "Medical Researchers",
        "title": "Literature reviews",
        "description": "Find and synthesize research articles for systematic reviews and meta-analyses"
      },
      {
        "target_user": "Clinical Professionals",
        "title": "Evidence-based searches",
        "description": "Locate latest clinical guidelines and treatment evidence for patient care"
      },
      {
        "target_user": "Data Scientists",
        "title": "Automated data extraction",
        "description": "Batch retrieve article metadata for biomedical text mining and analysis"
      }
    ],
    "prompt_templates": [
      {
        "title": "Basic search",
        "scenario": "Find recent research",
        "prompt": "Search PubMed for clinical trials on type 2 diabetes treatment published in 2024"
      },
      {
        "title": "Advanced query",
        "scenario": "Complex search",
        "prompt": "Find systematic reviews about immunotherapy for lung cancer using MeSH terms and publication type filters"
      },
      {
        "title": "Citation lookup",
        "scenario": "Find specific article",
        "prompt": "Find articles by author Smith JA about cardiovascular disease from 2020 to 2024"
      },
      {
        "title": "Batch retrieval",
        "scenario": "API workflow",
        "prompt": "Show me how to use ESearch and EFetch to retrieve abstracts for the first 100 PMIDs matching cancer research"
      }
    ],
    "output_examples": [
      {
        "input": "Search PubMed for systematic reviews on machine learning in medical imaging",
        "output": [
          "Found 47 systematic reviews matching your query",
          "Top results include:",
          "â€¢ Deep learning for radiology image analysis - 2024",
          "â€¢ AI-assisted diagnosis systematic review - 2023",
          "â€¢ Machine learning in pathology: a systematic review - 2023"
        ]
      }
    ],
    "best_practices": [
      "Include MeSH terms alongside text terms for comprehensive coverage",
      "Use publication type filters to find specific study designs",
      "Apply date filters to focus on recent research"
    ],
    "anti_patterns": [
      "Using only text terms without MeSH terms reduces search comprehensiveness",
      "Omitting publication type filters returns too many irrelevant results",
      "Not respecting rate limits may result in temporary API access loss"
    ],
    "faq": [
      {
        "question": "Do I need an API key for PubMed?",
        "answer": "API keys are optional but recommended. They increase rate limits from 3 to 10 requests per second."
      },
      {
        "question": "What is the difference between MeSH and text search?",
        "answer": "MeSH terms use controlled vocabulary for consistent results; text searches match words anywhere in the record."
      },
      {
        "question": "How do I find related articles?",
        "answer": "Use ELink API with the neighbor command to find articles related by topic, citations, or shared MeSH terms."
      },
      {
        "question": "What export formats are available?",
        "answer": "PubMed supports MEDLINE, XML, JSON, and text formats for different use cases and reference managers."
      },
      {
        "question": "How many results can I retrieve?",
        "answer": "Maximum 10,000 results per search. Use history server for larger result sets with batch retrieval."
      },
      {
        "question": "Can I search for full-text articles only?",
        "answer": "Add free full text[sb] to your query to filter for articles with freely available full text."
      }
    ]
