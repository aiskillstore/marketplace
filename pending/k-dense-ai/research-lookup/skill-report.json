  "content": {
    "user_title": "Search Academic Literature and Research Papers",
    "value_statement": "Finding current academic research requires searching multiple databases and evaluating source quality. This tool searches scholarly literature through Perplexity Sonar models with automatic model selection based on query complexity and citation-focused results.",
    "seo_keywords": [
      "research lookup",
      "academic search",
      "literature review",
      "scientific papers",
      "citation finder",
      "Claude Code",
      "Claude",
      "Codex",
      "Perplexity",
      "OpenRouter"
    ],
    "actual_capabilities": [
      "Search academic papers and scholarly literature using Perplexity Sonar models",
      "Automatic model selection between fast lookup and deep reasoning modes",
      "Extract and format citations with DOI links and source attribution",
      "Prioritize high-impact journals and highly-cited papers in results",
      "Batch multiple research queries with rate limiting",
      "Filter by publication timeframe and venue quality"
    ],
    "limitations": [
      "Requires OPENROUTER_API_KEY for access to Perplexity models",
      "Cannot access paywalled full-text content directly",
      "May miss very recent papers not yet indexed in databases",
      "Dependent on OpenRouter service availability and rate limits"
    ],
    "use_cases": [
      {
        "target_user": "Researchers",
        "title": "Literature Review Assistant",
        "description": "Gather current research for manuscripts and stay current with recent developments in your field."
      },
      {
        "target_user": "Students",
        "title": "Academic Source Finder",
        "description": "Find credible academic sources for essays and papers with proper citations."
      },
      {
        "target_user": "Technical Writers",
        "title": "Technical Documentation Lookup",
        "description": "Look up specifications, protocols, and methodologies for technical documentation."
      }
    ],
    "prompt_templates": [
      {
        "title": "Basic Search",
        "scenario": "Find recent papers",
        "prompt": "Research the latest advances in [TOPIC] from 2024. List key papers with citations."
      },
      {
        "title": "Comparative Analysis",
        "scenario": "Compare methods",
        "prompt": "Compare [METHOD_A] versus [METHOD_B] for [APPLICATION]. Include pros, cons, and relevant studies."
      },
      {
        "title": "Deep Analysis",
        "scenario": "Analyze mechanisms",
        "prompt": "Explain the mechanism of [PHENOMENON] and identify key papers that established this understanding."
      },
      {
        "title": "Citation Finding",
        "scenario": "Find foundational papers",
        "prompt": "Find the most influential foundational papers on [TOPIC] that every researcher should know."
      }
    ],
    "output_examples": [
      {
        "input": "Recent advances in mRNA vaccine technology 2024",
        "output": [
          "â€¢ Key Findings: Summary of 5 major research advances",
          "â€¢ Top Paper: Smith et al. 2024, Nature Medicine (cited 200+ times)",
          "â€¢ Venue: Nature Medicine (Tier 1)",
          "â€¢ DOI: https://doi.org/10.xxxx/xxxx",
          "â€¢ 4 additional high-impact citations included"
        ]
      }
    ],
    "best_practices": [
      "Use specific query terms including timeframe and research type for best results",
      "Use reasoning keywords like 'compare', 'analyze', or 'explain' for complex queries",
      "Verify all citations against primary sources before including in manuscripts",
      "Break complex research questions into multiple focused queries"
    ],
    "anti_patterns": [
      "Do not use vague queries like 'tell me about AI' - be specific about topic and scope",
      "Do not rely solely on AI-generated summaries - always verify with primary sources",
      "Do not use for circumventing paywalls or accessing restricted content",
      "Do not present AI-generated content as original research without proper attribution"
    ],
    "faq": [
      {
        "question": "What models does this skill use?",
        "answer": "It uses Perplexity Sonar Pro Search for fast lookups and Sonar Reasoning Pro for complex analytical queries."
      },
      {
        "question": "How does automatic model selection work?",
        "answer": "Queries with reasoning keywords like 'compare', 'analyze', or 'explain' trigger the deep reasoning model automatically."
      },
      {
        "question": "What API key do I need?",
        "answer": "Set the OPENROUTER_API_KEY environment variable with your OpenRouter API credentials."
      },
      {
        "question": "What sources does it prioritize?",
        "answer": "Peer-reviewed journals, high-impact venues like Nature and Science, and highly-cited papers are prioritized."
      },
      {
        "question": "Can I save research results to a file?",
        "answer": "Use the --output flag to save results to a file, or --json for JSON format output."
      },
      {
        "question": "How many citations does it return?",
        "answer": "The skill returns 5-10 high-quality citations with full bibliographic information and DOI links when available."
      }
    ]
