        "target_user": "Agency owners",
        "title": "Site audits",
        "description": "Run comprehensive audits across SEO, performance, visual QA, and security for client sites."
      },
      {
        "target_user": "Freelance developers",
        "title": "Client handoff",
        "description": "Prepare white-labeled admin areas and documentation for client site delivery."
      }
    ],
    "prompt_templates": [
