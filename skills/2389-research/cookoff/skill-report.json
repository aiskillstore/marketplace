  "content": {
    "user_title": "Run parallel implementation cookoffs",
    "value_statement": "Implementing features without competition can miss better solutions. Cookoff runs multiple agents in parallel, each creating their own plan from shared design. Tests and reviews pick the winner.",
    "seo_keywords": [
      "cookoff",
      "parallel implementation",
      "claude code skill",
      "agent competition",
      "implementation competition",
      "multiple agents",
      "test driven development",
      "codex",
      "claude",
      "claude-code"
    ],
    "actual_capabilities": [
      "Detect design-to-implementation transition and present options",
      "Assess feature complexity and recommend implementation count",
      "Create git worktrees for parallel agent execution",
      "Dispatch all agents in single message for true parallelism",
      "Orchestrate fresh-eyes review and judge scoring",
      "Cleanup loser branches and document results"
    ],
    "limitations": [
      "Requires a completed design document to work effectively",
      "Dependent on other skills for execution (writing-plans, executing-plans, fresh-eyes)",
      "Not designed for trivial or emergency fixes where speed is critical",
      "Higher compute cost due to parallel execution"
    ],
    "use_cases": [
      {
        "target_user": "Engineering teams",
        "title": "Evaluate architectural approaches",
        "description": "When multiple valid architectures exist, run parallel implementations to let tests decide the winner."
      },
      {
        "target_user": "Open source maintainers",
        "title": "Quality gate implementations",
        "description": "Before accepting major contributions, run cookoff to validate approach and discover edge cases."
      },
      {
        "target_user": "Solo developers",
        "title": "Challenge assumptions",
        "description": "Second-guess your first implementation approach by running an alternative in parallel."
      }
    ],
    "prompt_templates": [
      {
        "title": "Start basic cookoff",
        "scenario": "Ready to implement design",
        "prompt": "We have a design doc ready at docs/plans/feature/design.md. Let's run a cookoff."
      },
      {
        "title": "Cookoff specific feature",
        "scenario": "Implement feature with cookoff",
        "prompt": "Cookoff this feature: build an API rate limiter with Redis backend."
      },
      {
        "title": "Run with existing design",
        "scenario": "Design completed at path",
        "prompt": "Our design doc is complete at docs/plans/auth-system/design.md. How should we cookoff?"
      },
      {
        "title": "Parallel with complexity",
        "scenario": "Complex implementation",
        "prompt": "Run cookoff on our new payments integration. Design is in docs/plans/payments/design.md. Complexity is high."
      }
    ],
