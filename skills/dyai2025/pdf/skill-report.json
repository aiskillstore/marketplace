    "user_title": "Process PDF documents efficiently",
    "value_statement": "Working with PDFs can be time-consuming and error-prone. This skill provides automated tools for extracting text and tables, filling forms, merging documents, and creating new PDFs from scratch.",
    "seo_keywords": [
      "pdf processing",
      "pdf extraction",
      "pdf manipulation",
      "pdf forms",
      "claude pdf",
      "codex pdf",
      "claude code pdf",
      "document automation",
      "text extraction",
      "table extraction"
    ],
    "actual_capabilities": [
      "Extract text and tables from PDF documents",
      "Fill PDF forms with data programmatically",
      "Merge and split PDF files",
      "Create new PDFs with custom content",
      "Convert PDFs to images for analysis",
      "Add watermarks and password protection"
    ],
    "limitations": [
      "Cannot decrypt password-protected PDFs without the password",
      "Scanned PDFs require OCR for text extraction",
      "Complex form layouts may require manual verification",
      "Some PDF features may not be fully compatible across all viewers"
    ],
    "use_cases": [
      {
