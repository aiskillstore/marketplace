    ],
    "actual_capabilities": [
      "Create design philosophy documents outlining aesthetic movements",
      "Generate original visual art in PNG and PDF formats",
      "Apply professional typography using bundled open-source fonts",
      "Express abstract concepts through color, form, and composition",
      "Produce museum-quality single-page visual compositions",
      "Bundle multi-page artwork into single PDF documents"
    ],
    "limitations": [
      "Cannot edit or modify existing images or photographs",
      "Limited to fonts included in the canvas-fonts directory",
      "Cannot create interactive or animated content",
      "Output is limited to static visual compositions only"
    ],
    "use_cases": [
      {
        "target_user": "Graphic Designers",
        "title": "Design Philosophy Exploration",
        "description": "Generate original visual concepts based on philosophical aesthetic frameworks for branding or editorial projects."
      },
      {
        "target_user": "Content Creators",
        "title": "Poster and Art Creation",
        "description": "Create unique posters and visual art pieces that communicate ideas through design rather than text."
      },
      {
        "target_user": "Writers and Publishers",
        "title": "Book Cover Concepts",
        "description": "Develop sophisticated visual concepts for book covers and publication materials using typography-led design."
      }
    ],
    "prompt_templates": [
      {
        "title": "Basic Art Request",
        "scenario": "Simple design task",
        "prompt": "Create a visual art piece using a design philosophy that emphasizes minimal text and spatial expression. The subject is [describe your concept]."
      },
      {
        "title": "Style-Specific Design",
        "scenario": "Requesting specific aesthetic",
        "prompt": "Design a poster using the philosophy of [Brutalist Joy, Chromatic Silence, Analog Meditation, or similar]. The topic is [describe theme]. Apply expert-level craftsmanship throughout."
      },
      {
        "title": "Multi-Page Project",
        "scenario": "Series or book project",
        "prompt": "Create a coffee table book concept with 3-5 pages following a unified design philosophy. The theme explores [describe concept]. Each page should feel like a distinct work in a cohesive series."
      },
      {
        "title": "Refined Masterpiece",
        "scenario": "Polishing existing work",
        "prompt": "Review the last design and refine it to museum-quality perfection. Ensure every element shows painstaking craftsmanship. Make the composition more cohesive and philosophically aligned."
      }
    ],
    "output_examples": [
      {
        "input": "Create a visual art piece about the concept of time passing. Use a design philosophy that emphasizes spatial expression and visual hierarchy.",
        "output": [
          "Design Philosophy Document: Temporal Layers - A manifesto exploring time through progressive decay and accumulation",
          "Canvas Output: Abstract composition with layered geometric forms suggesting memory traces, subtle color gradients indicating temporal shift, minimal typography reading CONTINUOUS in precise horizontal alignment"
        ]
      },
      {
        "input": "Design a poster using Chromatic Silence philosophy about digital isolation.",
        "output": [
          "Design Philosophy: Chromatic Silence - Color as the primary information system",
          "Canvas Output: Geometric precision with isolated color zones creating meaning, small sans-serif labels, monochromatic palette suggesting disconnection, empty space as active element"
        ]
      }
    ],
    "best_practices": [
      "Provide clear conceptual direction while allowing creative interpretation of the visual philosophy",
      "Request refinement passes to achieve museum-quality polish and craftsmanship",
      "Specify whether you want single-page or multi-page output formats"
    ],
    "anti_patterns": [
      "Asking for literal text-heavy posters instead of visual-first design philosophy",
      "Requesting the same style repeatedly without exploring different aesthetic movements",
      "Expecting photorealistic imagery or photographic content from this design-focused skill"
    ],
    "faq": [
      {
        "question": "What file formats does this skill output?",
        "answer": "The skill generates PNG images and PDF documents. Multi-page projects can be bundled into single PDF files."
      },
      {
        "question": "Can I use my own fonts?",
        "answer": "No. The skill only uses fonts bundled in the canvas-fonts directory. Over 40 open-source professional fonts are included."
      },
      {
        "question": "How is this different from image generation tools?",
        "answer": "This skill creates vector-based visual compositions using typography and design principles, not pixel-based image generation."
      },
      {
        "question": "Is my data safe?",
        "answer": "Yes. This is a prompt-based skill with no network access. All processing occurs locally within the AI context."
      },
      {
        "question": "Can I create animated or interactive content?",
        "answer": "No. This skill produces static visual compositions only. For animations, use a different skill or tool."
      },
      {
        "question": "How does this compare to Adobe Illustrator?",
        "answer": "This is a generative AI skill that creates original designs from philosophical prompts. It complements but does not replace manual design tools."
      }
    ]
