    "use_cases": [
      {
        "target_user": "AI developers creating custom skills",
        "title": "Build robust AI skills that work under pressure",
        "description": "Apply TDD principles to ensure your skills handle edge cases and resist rationalization when agents are under time pressure or stress."
      },
      {
        "target_user": "Teams standardizing AI workflows",
        "title": "Create consistent, tested skill documentation",
        "description": "Establish team standards for skill creation with proven testing methodologies that ensure quality and reliability."
      },
      {
        "target_user": "Skill marketplace contributors",
        "title": "Publish bulletproof skills to marketplaces",
        "description": "Use rigorous testing to create skills that work reliably across different AI models and usage scenarios."
      }
    ],
    "prompt_templates": [
      {
        "title": "Test existing skill",
        "scenario": "Verify a skill works under pressure",
        "prompt": "I have a skill about [TOPIC]. Test it with subagents using pressure scenarios to see if it resists rationalization. Document any failures."
      },
      {
        "title": "Create new skill",
        "scenario": "Build a skill using TDD approach",
        "prompt": "I need to create a skill for [PROBLEM]. Use the TDD approach: first test with subagents to see baseline behavior, then write minimal skill, then iterate to close loopholes."
      },
      {
        "title": "Improve skill compliance",
        "scenario": "Make a skill more resistant to violations",
        "prompt": "My skill about [TOPIC] has compliance issues. Help me apply persuasion principles and add explicit anti-rationalization counters to make it bulletproof."
      },
      {
        "title": "Skill deployment checklist",
        "scenario": "Ensure skill is ready for production",
        "prompt": "I'm ready to deploy my skill about [TOPIC]. Walk me through the complete deployment checklist to ensure it's properly tested and documented."
      }
    ],
    "output_examples": [
      {
        "input": "Test my skill about defensive programming",
        "output": [
          "Baseline test results: Agent skipped input validation when under time pressure",
          "Rationalizations used: 'This is just a demo', 'Input looks safe'",
          "Skill gaps identified: No explicit requirement for validation before processing",
          "Recommended fixes: Add 'YOU MUST validate all inputs' with specific examples",
          "Pressure scenario passed after skill update: Agent now validates despite time pressure"
        ]
      },
      {
        "input": "Create a new skill for root cause tracing",
        "output": [
          "RED phase: Tested agent without skill - jumped to solutions without analysis",
          "GREEN phase: Wrote skill requiring systematic problem decomposition",
          "REFACTOR: Added counters for 'obvious fix' rationalization",
          "Result: Agent now traces root causes before proposing solutions"
        ]
      }
    ],
    "best_practices": [
      "Always test skills with subagents before deployment - watch them fail first",
      "Use imperative language and explicit anti-rationalization counters for discipline skills",
      "Keep skills concise and focused - one excellent example beats many mediocre ones",
      "Include searchable keywords in descriptions for better discovery"
    ],
    "anti_patterns": [
      "Writing skills without testing - untested skills always have issues",
      "Using narrative examples - too specific and not reusable",
      "Creating multi-language examples - dilutes quality and adds maintenance burden",
      "Skipping the RED phase - you must see baseline behavior before writing the skill"
    ],
