  "content": {
    "user_title": "Generate AI Images and Videos with MuleRouter",
    "value_statement": "Creating professional AI-generated media requires complex API integrations and model management. MuleRouter skill simplifies this by providing a unified interface for generating images and videos using state-of-the-art AI models.",
    "seo_keywords": [
      "AI image generation",
      "text-to-video",
      "Claude Code",
      "MuleRouter API",
      "Wan2.6 models",
      "Nano Banana",
      "AI media generation",
      "video editing",
      "image transformation",
      "multimodal AI"
    ],
    "actual_capabilities": [
      "Generate images from text prompts",
      "Create videos from text or images",
      "Transform images with AI editing",
      "Support for Alibaba Wan2.6 models",
      "Google Nano Banana integration",
      "Async task polling for long operations"
    ],
    "limitations": [
      "Requires MuleRouter or MuleRun API key",
      "Media generation takes 5-15 minutes for videos",
      "Limited to supported model providers",
      "Network access required for API calls"
    ],
    "use_cases": [
      {
        "target_user": "Content creators",
        "title": "Create social media videos",
        "description": "Generate engaging video content from text descriptions for YouTube, TikTok, or Instagram."
      },
      {
        "target_user": "Designers",
        "title": "Prototype visual concepts",
        "description": "Quickly generate and iterate on visual ideas before committing to full production."
      },
      {
        "target_user": "Developers",
        "title": "Build AI-powered applications",
        "description": "Integrate media generation capabilities into applications without managing multiple AI providers."
      }
    ],
    "prompt_templates": [
      {
        "title": "Basic image",
        "scenario": "Generate a simple image",
        "prompt": "Create an image of a serene mountain lake at sunset with pine trees reflected in the water"
      },
      {
        "title": "Video from image",
        "scenario": "Animate a static image",
        "prompt": "Convert this landscape photo into a 5-second video with gentle camera movement and swaying trees"
      },
      {
        "title": "Style transfer",
        "scenario": "Transform image style",
        "prompt": "Transform this portrait into anime style while keeping the persons features recognizable"
      },
      {
        "title": "Advanced video edit",
        "scenario": "Complex video transformation",
        "prompt": "Apply keyframe interpolation to create smooth slow-motion effect from this 2-second clip"
      }
    ],
    "output_examples": [
      {
        "input": "Generate a cyberpunk cityscape at night with neon lights",
        "output": [
          "Task submitted successfully",
          "Progress: 45% - Generating initial frames",
          "Progress: 100% - Complete",
          "Output URL: https://cdn.mulerun.com/generated/cyberpunk-city-12345.jpg"
        ]
      },
      {
        "input": "Create a 10-second video of a waterfall in a forest",
        "output": [
          "Task created: abc123def456",
          "Progress: 10% - Initializing model",
          "Progress: 50% - Rendering frames",
          "Progress: 100% - Encoding video",
          "Output URL: https://cdn.mulerun.com/generated/waterfall-xyz789.mp4"
        ]
      }
    ],
    "best_practices": [
      "Set appropriate timeouts - 5 minutes for images, 15 for videos",
      "Check model parameters before generation for best results",
      "Store API keys in environment variables, never in code",
      "Use specific, detailed prompts for better output quality"
    ],
    "anti_patterns": [
      "Hardcoding API keys in source code",
      "Using vague or generic prompts",
      "Not checking model availability before generation",
      "Ignoring async task status during long operations"
    ],
    "faq": [
      {
        "question": "How do I get an API key?",
        "answer": "Sign up at mulerouter.ai or mulerun.com to obtain your API key."
      },
      {
        "question": "Which models are supported?",
        "answer": "Alibaba Wan2.6 series and Google Nano Banana models for media tasks."
      },
      {
        "question": "How long does generation take?",
        "answer": "Images take 2-5 minutes, videos 10-15 minutes depending on complexity."
      },
      {
        "question": "Can I use my own images?",
        "answer": "Yes, provide image URLs for image-to-video or image-to-image transformations."
      },
      {
        "question": "What file formats are supported?",
        "answer": "JPG, PNG for images and MP4 for videos are supported."
      },
      {
        "question": "Is there a cost for using this?",
        "answer": "Yes, MuleRouter charges per generation based on model and output type."
      }
    ]
  },
