  "content": {
    "user_title": "Create custom skills for Claude",
    "value_statement": "Creating skills from scratch is time-consuming and error-prone. This skill provides structured guidance, templates, and scripts to help you build, validate, and package reusable skills that extend Claude with specialized capabilities.",
    "seo_keywords": [
      "skill development",
      "Claude Code skills",
      "custom skills",
      "skill templates",
      "Claude automation",
      "workflow automation",
      "skill creation guide",
      "Claude plugins"
    ],
    "actual_capabilities": [
      "Follows a 6-step process to create skills from concept to packaged distribution",
      "Initializes new skills with proper directory structure and template files",
      "Validates skills for correct YAML frontmatter, naming conventions, and file structure",
      "Packages skills into distributable zip files for sharing",
      "Provides templates for agent skills and subagent configurations"
    ],
    "limitations": [
      "Does not execute code or interact with external systems",
      "Requires manual editing of skill content based on domain knowledge",
      "Output is documentation and file structure, not direct task completion"
    ],
    "use_cases": [
      {
        "target_user": "AI Engineers",
        "title": "Build domain-specific skills",
        "description": "Create specialized skills for your industry with custom workflows, schemas, and best practices."
      },
      {
        "target_user": "Team Leads",
        "title": "Standardize team workflows",
        "description": "Package team processes into skills that ensure consistent execution across all Claude instances."
      },
      {
        "target_user": "Content Creators",
        "title": "Share expertise as skills",
        "description": "Turn detailed knowledge into reusable skills that others can use with Claude."
      }
    ],
    "prompt_templates": [
      {
        "title": "Create new skill",
        "scenario": "Start a new skill",
        "prompt": "Help me create a new skill called [skill-name] for [domain]. Follow the skill creation process to build it step by step."
      },
      {
        "title": "Package existing skill",
        "scenario": "Prepare skill for distribution",
        "prompt": "I want to package my existing skill at [path] for distribution. Use the packaging script to validate and create a zip file."
      },
      {
        "title": "Add resources to skill",
        "scenario": "Enhance existing skill",
        "prompt": "My [existing-skill] needs additional resources. Analyze what scripts, references, or assets would help, then add them."
      },
      {
        "title": "Iterate on skill",
        "scenario": "Improve skill after use",
        "prompt": "I used my [skill-name] skill for [task]. It struggled with [specific-issue]. How should I update the skill to handle this better?"
      }
    ],
    "output_examples": [
      {
        "input": "Help me create a pdf-editor skill for managing PDF documents",
        "output": [
          "Created skill structure at /path/to/pdf-editor",
          "Generated SKILL.md with PDF editing workflow",
          "Added example scripts/ directory with PDF utilities",
          "Next: Edit SKILL.md to add your PDF expertise"
        ]
      },
      {
        "input": "Package my brand-guidelines skill for the design team",
        "output": [
          "Validating brand-guidelines skill structure...",
          "Validation passed: frontmatter, naming, file structure all OK",
          "Packaged to brand-guidelines.zip (12 files)",
          "Ready to share with your team"
        ]
      }
    ],
    "best_practices": [
      "Keep SKILL.md under 5k words by moving detailed reference material to separate files in references/",
      "Use concrete examples to validate your skill's purpose before investing in full implementation",
      "Follow progressive disclosure design: essential info in metadata, core workflow in SKILL.md, details in references/"
    ],
    "anti_patterns": [
      "Including obvious or general knowledge that Claude already understands",
      "Duplicating information between SKILL.md and references files",
      "Leaving placeholder text (TODO items) in the final skill"
    ],
    "faq": [
      {
        "question": "What makes a good skill?",
        "answer": "Good skills contain non-obvious domain knowledge, reusable workflows, and bundled resources that would otherwise be rewritten repeatedly."
      },
      {
        "question": "How are skills different from prompts?",
        "answer": "Skills are structured packages with metadata, documentation, and bundled scripts. They persist and can be shared; prompts are one-off instructions."
      },
      {
        "question": "Can I create skills without scripts?",
        "answer": "Yes. Many skills only need SKILL.md with documentation and references/. Scripts are optional for deterministic or repeated operations."
      },
      {
        "question": "How do I test my skill?",
        "answer": "Use the skill on real tasks. Notice struggles or inefficiencies, then iterate by updating SKILL.md or adding bundled resources."
      },
      {
        "question": "What goes in assets versus references?",
        "answer": "Assets are used in output (templates, logos). References are loaded into context (schemas, docs). Assets stay external; references inform Claude."
      },
      {
        "question": "How do I share my skill with others?",
        "answer": "Run scripts/package_skill.py to create a distributable zip file. Share the zip file; recipients can extract it to their skills directory."
      }
    ]
  },
