  "content": {
    "user_title": "Analyze single-cell RNA-seq data",
    "value_statement": "Single-cell RNA sequencing generates complex datasets requiring specialized analysis. This skill provides a complete workflow for quality control, dimensionality reduction, clustering, and visualization of single-cell gene expression data.",
    "seo_keywords": [
      "scanpy",
      "single-cell RNA-seq",
      "single-cell analysis",
      "bioinformatics",
      "scRNA-seq",
      "cell clustering",
      "UMAP",
      "t-SNE",
      "Claude Code",
      "Claude"
    ],
    "actual_capabilities": [
      "Load and preprocess single-cell data from 10X, h5ad, CSV formats",
      "Perform quality control and filter low-quality cells and genes",
      "Apply dimensionality reduction techniques including PCA, UMAP, and t-SNE",
      "Identify cell clusters using Leiden and Louvain algorithms",
      "Find marker genes and annotate cell types from gene expression",
      "Generate publication-quality visualizations of single-cell data"
    ],
    "limitations": [
      "Requires existing scanpy installation and dependencies",
      "Processes local files only, no direct database connections",
      "Does not include deep learning models (use scvi-tools for those)",
      "Performance scales with available memory for large datasets"
    ],
    "use_cases": [
      {
        "target_user": "Bioinformatics researchers",
        "title": "Exploratory scRNA-seq analysis",
        "description": "Analyze single-cell gene expression datasets to identify cell types, states, and populations."
      },
      {
        "target_user": "Computational biologists",
        "title": "Marker gene discovery",
        "description": "Identify differentially expressed genes between clusters and characterize cell populations."
      },
      {
        "target_user": "Data scientists",
        "title": "Visualization pipelines",
        "description": "Generate UMAP, t-SNE, and other dimensionality reduction plots for publications."
      }
    ],
    "prompt_templates": [
      {
        "title": "Load and inspect data",
        "scenario": "New single-cell dataset",
        "prompt": "Load my single-cell data from data.h5ad and show me the basic structure including cell and gene counts."
      },
      {
        "title": "QC and filtering",
        "scenario": "Quality control workflow",
        "prompt": "Run quality control on my dataset, filter cells with less than 200 genes or more than 5% mitochondrial reads, and generate QC plots."
      },
      {
        "title": "Cluster and annotate",
        "scenario": "Cell type identification",
        "prompt": "Perform clustering at resolution 0.5, generate UMAP visualization, and identify marker genes for each cluster."
      },
      {
        "title": "Full analysis",
        "scenario": "Complete pipeline",
        "prompt": "Run a complete scanpy workflow: QC, normalization, highly variable genes, PCA, neighbors, UMAP, Leiden clustering at resolution 0.8, and save results."
      }
    ],
    "output_examples": [
      {
        "input": "Load my single-cell data and perform quality control",
        "output": [
          "Loaded 3,245 cells x 20,000 genes",
          "QC metrics: mean 1,542 genes per cell, 4.2% mitochondrial reads",
          "After filtering: 2,987 cells x 15,432 genes (92% cells retained)",
          "Saved QC violin plots to figures/qc_violin.pdf"
        ]
      }
    ],
    "best_practices": [
      "Always save raw counts before filtering: adata.raw = adata",
      "Validate clustering by checking known marker gene expression",
      "Save intermediate results to avoid re-running long workflows"
    ],
    "anti_patterns": [
      "Skipping quality control steps before downstream analysis",
      "Using default clustering resolution without testing multiple values",
      "Not visualizing data before and after filtering steps"
    ],
    "faq": [
      {
        "question": "What file formats does scanpy support?",
        "answer": "Scanpy supports h5ad, 10X Genomics MTX, HDF5, CSV, loom, and text files."
      },
      {
        "question": "How many neighbors should I use for UMAP?",
        "answer": "Default is 10-30 neighbors. Lower values preserve local structure; higher values capture global patterns."
      },
      {
        "question": "Leiden vs Louvain clustering?",
        "answer": "Leiden is recommended as it produces better-quality clusters and is more efficient than Louvain."
      },
      {
        "question": "How many PCs should I use?",
        "answer": "Check the PCA variance ratio plot. Use PCs before the elbow, typically 30-50 for datasets with high variability."
      },
      {
        "question": "What is the mitochondrial threshold?",
        "answer": "Typical threshold is 5-20%. Lower thresholds remove more cells; validate with known cell type markers."
      },
      {
        "question": "How to annotate cell types?",
        "answer": "Use known marker genes for cell types. Visualize expression on UMAP and compare cluster marker genes to reference databases."
      }
    ]
  },
