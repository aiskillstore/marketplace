  "content": {
    "user_title": "Query public statistics from Data Commons",
    "value_statement": "Access global statistical data from Data Commons including demographics, economics, health, and environmental indicators. Query population figures, GDP, unemployment rates, and geographic relationships using Python client methods.",
    "seo_keywords": [
      "datacommons",
      "statistical data",
      "demographic data",
      "population statistics",
      "economic indicators",
      "API integration",
      "Claude",
      "Codex",
      "Claude Code",
      "public datasets"
    ],
    "actual_capabilities": [
      "Query statistical observations by variable and entity",
      "Resolve place names and coordinates to Data Commons IDs",
      "Explore knowledge graph relationships between entities",
      "Access time-series data for historical trends",
      "Filter data by source for consistency"
    ],
    "limitations": [
      "Requires Data Commons API key for datacommons.org access",
      "Only supports place entities for resolution (countries, states, cities)",
      "Cannot query non-place entities directly without DCID knowledge"
    ],
    "use_cases": [
      {
        "target_user": "Data analysts",
        "title": "Compare regional statistics",
        "description": "Query and compare population, income, and unemployment data across multiple states or countries."
      },
      {
        "target_user": "Researchers",
        "title": "Access historical trends",
        "description": "Retrieve time-series data for economic indicators, health statistics, or environmental measurements."
      },
      {
        "target_user": "Developers",
        "title": "Build data-driven applications",
        "description": "Integrate public statistical data into applications using Python client library methods."
      }
    ],
    "prompt_templates": [
      {
        "title": "Basic query",
        "scenario": "Get latest population",
        "prompt": "Get the latest population for California, Texas, and New York using the Data Commons client."
      },
      {
        "title": "Time series",
        "scenario": "Historical unemployment",
        "prompt": "Query the unemployment rate time series for the United States from 2010 to 2023."
      },
      {
        "title": "Geographic hierarchy",
        "scenario": "All counties in state",
        "prompt": "Get median household income for all counties in California for year 2020."
      },
      {
        "title": "Multi-variable",
        "scenario": "State comparison",
        "prompt": "Compare population, median income, and median age across Florida, Georgia, and South Carolina."
      }
    ],
    "output_examples": [
      {
        "input": "Get the population of France and Germany",
        "output": [
          "France: 67,848,156 people (2023)",
          "Germany: 84,358,845 people (2023)",
          "Data source: World Bank"
        ]
      },
      {
        "input": "Show US unemployment trend from 2018 to 2023",
        "output": [
          "2018: 3.9%",
          "2019: 3.7%",
          "2020: 8.1%",
          "2021: 5.4%",
          "2022: 3.6%",
          "2023: 3.6%"
        ]
      }
    ],
    "best_practices": [
      "Always resolve place names to DCIDs before querying to handle ambiguous names",
      "Use entity expressions to query hierarchies efficiently (all counties in a state at once)",
      "Cache DCID resolutions when querying the same entities repeatedly"
    ],
    "anti_patterns": [
      "Hardcoding DCIDs instead of resolving names dynamically",
      "Making individual queries for each entity instead of batch queries",
      "Ignoring data source facets when consistency matters"
    ],
    "faq": [
      {
        "question": "What is Data Commons?",
        "answer": "Data Commons is a platform aggregating public statistical data from sources like census bureaus and health organizations into a unified knowledge graph."
      },
      {
        "question": "Do I need an API key?",
        "answer": "Yes, for datacommons.org. Request one at apikeys.datacommons.org and set it via DC_API_KEY environment variable."
      },
      {
        "question": "What data types are available?",
        "answer": "Population, income, unemployment, health statistics, environmental data, and geographic relationships for places worldwide."
      },
      {
        "question": "How do I find statistical variables?",
        "answer": "Use fetch_available_statistical_variables() to check what data exists for an entity, or browse at datacommons.org/tools/statvar."
      },
      {
        "question": "Can I query historical data?",
        "answer": "Yes, set date parameter to 'all' to retrieve complete time series for trend analysis."
      },
      {
        "question": "What are DCIDs?",
        "answer": "Data Commons Identifiers are unique keys for entities. Use the Resolve API to convert place names to DCIDs."
      }
    ]
