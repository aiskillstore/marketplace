  "content": {
    "user_title": "Search Protein Data Bank for molecular structures",
    "value_statement": "Access the RCSB Protein Data Bank to search for and download 3D structures of proteins and nucleic acids. This skill enables structural biology research, drug discovery, and protein engineering by providing programmatic access to over 200,000 experimentally determined and computationally modeled molecular structures.",
    "seo_keywords": [
      "Protein Data Bank",
      "PDB database",
      "molecular structure retrieval",
      "protein structure search",
      "RCSB PDB API",
      "structural biology",
      "Claude Code",
      "Claude",
      "Codex",
      "drug discovery"
    ],
    "actual_capabilities": [
      "Search PDB by text, protein name, or keywords",
      "Search by sequence similarity using MMseqs2",
      "Search by 3D structure similarity using BioZernike",
      "Download coordinate files in PDB, mmCIF, or BinaryCIF formats",
      "Retrieve metadata including experimental method, resolution, and deposition date",
      "Perform batch operations across multiple structures"
    ],
    "limitations": [
      "Requires internet connection to access RCSB PDB public API",
      "Rate limits apply for high-volume requests",
