    "user_title": "Query ChEMBL bioactive molecules and drug discovery data",
    "value_statement": "Access the ChEMBL database to search compounds by structure and properties. Retrieve bioactivity measurements, find inhibitors, and perform structure-activity relationship studies for drug discovery research.",
    "seo_keywords": [
      "ChEMBL database",
      "drug discovery",
      "bioactive molecules",
      "molecular database",
      "IC50 Ki EC50",
      "cheminformatics",
      "Claude Code",
      "Claude",
      "Codex",
      "inhibitor search"
    ],
    "actual_capabilities": [
      "Search molecules by name, SMILES structure, or physicochemical properties",
      "Retrieve bioactivity data including IC50, Ki, EC50, and Kd values",
      "Find similar compounds and perform substructure searches",
      "Query drug information, mechanisms of action, and clinical indications",
      "Identify bioactive compounds",
      "kinase inhibitors and other",
      "Export query results to pandas DataFrames for analysis"
    ],
    "limitations": [
      "Rate limited by ChEMBL API fair usage policies",
      "Cannot access local databases - only queries the ChEMBL web API",
      "Data completeness depends on ChEMBL curation status"
    ],
    "use_cases": [
      {
        "target_user": "Medicinal Chemists",
        "title": "Find Inhibitors for Drug Targets",
        "description": "Search for potent inhibitors of specific targets like EGFR or kinases by querying bioactivity measurements."
      },
      {
        "target_user": "Pharmacologists",
        "title": "Analyze Drug-Target Interactions",
        "description": "Retrieve drug mechanisms, indications, and activity profiles to understand compound-target relationships."
      },
      {
        "target_user": "Computational Chemists",
        "title": "Structure-Activity Relationship Studies",
        "description": "Find similar compounds and analyze molecular properties to identify structure-activity patterns."
      }
    ],
    "prompt_templates": [
      {
