    "user_title": "Query GWAS Catalog for SNP Trait Associations",
    "value_statement": "Access comprehensive genome-wide association study data from the NHGRI-EBI GWAS Catalog. Search variants by rs ID, trait, or gene to retrieve p-values, effect sizes, and summary statistics for genetic research.",
    "seo_keywords": [
      "Claude",
      "Codex",
      "Claude Code",
      "GWAS",
      "genetics",
      "SNP",
      "genome-wide association",
      "EBI",
      "NHGRI",
      "bioinformatics"
    ],
    "actual_capabilities": [
      "Query SNP-trait associations from the GWAS Catalog",
      "Search variants by rs ID, disease trait, or gene symbol",
      "Retrieve p-values, effect sizes, and odds ratios",
      "Access summary statistics for genome-wide studies"
    ],
    "limitations": [
      "API rate limits apply to excessive requests",
      "Full summary statistics not available for all studies",
      "Requires EFO trait identifiers for precise trait queries"
    ],
    "use_cases": [
      {
        "target_user": "Genetic epidemiologists",
        "title": "Find disease-associated variants",
        "description": "Query the GWAS Catalog to identify genetic variants associated with specific diseases or traits for research studies."
      },
      {
        "target_user": "Clinical researchers",
        "title": "Build polygenic risk scores",
        "description": "Retrieve significant SNP associations with p-values and effect sizes to develop polygenic risk prediction models."
      },
      {
        "target_user": "Drug discovery scientists",
        "title": "Identify drug targets",
        "description": "Explore genetic associations between variants and traits to identify potential therapeutic targets and biomarkers."
      }
    ],
    "prompt_templates": [
      {
        "title": "Find variant associations",
        "scenario": "Query by rs ID",
        "prompt": "Show me all trait associations for the genetic variant rs7903146 including p-values and risk alleles."
      },
      {
        "title": "Search by disease trait",
        "scenario": "Query by trait name",
        "prompt": "Find all genetic variants associated with type 2 diabetes from the GWAS Catalog with genome-wide significance."
      },
      {
        "title": "Gene-centric analysis",
        "scenario": "Query by gene symbol",
        "prompt": "What variants in or near the TCF7L2 gene are associated with any traits in the GWAS Catalog?"
      },
      {
        "title": "Summary statistics access",
        "scenario": "Get full study data",
        "prompt": "Retrieve the summary statistics for GWAS study GCST001234 including all tested variants and their effect sizes."
      }
