  "content": {
    "user_title": "Analyze genomic intervals with machine learning",
    "value_statement": "Geniml enables machine learning on genomic interval data from BED files. Train embeddings for region similarity, cluster single cells, and build consensus peak references for downstream bioinformatics analysis.",
    "seo_keywords": [
      "genomic intervals",
      "BED files",
      "machine learning",
      "embeddings",
      "single-cell analysis",
      "chromatin accessibility",
      "genomics",
      "bioinformatics",
      "Claude",
      "Claude Code"
    ],
    "actual_capabilities": [
      "Train Region2Vec embeddings for genomic regions from BED files",
      "Generate joint region and metadata embeddings with BEDspace",
      "Create single-cell ATAC-seq embeddings with scEmbed",
      "Build consensus peak universes from multiple experiments",
      "Evaluate embedding quality with clustering metrics",
      "Query similar regions across datasets"
    ],
    "limitations": [
      "Requires BED file format input for region analysis",
      "StarSpace must be installed separately for BEDspace training",
      "Pre-trained models available only for common reference datasets",
      "Memory intensive for large-scale genome-wide analysis"
    ],
    "use_cases": [
      {
        "target_user": "Bioinformatics researchers",
        "title": "Genomic region embedding",
        "description": "Convert BED file collections into low-dimensional embeddings for clustering and similarity analysis"
      },
      {
        "target_user": "Single-cell analysts",
        "title": "scATAC-seq cell clustering",
        "description": "Generate cell embeddings from chromatin accessibility data for cell-type annotation"
      },
      {
        "target_user": "Genomics pipelines",
        "title": "Consensus peak building",
        "description": "Create standardized reference peak sets from multiple experiments for reproducible analysis"
      }
    ],
    "prompt_templates": [
      {
        "title": "Basic region embedding",
        "scenario": "Convert genomic regions to vectors",
        "prompt": "Use geniml to train Region2Vec embeddings on my BED files in bed_files/ using universe.bed as reference. Save the model to model_output/"
      },
      {
