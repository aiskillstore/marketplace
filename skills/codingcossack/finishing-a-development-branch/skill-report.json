  "content": {
    "user_title": "Finish Git development branches",
    "value_statement": "Managing feature branch completion manually takes time and risks errors. This skill automates the workflow for merging, creating pull requests, preserving, or discarding branches with built-in test verification and safety checks.",
    "seo_keywords": [
      "git branch completion",
      "git merge workflow",
      "pull request automation",
      "git branch cleanup",
      "development workflow",
      "Claude Code skill",
      "Codex skill",
      "Claude skill",
      "git worktree",
      "branch management"
    ],
    "actual_capabilities": [
      "Detects test runner from project files and runs tests",
      "Identifies the correct base branch for merge",
      "Presents four options: merge locally, create PR, preserve, or discard",
      "Executes Git operations with safety guards and confirmations"
    ],
    "limitations": [
      "Does not resolve merge conflicts automatically",
      "Requires GitHub CLI installed for automated PR creation",
      "Does not force-push without explicit user request"
    ],
    "use_cases": [
      {
        "target_user": "Software developers",
        "title": "Complete feature development",
        "description": "Integrate finished feature branches into main line with merge or pull request."
      },
      {
        "target_user": "DevOps engineers",
        "title": "Standardize branch cleanup",
        "description": "Enforce consistent branch completion workflow across teams with safety checks."
      },
      {
        "target_user": "AI coding assistants",
        "title": "End-of-task branch handling",
        "description": "Clean up worktrees and branches after AI-assisted implementation tasks complete."
      }
    ],
    "prompt_templates": [
      {
        "title": "Basic merge",
        "scenario": "Feature complete and tested",
        "prompt": "Use finishing-a-development-branch to merge this feature branch into main."
      },
      {
        "title": "Create pull request",
        "scenario": "Ready for code review",
        "prompt": "Use finishing-a-development-branch to push this branch and create a pull request on GitHub."
      },
      {
        "title": "Discard with confirmation",
        "scenario": "Abandon experimental work",
        "prompt": "Use finishing-a-development-branch to discard this experimental branch after I confirm."
      },
      {
        "title": "Worktree cleanup",
        "scenario": "Merged successfully, clean up",
        "prompt": "The feature is merged. Use finishing-a-development-branch to clean up the worktree."
      }
    ],
