  "content": {
    "user_title": "Create and edit Word documents",
    "value_statement": "Need to create professional Word documents or edit existing files with tracked changes? This skill provides complete tools for DOCX manipulation including comments, formatting preservation, and schema validation.",
    "seo_keywords": [
      "DOCX editor",
      "Word document creation",
      "Claude Code",
      "tracked changes",
      "document formatting",
      "Office Open XML",
      "Word editing",
      "docx manipulation",
      "document templates",
      "OOXML"
    ],
    "actual_capabilities": [
      "Create new DOCX files from JavaScript using the docx library",
      "Unpack existing DOCX files into XML for detailed editing",
      "Edit Word document XML directly with tracked changes support",
      "Add and manage comments in Word documents",
      "Validate documents against OOXML schema standards",
      "Repack edited XML directories back into valid DOCX files"
    ],
    "limitations": [
      "Does not modify PDF or other non-Word document formats",
      "Requires external tools (pandoc, LibreOffice) for full workflow",
      "Complex formatting changes require direct XML manipulation"
    ],
    "use_cases": [
      {
        "target_user": "Legal professionals",
        "title": "Review legal documents",
        "description": "Edit contracts and legal documents while preserving version history with proper tracked changes"
      },
      {
        "target_user": "Technical writers",
        "title": "Generate documentation",
        "description": "Create professional Word documents with tables, images, and consistent styling programmatically"
      },
      {
        "target_user": "Business analysts",
        "title": "Collaborate on documents",
        "description": "Add comments, suggest edits, and review business documents with full change tracking"
      }
    ],
    "prompt_templates": [
      {
        "title": "Create simple document",
        "scenario": "Generate a basic Word file",
        "prompt": "Create a new Word document at /tmp/report.docx with a title heading and three paragraphs of sample text about quarterly results"
      },
      {
        "title": "Edit with changes",
        "scenario": "Modify existing document",
        "prompt": "Edit the document at /tmp/contract.docx to change all occurrences of '30 days' to '45 days' with tracked changes enabled"
      },
      {
        "title": "Add comments",
        "scenario": "Review with annotations",
        "prompt": "Add comments to /tmp/proposal.docx explaining the budget section needs more detail"
      },
      {
        "title": "Complex formatting",
        "scenario": "Create styled document",
        "prompt": "Create a professional invoice document with header logo, customer address table, and line items"
      }
    ],
    "output_examples": [
      {
        "input": "Create a Word document with a title and two paragraphs",
        "output": [
          "Created: /tmp/report.docx",
          "Document structure:",
          "  - Title: Quarterly Business Review",
          "  - Paragraph 1: Executive summary (85 words)",
          "  - Paragraph 2: Key metrics and highlights (120 words)",
          "  - Formatting: Arial 11pt, 1-inch margins"
        ]
      },
      {
        "input": "Edit contract to change payment terms",
        "output": [
          "Modified: /tmp/contract.docx",
          "Tracked changes applied:",
          "  - Changed 'Net 30' to 'Net 45' (1 location)",
          "  - Author: Claude",
          "  - RSID: 00AB1234"
        ]
      }
    ],
    "best_practices": [
      "Use tracked changes for all edits to maintain document history and enable collaboration review",
      "Validate documents before repacking to ensure OOXML schema compliance",
      "Preserve original RSID values when modifying existing content for compatibility"
    ],
    "anti_patterns": [
      "Avoid modifying text inside another author's tracked changes without proper nesting",
      "Do not skip XML validation - it catches structural issues that cause corrupt files",
      "Never use unicode bullets instead of proper numbering configuration"
    ],
    "faq": [
      {
        "question": "Can this skill create DOCX files only, or also edit existing ones?",
        "answer": "Both. Use docx-js library for creating new documents and the Document class for editing existing files with tracked changes."
      },
      {
        "question": "What external dependencies are required?",
        "answer": "Pandoc for text extraction, LibreOffice for validation, and defusedxml for secure XML parsing. All are standard tools."
      },
      {
        "question": "How does tracked changes work with multiple authors?",
        "answer": "The skill auto-nests changes when editing another author's edits. Use revert methods to accept or reject changes."
      },
      {
        "question": "Is my data safe when processing documents?",
        "answer": "Yes. All XML parsing uses defusedxml to prevent XXE attacks. Documents are processed locally with no network data transmission."
      },
      {
        "question": "Why does validation fail after my edits?",
        "answer": "Check for malformed XML, duplicate IDs, or broken relationships. The validator checks OOXML schema compliance."
      },
      {
        "question": "How is this different from python-docx?",
        "answer": "This skill works directly with OOXML structure for precise control over tracked changes, comments, and complex formatting."
      }
    ]
  },
