    "actual_capabilities": [
      "Deep copy and merge arrays and objects",
      "Format strings with template interpolation",
      "Calculate date ranges and relative time",
      "Format currency and percentage values",
      "Copy text to clipboard",
      "Check empty values and deep equality"
    ],
    "limitations": [
      "Requires @delon/util npm package installation",
      "Designed for Angular/ng-alain applications",
      "Does not include runtime validation",
      "Browser utilities only work in browser environment"
    ],
