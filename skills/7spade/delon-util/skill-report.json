      {
        "target_user": "Angular developers",
        "title": "Data transformation in components",
        "description": "Transform and format data for display in Angular templates using utility functions"
      },
      {
        "target_user": "Full-stack developers",
        "title": "Form data handling",
        "description": "Clone and merge form data with default values safely without mutating originals"
      },
      {
        "target_user": "Backend developers",
        "title": "API response processing",
        "description": "Process and normalize API responses using deep copy and grouping utilities"
      }
    ],
    "prompt_templates": [
      {
        "title": "Deep copy objects",
        "scenario": "Clone data without mutation",
        "prompt": "Use deepCopy to create an independent copy of this object. Show how changes to the copy do not affect the original."
      },
      {
        "title": "Format strings",
        "scenario": "Template-based string formatting",
        "prompt": "Use the format function to create a message with template variables. Demonstrate both indexed and named parameters."
      },
      {
        "title": "Calculate date ranges",
        "scenario": "Date range calculations",
        "prompt": "Use getTimeDistance to get the start and end dates for this week. Show how to calculate relative dates with offset."
      },
      {
        "title": "Group and sort data",
        "scenario": "Data organization",
        "prompt": "Use groupBy and orderBy to organize this list of items. Group by status field and sort by priority."
      }
    ],
    "output_examples": [
      {
