  "content": {
    "user_title": "Generate scientific hypotheses with AI",
    "value_statement": "Manual hypothesis generation is slow and limited by human creativity. Hypogenic automates systematic hypothesis generation and testing using LLMs, combining data-driven patterns with literature insights for accelerated scientific discovery.",
    "seo_keywords": [
      "hypogenic",
      "hypothesis generation",
      "scientific discovery",
      "Claude",
      "LLM research",
      "empirical research",
      "AI hypothesis testing",
      "data analysis",
      "scientific AI",
      "research automation"
    ],
    "actual_capabilities": [
      "Generate 10-20+ testable hypotheses from observational datasets",
      "Combine literature insights with empirical data patterns",
      "Test hypotheses against test data with voting and ensemble methods",
      "Support OpenAI, Anthropic, and local LLM providers",
      "Iterative refinement based on validation performance",
      "Process research papers for literature-integrated hypothesis generation"
    ],
    "limitations": [
      "Requires external hypogenic Python package installation via pip",
      "API keys for OpenAI or Anthropic models must be configured",
      "Datasets must be prepared in specific JSON format with text features and labels",
      "GROBID setup required for literature PDF processing"
    ],
    "use_cases": [
      {
        "target_user": "Academic researchers",
        "title": "Accelerate literature review",
        "description": "Generate testable hypotheses from datasets and validate against existing research papers"
      },
      {
        "target_user": "Data scientists",
        "title": "Pattern discovery",
        "description": "Automatically discover hypotheses from data patterns without existing theoretical frameworks"
      },
      {
        "target_user": "AI safety researchers",
        "title": "Deception detection",
        "description": "Systematically test hypotheses about AI-generated content and behavioral patterns"
      }
    ],
    "prompt_templates": [
      {
        "title": "Basic hypothesis request",
        "scenario": "Generate hypotheses from dataset",
        "prompt": "Generate 20 testable hypotheses from this observational dataset about patterns that distinguish between classes. Focus on specific, measurable characteristics."
      },
      {
        "title": "Literature integration",
        "scenario": "Combine research with data",
        "prompt": "Based on these research paper insights and observational data patterns, generate hypotheses that combine theoretical foundations with empirical observations."
      },
      {
        "title": "Hypothesis refinement",
        "scenario": "Improve weak hypotheses",
        "prompt": "These hypotheses performed poorly on challenging examples. Generate improved versions that address the failures while maintaining the core insight."
      },
      {
        "title": "Multi-hypothesis testing",
        "scenario": "Test multiple hypotheses",
        "prompt": "Run inference using multiple hypotheses from the hypothesis bank. Report which hypotheses best explain each test sample and their combined predictions."
      }
    ],
    "output_examples": [
      {
        "input": "Generate hypotheses for detecting AI-generated text vs human writing",
        "output": [
          "Hypothesis 1: AI-generated text contains fewer grammatical errors than human writing",
          "Hypothesis 2: AI text shows higher formality consistency across sentences",
          "Hypothesis 3: Human text contains more personal pronouns and emotional language",
          "Hypothesis 4: AI-generated content has more uniform sentence length distribution"
        ]
      },
      {
        "input": "Generate hypotheses about stress indicators in social media posts",
        "output": [
          "Hypothesis 1: Stressed individuals use more first-person singular pronouns",
          "Hypothesis 2: Social media posts from stressed users contain more negative emotion words",
          "Hypothesis 3: Posting frequency decreases during high-stress periods",
          "Hypothesis 4: Stressed users share fewer links and external content"
        ]
      }
    ],
    "best_practices": [
      "Prepare datasets in the required JSON format with text features and labels",
      "Start with 20 hypotheses and increase based on validation performance",
      "Use Redis caching to reduce API costs for iterative experiments",
      "Customize extract_label function for domain-specific LLM output parsing"
    ],
    "anti_patterns": [
      "Using the same hypothesis bank without iterative refinement",
      "Neglecting to validate hypotheses against a holdout test set",
      "Ignoring literature integration when theoretical frameworks exist",
      "Failing to customize prompt templates for your specific domain"
    ],
    "faq": [
      {
        "question": "Which LLM providers are supported?",
        "answer": "OpenAI (GPT-4, GPT-3.5-turbo), Anthropic (Claude models), and local LLMs via compatible APIs."
      },
      {
        "question": "How many hypotheses should I generate?",
        "answer": "Start with 20 hypotheses. The framework supports iterative refinement to replace poorly-performing ones."
      },
      {
        "question": "Can I use local models instead of APIs?",
        "answer": "Yes, Hypogenic supports local LLMs through compatible API interfaces for reduced costs."
      },
      {
        "question": "Is my data sent to external servers?",
        "answer": "Data is sent to your configured LLM API provider. Configure which provider to use in config.yaml."
      },
      {
        "question": "Why are my hypotheses too generic?",
        "answer": "Refine prompt templates in config.yaml to request more specific, testable hypotheses with clear measurable characteristics."
      },
      {
        "question": "How does this compare to manual hypothesis generation?",
        "answer": "Studies show 8-15% improvement over few-shot and literature-only baselines with 80-84% hypothesis diversity."
      }
    ]
