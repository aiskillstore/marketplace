  "content": {
    "user_title": "Download Cancer Mutation Data from COSMIC Database",
    "value_statement": "Cancer researchers need programmatic access to the world's largest somatic mutation database. This skill enables authenticated downloads of COSMIC cancer genomics data including mutations, gene annotations, and clinical variants for research and precision oncology.",
    "seo_keywords": [
      "COSMIC database",
      "cancer mutations",
      "genomics data",
      "bioinformatics",
      "Claude Code",
      "cancer research",
      "somatic mutations",
      "precision oncology",
      "Claude",
      "data download"
    ],
    "actual_capabilities": [
      "Download authenticated cancer mutation data from COSMIC database API",
      "Access Cancer Gene Census with 700+ curated cancer genes",
      "Retrieve mutational signatures and structural variant data",
      "Download gene fusions, copy number alterations, and resistance mutations",
      "Support for both GRCh37 and GRCh38 genome assemblies",
      "Process VCF and TSV format genomic data files"
    ],
    "limitations": [
      "Requires valid COSMIC account registration for data access",
      "Commercial users need separate QIAGEN license agreement",
      "Large file downloads may require significant disk space and time",
      "Does not perform mutation analysis or variant annotation"
    ],
    "use_cases": [
      {
        "target_user": "Cancer researchers",
        "title": "Download mutation datasets for analysis",
        "description": "Retrieve comprehensive somatic mutation data for specific cancer types or genes to support research studies and publications."
      },
      {
        "target_user": "Bioinformaticians",
        "title": "Build cancer genomics data pipelines",
        "description": "Integrate COSMIC data downloads into automated bioinformatics workflows for variant annotation and clinical interpretation."
      },
      {
        "target_user": "Precision oncology teams",
        "title": "Access drug resistance mutation data",
        "description": "Download clinically annotated resistance mutations and gene census data to support treatment decision making and biomarker discovery."
      }
    ],
    "prompt_templates": [
      {
        "title": "Download mutation data",
        "scenario": "Get complete coding mutations",
        "prompt": "Download the latest COSMIC coding mutations file for GRCh38 using my credentials"
      },
      {
        "title": "Access Cancer Gene Census",
        "scenario": "Retrieve curated cancer gene list",
        "prompt": "Download the Cancer Gene Census file and show me oncogenes versus tumor suppressors"
      },
      {
        "title": "Get structural variants",
        "scenario": "Retrieve gene fusion data",
        "prompt": "Download COSMIC gene fusion data for the latest version and extract fusions involving NTRK genes"
      },
      {
        "title": "Download resistance mutations",
        "scenario": "Access drug resistance data",
        "prompt": "Download COSMIC resistance mutations file and filter for mutations associated with tyrosine kinase inhibitor resistance"
      }
    ],
    "output_examples": [
      {
        "input": "Download the Cancer Gene Census for GRCh38",
        "output": [
          "Authenticated with COSMIC using provided credentials",
          "Downloaded cancer_gene_census.csv (723 cancer genes)",
          "File contains expert-curated gene annotations with roles, mutation types, and tissue associations",
          "Data includes 356 oncogenes and 367 tumor suppressor genes"
        ]
      },
      {
        "input": "Get the latest COSMIC mutations for TP53 gene",
        "output": [
          "Downloaded CosmicMutantExport.tsv.gz (3.2 GB compressed)",
          "File contains 8.7 million somatic mutations across all cancer types",
          "TP53 has 47,823 unique mutations documented in COSMIC",
          "Data includes genomic coordinates, amino acid changes, and tumor type associations"
        ]
      },
      {
        "input": "Download mutational signature definitions",
        "output": [
          "Retrieved signatures.tsv from COSMIC signatures directory",
          "File contains 96-channel profiles for 67 SBS signatures",
          "Includes etiology annotations linking signatures to mutational processes",
          "Compatible with SigProfiler and deconstructSigs analysis tools"
        ]
      }
    ],
    "best_practices": [
      "Always use latest version for current data unless reproducibility requires specific version",
      "Verify genome assembly matches your analysis pipeline to avoid coordinate mismatches",
      "Use data type shortcuts when possible to simplify download commands and reduce errors"
    ],
    "anti_patterns": [
      "Do not hardcode passwords in scripts or share credentials in version control systems",
      "Avoid downloading entire mutation export files when gene-specific queries would suffice",
      "Do not use GRCh37 assembly for new projects as GRCh38 is the current standard"
    ],
    "faq": [
      {
        "question": "Do I need a COSMIC account to use this skill?",
        "answer": "Yes. Academic users get free access with registration at cancer.sanger.ac.uk/cosmic/register. Commercial users require a QIAGEN license."
      },
      {
        "question": "How large are COSMIC data files?",
        "answer": "Complete mutation exports can be several gigabytes. Specific datasets like Cancer Gene Census are smaller (under 1 MB). Check available disk space."
      },
      {
        "question": "Which genome assembly should I use?",
        "answer": "GRCh38 is recommended for new projects as the current standard. Use GRCh37 only if required for compatibility with legacy analysis pipelines."
      },
      {
        "question": "How often is COSMIC data updated?",
        "answer": "COSMIC releases quarterly updates. Use latest in file paths to always get the most recent version or specify version numbers for reproducibility."
      },
      {
        "question": "Can I download data for specific genes only?",
        "answer": "The skill downloads complete files. Filter by gene after download using pandas or command-line tools. CosmicMutantExportCensus.tsv.gz contains only Cancer Gene Census genes."
      },
      {
        "question": "What file formats does COSMIC provide?",
        "answer": "TSV and CSV for tabular data, VCF for variants. All files are gzip compressed. Read with pandas, pysam, bcftools, or standard decompression tools."
      }
    ]
