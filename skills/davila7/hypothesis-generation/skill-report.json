  "content": {
    "user_title": "Generate Scientific Hypotheses",
    "value_statement": "Researchers struggle to develop rigorous testable hypotheses grounded in evidence. This skill provides a systematic 8-step workflow to formulate mechanistic hypotheses, design discriminating experiments, and create professional LaTeX reports.",
    "seo_keywords": [
      "scientific hypothesis generation",
      "hypothesis testing",
      "experimental design",
      "research methodology",
      "literature review",
      "scientific inquiry",
      "Claude",
      "Claude Code",
      "testable predictions",
      "scientific research"
    ],
    "actual_capabilities": [
      "Generate 3-5 competing mechanistic hypotheses from observations",
      "Evaluate hypotheses against quality criteria (testability, falsifiability, parsimony)",
      "Design experiments to test and distinguish between hypotheses",
      "Create publication-ready LaTeX hypothesis reports with colored boxes",
      "Search PubMed and web for relevant scientific literature",
      "Synthesize evidence supporting and challenging each hypothesis"
    ],
    "limitations": [
      "Does not provide domain-specific scientific expertise",
      "Cannot perform actual experiments or access physical systems",
      "Relies on publicly available online literature",
      "Generated hypotheses require validation by subject matter experts"
    ],
    "use_cases": [
      {
        "target_user": "Academic Researchers",
        "title": "Grant Proposal Development",
        "description": "Develop rigorous hypotheses with supporting evidence for research funding applications"
      },
      {
        "target_user": "Graduate Students",
        "title": "Thesis Planning",
        "description": "Structure research questions and design experimental approaches for dissertations"
