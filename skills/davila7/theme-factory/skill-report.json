    "user_title": "Apply professional themes to presentations",
    "value_statement": "Creating consistent, professional visual themes for presentations and documents takes significant design effort. This skill provides 10 curated color and font theme combinations that can be instantly applied to any artifact, plus the ability to generate custom themes on demand.",
    "seo_keywords": [
      "theme factory",
      "claude design themes",
      "color palette generator",
      "presentation themes",
      "font pairing",
      "style guide",
      "design system",
      "claude code themes",
      "document styling",
      "slide design"
    ],
    "actual_capabilities": [
      "Apply 10 professional pre-set themes to any artifact",
      "Generate custom themes based on description inputs",
      "Access curated color palettes with hex codes",
      "Use matched font pairings for headers and body text",
      "Display theme showcase for visual selection"
    ],
    "limitations": [
      "Does not directly modify files - provides guidance only",
      "Themes are preset combinations - customizability is prompt-based",
      "Does not integrate with design software APIs",
      "Font availability depends on system fonts"
    ],
    "use_cases": [
      {
        "target_user": "Business professionals",
        "title": "Corporate presentation styling",
        "description": "Apply Ocean Depths or Modern Minimalist themes to professional decks for consistent branding."
      },
      {
        "target_user": "Creative teams",
        "title": "Marketing material themes",
        "description": "Choose from Sunset Boulevard or Golden Hour themes for vibrant, engaging creative presentations."
      },
      {
        "target_user": "Content creators",
        "title": "Document visual consistency",
        "description": "Maintain visual identity across reports, slides, and landing pages with matched theme specifications."
      }
    ],
    "prompt_templates": [
      {
        "title": "Apply existing theme",
        "scenario": "Use pre-set theme",
        "prompt": "Apply the [Theme Name] theme to my presentation. Use the color palette and font pairings specified in the theme factory skill."
      },
      {
        "title": "Show theme options",
        "scenario": "Browse available themes",
        "prompt": "Show me all available themes from the theme factory. Display the color palettes and describe the use cases for each."
      },
      {
        "title": "Create custom theme",
        "scenario": "Generate new theme",
        "prompt": "Create a custom theme for [describe your use case]. Generate a cohesive color palette with hex codes and matched font pairings similar to the existing themes."
      },
      {
        "title": "Match brand colors",
        "scenario": "Brand-aligned theme",
        "prompt": "Create a theme that matches these brand colors: [list colors]. Generate a complete theme with complementary accent colors, background colors, and font recommendations."
      }
    ],
