  "content": {
    "user_title": "Parse FCS flow cytometry files",
    "value_statement": "Flow cytometry generates large binary FCS files that require specialized parsing. This skill provides complete guidance for reading FCS files, extracting event data as NumPy arrays, and converting data to CSV or pandas DataFrame formats for downstream analysis.",
    "seo_keywords": [
      "FlowIO",
      "FCS file parser",
      "flow cytometry data",
      "Claude Code skill",
      "NumPy array extraction",
      "scientific data processing",
      "bioinformatics tool",
      "FCS to CSV conversion",
      "FCS 3.1"
    ],
    "actual_capabilities": [
      "Read FCS files v2.0-3.1 and extract metadata",
      "Convert event data to NumPy arrays",
      "Create new FCS files from NumPy data",
      "Export flow cytometry data to CSV or pandas DataFrame",
      "Handle multi-dataset FCS files",
      "Parse channel information and acquisition metadata"
    ],
    "limitations": [
      "Does not perform advanced analysis like compensation or gating",
      "Requires Python 3.9 or later with flowio library installed",
      "Cannot modify FCS files in place - must recreate with create_fcs()",
      "Does not support FlowJo or GatingML file formats"
    ],
    "use_cases": [
      {
        "target_user": "Research Scientists",
        "title": "Inspect flow cytometry files",
        "description": "Quickly examine FCS file structure, channel names, event counts, and acquisition metadata before analysis."
      },
      {
        "target_user": "Data Engineers",
        "title": "Convert FCS to CSV",
        "description": "Transform flow cytometry event data from FCS binary format to CSV or pandas DataFrame for downstream pipelines."
      },
      {
        "target_user": "Bioinformatics Teams",
        "title": "Batch process FCS files",
        "description": "Process directories of FCS files, extract summary statistics, and prepare data for R or Python analysis workflows."
      }
    ],
    "prompt_templates": [
      {
        "title": "Basic FCS Reading",
        "scenario": "Read a flow cytometry file",
        "prompt": "Use FlowIO to read sample.fcs and show me the FCS version, event count, channel names, and acquisition date from the metadata."
      },
      {
        "title": "Extract Event Data",
        "scenario": "Get NumPy array from FCS",
        "prompt": "Extract all event data from experiment.fcs as a NumPy array and calculate the mean and standard deviation for each fluorescence channel."
      },
      {
        "title": "Convert to DataFrame",
        "scenario": "Export flow data",
        "prompt": "Read the FCS file and convert the event data to a pandas DataFrame with proper column names, then save it to output.csv."
      },
      {
        "title": "Batch Processing",
        "scenario": "Process multiple files",
        "prompt": "Process all .fcs files in the data directory. For each file, extract the filename, event count, channel count, and acquisition date. Return a summary table."
      }
    ],
    "output_examples": [
      {
        "input": "Read myflow.fcs and show me the key information",
