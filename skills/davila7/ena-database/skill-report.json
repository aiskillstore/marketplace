  "content": {
    "user_title": "Search ENA nucleotide archive",
    "value_statement": "Finding and downloading nucleotide sequences requires understanding complex API syntax and multiple endpoint patterns. This skill provides comprehensive documentation for querying the European Nucleotide Archive to retrieve DNA/RNA sequences, genome assemblies, and raw sequencing reads by accession.",
    "seo_keywords": [
      "European Nucleotide Archive",
      "ENA database",
      "DNA sequences",
      "RNA sequences",
      "genome assemblies",
      "bioinformatics",
      "FASTA download",
      "FASTQ files",
      "Claude Code",
      "sequence retrieval"
    ],
    "actual_capabilities": [
      "Search nucleotide sequences by accession number",
      "Query metadata for studies, samples, and runs",
      "Download genome assemblies in FASTA format",
      "Retrieve raw sequencing reads (FASTQ) and annotations",
      "Access taxonomic information via ENA Taxonomy API",
      "Find cross-references to related database records"
    ],
    "limitations": [
      "Rate limited to 50 requests per second from ENA APIs",
      "Large file downloads require FTP or Aspera (not direct API)",
      "Does not provide local file caching or pipeline integration",
      "No built-in authentication for restricted datasets"
    ],
    "use_cases": [
      {
        "target_user": "Bioinformatics researcher",
        "title": "Download reference genomes",
        "description": "Retrieve genome assemblies for a specific organism to use as reference in downstream analysis pipelines"
      },
      {
        "target_user": "Genomic scientist",
        "title": "Access raw sequencing data",
        "description": "Find and download FASTQ files from public sequencing projects for comparative genomics studies"
      },
      {
        "target_user": "Computational biologist",
        "title": "Build metadata queries",
        "description": "Search ENA by taxonomy, study accession, or date range to build large-scale dataset collections"
      }
    ],
    "prompt_templates": [
      {
        "title": "Simple accession lookup",
        "scenario": "Get sequence by accession",
        "prompt": "How do I retrieve the FASTA sequence for accession {accession} from ENA?"
      },
      {
        "title": "Study sample search",
        "scenario": "Find samples in study",
        "prompt": "List all samples associated with study {study_id} using the ENA Portal API"
      },
      {
        "title": "Organism assembly query",
        "scenario": "Find genome assemblies",
        "prompt": "Find all genome assemblies available for {organism} with assembly level and contig N50 values"
      },
      {
        "title": "Bulk download setup",
        "scenario": "Prepare batch download",
        "prompt": "What are the FTP download URLs for all FASTQ files in run {run_accession} and what retry logic should I implement?"
      }
    ],
    "output_examples": [
      {
        "input": "Get the FASTA sequence for accession LN847353",
        "output": [
          "Endpoint: GET https://www.ebi.ac.uk/ena/browser/api/fasta/LN847353",
          "Response: FASTA format sequence with header containing accession and description",
          "To download: Add ?download=true to the URL",
          "For subsequences: Add ?range=start-end (e.g., ?range=100-500)"
        ]
      },
      {
        "input": "Find all human sequencing samples from 2023",
        "output": [
          "Use Portal API with query: tax_eq(9606) AND collection_date>=2023-01-01",
          "Set result type to 'sample' and format to 'json'",
          "Limit fields to accession, sample_title, collection_date",
          "Implement 20ms delay between requests to respect rate limit"
        ]
      }
    ],
    "best_practices": [
      "Implement exponential backoff to handle HTTP 429 rate limit responses from ENA APIs",
      "Use the filereport endpoint to get FTP URLs for large FASTQ downloads instead of iterating API calls",
      "Cache taxonomy lookups locally when processing many records to reduce API calls"
    ],
    "anti_patterns": [
      "Making individual API calls for each file in a large batch instead of using FTP bulk downloads",
      "Using XML format when only metadata is needed (prefer JSON or TSV for efficiency)",
      "Ignoring HTTP status codes and failing to handle 404 or 429 responses gracefully"
    ],
    "faq": [
      {
        "question": "Is ENA free to use?",
        "answer": "Yes, ENA is a free public repository. No authentication is required for accessing publicly available data."
      },
      {
        "question": "What formats can I download sequences in?",
        "answer": "Sequences available in FASTA (nucleotide), XML (metadata), and EMBL flat file format. Raw reads in FASTQ."
      },
      {
        "question": "How do I download large datasets efficiently?",
        "answer": "Use the filereport endpoint to get FTP URLs, then download via FTP or Aspera for large files over 100MB."
      },
      {
        "question": "Can I search ENA by organism name?",
        "answer": "Yes, use the taxonomy API with tax_tree(taxon_id) or tax_eq() operators in the Portal API query syntax."
      },
      {
        "question": "What happens if I exceed the rate limit?",
        "answer": "ENA returns HTTP 429. Implement retry logic with exponential backoff (start with 1 second delay)."
      },
      {
        "question": "How is ENA different from NCBI GenBank?",
        "answer": "ENA is the European counterpart that exchanges data with GenBank and SRA. Both contain similar datasets with synchronized updates."
      }
    ]
