  "content": {
    "user_title": "Query Metabolomics Workbench Database",
    "value_statement": "Search thousands of metabolomics studies and metabolite data from the NIH database. Access standardized nomenclature, mass spectrometry data, and study metadata for biomarker discovery and metabolic research.",
    "seo_keywords": [
      "metabolomics database",
      "NIH Metabolomics Workbench",
      "mass spectrometry data",
      "RefMet nomenclature",
      "Claude AI",
      "Claude Code",
      "metabolomics studies",
      "biomarker discovery",
      "metabolite identification",
      "metabolomics research"
    ],
    "actual_capabilities": [
      "Query metabolite structures by PubChem CID, InChI Key, KEGG ID, HMDB ID",
      "Search 4,200+ metabolomics studies by metabolite, institute, or investigator",
      "Standardize metabolite names using RefMet nomenclature database",
      "Perform m/z searches for mass spectrometry compound identification",
      "Filter studies by analytical platform, species, sample source, and disease",
      "Access gene and protein data linked to metabolic pathways"
    ],
    "limitations": [
      "Requires internet connection to access NIH API endpoints",
      "Does not perform offline analysis of local metabolomics data",
      "Rate limits may apply for high-volume automated queries",
      "Limited to publicly available data in Metabolomics Workbench"
    ],
    "use_cases": [
      {
        "target_user": "Computational biologists",
        "title": "Identify metabolites from MS data",
        "description": "Search compound databases by m/z values to identify unknown metabolites from mass spectrometry experiments."
      },
      {
        "target_user": "Clinical researchers",
        "title": "Find disease biomarker studies",
        "description": "Filter metabolomics studies by disease type, sample source, and analytical method for biomarker research."
      },
      {
        "target_user": "Analytical chemists",
        "title": "Standardize compound names",
        "description": "Use RefMet to convert common metabolite names to standardized nomenclature for consistent reporting."
      }
    ],
    "prompt_templates": [
      {
        "title": "Basic metabolite search",
        "scenario": "Find information about a metabolite",
        "prompt": "Find studies in Metabolomics Workbench containing measurements of [metabolite_name]"
      },
      {
        "title": "Compound identification",
        "scenario": "Identify compounds from mass spec data",
        "prompt": "Search for compounds with m/z value of [mz_value] using [adduct_type] adduct and tolerance of [tolerance] Da"
      },
      {
        "title": "Disease study search",
        "scenario": "Find studies for a specific disease",
        "prompt": "Find human blood studies on [disease_name] using LC-MS positive mode in Metabolomics Workbench"
      },
      {
        "title": "Complete workflow",
        "scenario": "Full metabolomics investigation",
        "prompt": "Standardize the metabolite name [metabolite], find all related studies, retrieve experimental data from the most relevant study, and identify compounds from my MS data with m/z of [mz_value]"
      }
    ],
    "output_examples": [
      {
        "input": "Find studies containing measurements of glucose in Metabolomics Workbench",
        "output": [
          "Found 847 studies containing glucose measurements",
          "Top studies: ST000001 (Diabetes study), ST000002 (Metabolic syndrome), ST000003 (Blood glucose regulation)",
          "First study: 156 metabolites measured using LC-MS positive mode",
          "Glucose registry number: 1590 in Metabolomics Workbench"
        ]
      },
      {
        "input": "Search for compounds with m/z value of 180.06 using M+H adduct",
        "output": [
          "Possible compounds: Glucose (m/z match within 0.5 Da)",
          "Fructose (m/z match within 0.5 Da)",
          "Galactose (m/z match within 0.5 Da)",
          "Registry numbers: 1590, 2313, 2088"
        ]
      },
      {
        "input": "Find human blood studies on diabetes using LC-MS",
        "output": [
          "Found 23 studies matching criteria",
          "Recent studies: ST000001 (Diabetes baseline), ST000002 (Metabolic syndrome), ST000045 (Type 2 diabetes progression)",
          "Sample types: Whole blood, Plasma, Serum"
        ]
      }
    ],
    "best_practices": [
      "Always standardize metabolite names through RefMet before searching to ensure consistent nomenclature",
      "Use appropriate ion adduct types for your analytical method (M+H for positive ESI, M-H for negative ESI)",
      "Set reasonable mass tolerance values based on your instrument resolution (0.5 Da for low-res, 0.01 Da for high-res MS)"
    ],
    "anti_patterns": [
      "Using inconsistent metabolite names across queries without standardization",
      "Ignoring study metadata when selecting datasets for analysis",
      "Failing to cross-reference metabolite identifiers across multiple databases"
    ],
    "faq": [
      {
        "question": "Which AI tools support this skill?",
        "answer": "The skill works with Claude, Codex, and Claude Code. Install via npx claude-code-templates@latest --skill metabolomics-workbench-database."
      },
      {
        "question": "How many studies are available?",
        "answer": "Over 4,200 processed studies with 3,790+ publicly available. Access study metadata, experimental data, and metabolite lists."
      },
      {
        "question": "Can I integrate this with analysis pipelines?",
        "answer": "Yes. Use the REST API endpoints with Python requests, the mwtab Python package, or R metabolomicsWorkbenchR package."
      },
      {
        "question": "Is my data safe when using this skill?",
        "answer": "This skill only provides API documentation. No data is stored or transmitted. All requests go directly to NIH servers."
      },
      {
        "question": "Why are my queries returning no results?",
        "answer": "Check spelling of metabolite names, use RefMet standardization, verify study filters are not too restrictive, or try broader search terms."
      },
      {
        "question": "How does this compare to PubChem or HMDB?",
        "answer": "Metabolomics Workbench focuses on experimental metabolomics studies. PubChem and HMDB provide broader compound databases with more structural information."
      }
    ]
