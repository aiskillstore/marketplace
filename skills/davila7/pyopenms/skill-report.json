  "content": {
    "user_title": "Analyze mass spectrometry data with PyOpenMS",
    "value_statement": "Mass spectrometry generates complex data requiring specialized tools for proteomics and metabolomics analysis. PyOpenMS provides Python bindings enabling automated LC-MS/MS processing, peptide identification, and quantitative analysis with full reproducibility.",
    "seo_keywords": [
      "PyOpenMS",
      "mass spectrometry",
      "proteomics",
      "metabolomics",
      "LC-MS/MS",
      "Claude Code",
      "peptide identification",
      "feature detection",
      "mzML",
      "OpenMS"
    ],
    "actual_capabilities": [
      "Read and write mass spectrometry file formats including mzML, mzXML, mzTab, FASTA, and idXML",
      "Apply signal processing algorithms for smoothing, peak picking, normalization, and baseline reduction",
      "Detect features in LC-MS data and link them across samples for quantitative comparison",
      "Perform peptide and protein identification with FDR filtering and protein inference",
      "Execute complete untargeted metabolomics workflows including adduct detection and retention time alignment",
      "Export data to pandas DataFrames for integration with statistical analysis tools"
    ],
    "limitations": [
      "Requires pyopenms package installation with complex binary dependencies",
      "Does not include search engine executables; Comet, Mascot, MSGFPlus must be installed separately",
      "Memory usage scales with data size; very large files require indexed or streaming access patterns",
      "Some TOPP tools are not directly accessible through the Python API"
    ],
    "use_cases": [
      {
        "target_user": "Proteomics researcher",
        "title": "Process LC-MS/MS datasets",
        "description": "Automate proteomics data processing from raw spectra to protein quantification with reproducible Python scripts"
      },
      {
        "target_user": "Metabolomics scientist",
        "title": "Untargeted metabolomics analysis",
        "description": "Detect metabolite features, align retention times across samples, and create quantification tables for statistical analysis"
      },
      {
        "target_user": "Bioinformatics analyst",
        "title": "Integrate MS data with pipelines",
        "description": "Combine PyOpenMS with pandas, scikit-learn, and other Python tools for custom mass spectrometry analysis workflows"
      }
    ],
    "prompt_templates": [
      {
        "title": "Load MS data",
        "scenario": "Read mzML file and explore",
        "prompt": "Show me how to load an mzML file with PyOpenMS and access basic spectrum information like MS level, retention time, and peak count"
      },
      {
        "title": "Process spectra",
        "scenario": "Apply signal processing",
        "prompt": "Write a PyOpenMS script that applies Gaussian smoothing and peak picking to profile mass spectrometry data"
      },
      {
        "title": "Detect features",
        "scenario": "Feature detection workflow",
        "prompt": "How do I detect features in centroided LC-MS data using the FeatureFinder and save results to featureXML format"
      },
      {
        "title": "Filter by FDR",
        "scenario": "Apply FDR filtering",
        "prompt": "Demonstrate how to load peptide identification results from idXML, apply 1% FDR filtering, and export filtered results"
      }
    ],
    "output_examples": [
      {
        "input": "Load my sample.mzML file and show me the key statistics",
        "output": [
          "Number of spectra in file",
          "MS level distribution (MS1 vs MS2 counts)",
          "Retention time range",
          "Total ion current",
          "Number of chromatograms"
        ]
      },
      {
        "input": "Apply Gaussian smoothing to my raw profile data",
        "output": [
          "Gaussian filter configuration",
          "Smoothed spectrum output",
          "Peak detection after smoothing",
          "Parameter values used"
        ]
      },
      {
        "input": "Detect features in my LC-MS experiment and save results",
        "output": [
          "Feature count detected",
          "Feature quality scores",
          "Output file saved to featureXML",
          "Quality metrics summary"
        ]
      }
    ],
    "best_practices": [
      "Use indexed or streaming access for large mzML files to avoid memory issues",
      "Always preserve original data and create copies before processing",
      "Validate file existence and handle exceptions for robust pipelines"
    ],
    "anti_patterns": [
      "Loading entire very large mzML files into memory without using indexed access",
      "Skipping quality control steps like blank filtering in metabolomics workflows",
      "Applying search parameters without testing tolerance values on a data subset first"
    ],
    "faq": [
      {
        "question": "What file formats does PyOpenMS support?",
        "answer": "mzML, mzXML, mzData, idXML, mzIdentML, pepXML, protXML, featureXML, consensusXML, mzTab, FASTA, and TraML formats"
      },
      {
        "question": "Can I use PyOpenMS on Windows?",
        "answer": "Yes, but installation is more complex on Windows. Using conda or WSL is recommended for easier dependency management"
      },
      {
        "question": "Does PyOpenMS include search engines?",
        "answer": "No, PyOpenMS provides adapters but you must install search engines like Comet, Mascot, or MSGFPlus separately"
      },
      {
        "question": "How do I handle very large files?",
        "answer": "Use IndexedMzMLFileLoader for random access or MSExperimentConsumer for streaming processing to reduce memory usage"
      },
      {
        "question": "Is my data safe with PyOpenMS?",
        "answer": "PyOpenMS only processes data locally using the OpenMS library. No data is sent anywhere unless you explicitly code network calls"
      },
      {
        "question": "How does PyOpenMS compare to other tools?",
        "answer": "PyOpenMS offers Python-native access to comprehensive C++ algorithms, combining ease of Python with performance of compiled code"
      }
    ]
