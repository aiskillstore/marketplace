  "content": {
    "user_title": "Automate scientific research workflows",
    "value_statement": "Denario automates the complete research pipeline from data analysis to publication. It uses AI agents to generate hypotheses, develop methodologies, execute experiments, and produce journal-formatted papers.",
    "seo_keywords": [
      "denario",
      "scientific research automation",
      "AI research assistant",
      "multiagent AI",
      "Claude Code",
      "Claude",
      "Codex",
      "research paper generation",
      "LaTeX automation",
      "hypothesis generation"
    ],
    "actual_capabilities": [
      "Generate research hypotheses from datasets using AI agents",
      "Develop structured research methodologies automatically",
      "Execute computational experiments with visualization output",
      "Produce publication-ready LaTeX papers in journal format"
    ],
    "limitations": [
      "Requires API keys for LLM providers (OpenAI, Google Vertex AI)",
      "Python 3.12 or higher required for local execution",
      "LaTeX distribution needed for PDF paper generation",
      "Full pipeline execution depends on LLM API availability and quota"
    ],
    "use_cases": [
      {
        "target_user": "Academic researchers",
        "title": "Automate hypothesis generation",
        "description": "Generate research questions from datasets and produce journal-ready papers with minimal manual effort"
      },
      {
        "target_user": "Data scientists",
        "title": "Streamline analysis workflow",
        "description": "Connect data analysis to methodology development and automated paper writing for reproducible research"
      },
      {
        "target_user": "Graduate students",
        "title": "Learn research best practices",
        "description": "Follow structured research pipelines with generated methodologies and formatting guidance"
      }
    ],
    "prompt_templates": [
      {
        "title": "Quick research start",
        "scenario": "Start a new research project",
        "prompt": "Use denario to analyze my climate dataset and generate a research hypothesis about temperature trends"
      },
      {
        "title": "Methodology development",
        "scenario": "Develop analysis approach",
        "prompt": "With denario, develop a methodology for analyzing the correlation between my variables and create appropriate visualizations"
      },
      {
        "title": "Paper generation",
        "scenario": "Create publication document",
        "prompt": "Use denario to generate a journal-formatted LaTeX paper from my research results with proper citations"
      },
      {
        "title": "Full pipeline",
        "scenario": "End-to-end automation",
        "prompt": "Run the complete denario pipeline: set data description for my medical imaging dataset, generate ideas, develop methodology, execute analysis, and create an APS-formatted paper"
      }
    ],
    "output_examples": [
      {
        "input": "Analyze gene expression data from cancer patients and generate research ideas",
        "output": [
          "Research hypothesis: Identify gene signatures predictive of treatment response using machine learning classification",
          "Generated methodology includes: data preprocessing, PCA for dimensionality reduction, random forest classification, cross-validation strategy, and SHAP feature importance analysis"
        ]
      },
      {
        "input": "Generate a LaTeX paper from existing research results",
        "output": [
          "Complete LaTeX document with title, abstract, introduction, methods, results, and discussion sections",
          "Figures embedded with proper captions, formatted bibliography, ready for Physical Review submission"
        ]
      }
    ],
    "best_practices": [
      "Provide detailed data descriptions including format, size, quality issues, and available analytical tools",
      "Review and refine generated methodologies before executing computational experiments",
      "Use version control to track research evolution through each pipeline stage"
    ],
    "anti_patterns": [
      "Using vague data descriptions that lack sufficient context for meaningful hypothesis generation",
      "Skipping methodology review before running computational experiments",
      "Committing API keys or credentials to version control instead of using environment variables"
    ],
    "faq": [
      {
        "question": "What LLM providers does denario support?",
        "answer": "Denario supports Google Vertex AI, OpenAI, and any provider compatible with AG2 or LangGraph frameworks."
      },
      {
        "question": "Do I need LaTeX installed to use denario?",
        "answer": "LaTeX is required for PDF generation. You can use the Docker image which includes LaTeX, or install TeX Live manually."
      },
      {
        "question": "Can I provide custom research ideas instead of generating them?",
        "answer": "Yes, use set_idea() to specify your own research hypothesis and skip automated idea generation."
      },
      {
        "question": "What Python version is required?",
        "answer": "Denario requires Python 3.12 or higher for local execution."
      },
      {
        "question": "Can I use denario with pre-computed results?",
        "answer": "Yes, use set_results() to provide existing analysis results and skip computational execution."
      },
      {
        "question": "How are API credentials secured?",
        "answer": "Credentials are managed through environment variables or .env files following security best practices."
      }
    ]
