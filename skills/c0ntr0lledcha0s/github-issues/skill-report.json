        "target_user": "Contributors",
        "title": "Feature request handling",
        "description": "Generate implementation plans, research Schema.org alignment, and coordinate dependency tracking"
      },
      {
        "target_user": "Development teams",
        "title": "Workflow coordination",
        "description": "Track issue progress, link PRs to issues, and manage release announcements"
      }
    ],
    "prompt_templates": [
