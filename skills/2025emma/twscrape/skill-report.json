    "user_title": "Scrape Twitter/X data with GraphQL API",
    "value_statement": "Need to extract tweets, user profiles, or trends from Twitter/X? This skill provides complete guidance on using twscrape with account rotation, async operations, and rate limit handling for reliable data collection.",
    "seo_keywords": [
      "twscrape",
      "twitter scraping",
      "x.com scraping",
      "twitter api",
      "social media scraping",
      "data collection",
      "python scraping",
      "graphql api",
      "claude",
      "claude code"
    ],
    "actual_capabilities": [
      "Search tweets by query with GraphQL API",
      "Extract user profiles, followers, and following lists",
      "Collect tweets, replies, and media from any user",
      "Access trending topics by category",
      "Handle multiple accounts with automatic rotation",
      "Export data to JSON or SQLite formats"
    ],
    "limitations": [
      "Requires Twitter/X accounts for API access",
      "Rate limits apply per account (resets every 15 minutes)",
      "Maximum 3,200 tweets per user account (Twitter platform limit)",
      "Account suspension risk with aggressive scraping"
    ],
    "use_cases": [
      {
        "target_user": "Data Researchers",
        "title": "Build social media datasets",
        "description": "Collect tweet datasets for academic research, sentiment analysis, or trend detection projects."
      },
      {
        "target_user": "Content Creators",
        "title": "Monitor brand mentions",
        "description": "Track conversations about topics, brands, or competitors across Twitter/X in real time."
      },
      {
        "target_user": "Developers",
        "title": "Integrate Twitter data",
        "description": "Add Twitter/X data collection to applications, dashboards, or automation workflows."
      }
    ],
    "prompt_templates": [
      {
        "title": "Basic tweet search",
        "scenario": "Search for specific tweets",
        "prompt": "Show me how to search for tweets matching \"artificial intelligence\" and collect the latest 50 results using twscrape."
      },
      {
        "title": "User analysis",
        "scenario": "Extract user profile data",
        "prompt": "How do I get user profile information, follower count, and recent tweets for a Twitter account like \"elonmusk\"?"
      },
      {
        "title": "Account setup",
        "scenario": "Configure multiple accounts",
        "prompt": "Explain how to add multiple Twitter accounts to twscrape with cookies for stable authentication and automatic rotation."
      },
      {
        "title": "Batch collection",
        "scenario": "Large scale data gathering",
        "prompt": "Create a complete workflow using twscrape to collect tweets from 100 users, handle rate limits, and export everything to a JSON file with error handling."
      }
    ],
    "output_examples": [
      {
        "input": "Search for recent Python programming tweets",
        "output": [
          "Found 20 tweets from 15 users:",
          "- @pydanny: Just released Django 5.0! #python #django",
          "- @realpython: Introduction to async/await in Python 3.11",
          "- @flask: Flask 3.0 adds native async support",
          "Total likes: 2,450 | Retweets: 312"
        ]
      },
      {
        "input": "Get user profile data for a specific account",
        "output": [
          "User: OpenAI",
          "Followers: 15.2M",
          "Following: 1,234",
          "Recent tweets: 50 collected",
          "Account verified: Yes"
        ]
      }
    ],
    "best_practices": [
      "Use cookie-based authentication for more stable account access",
      "Add multiple accounts to enable rotation and avoid rate limits",
      "Implement proper error handling and retry logic for production use"
    ],
    "anti_patterns": [
      "Using single account for high-volume scraping without rotation",
      "Ignoring rate limits and continuing requests after hitting limits",
      "Storing account credentials in plaintext without encryption"
    ],
    "faq": [
      {
        "question": "Is twscrape compatible with Twitter/X API v2?",
        "answer": "Twscrape uses GraphQL endpoints, not the official API. It scrapes data directly from Twitter/X web interface."
      },
