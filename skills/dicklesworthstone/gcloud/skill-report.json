  "content": {
    "user_title": "Manage Google Cloud Platform resources",
    "value_statement": "Managing Google Cloud Platform resources requires remembering many gcloud CLI commands and flags. This skill provides comprehensive documentation and examples for all major GCP services. Use it to quickly find and execute commands for compute instances, Cloud Run, GKE, storage buckets, BigQuery, and more.",
    "seo_keywords": [
      "gcloud CLI",
      "Google Cloud Platform",
      "GCP management",
      "Claude Code",
      "Claude Code gcloud",
      "Cloud Run",
      "GKE",
      "BigQuery",
      "Cloud Storage",
      "Compute Engine"
    ],
    "actual_capabilities": [
      "Authenticate with Google Cloud using user accounts or service accounts",
      "List, create, update, and delete Compute Engine virtual machines",
      "Deploy and manage Cloud Run services and Cloud Functions",
      "Create and manage GKE clusters with kubectl integration",
      "Work with Cloud Storage buckets and object operations",
      "Query datasets and manage tables in BigQuery"
    ],
    "limitations": [
      "Requires gcloud CLI to be installed on the system",
      "User must have appropriate GCP permissions for operations",
      "Does not provide interactive prompts for missing parameters",
      "Commands must be run in a terminal with gcloud access"
    ],
    "use_cases": [
      {
        "target_user": "DevOps engineers",
        "title": "Deploy cloud infrastructure",
        "description": "Create and manage virtual machines, Kubernetes clusters, and serverless containers on GCP."
      },
      {
        "target_user": "Data analysts",
        "title": "Query BigQuery datasets",
        "description": "Run SQL queries against BigQuery datasets and manage data loading from Cloud Storage."
      },
      {
        "target_user": "Cloud architects",
        "title": "Configure cloud resources",
        "description": "Set up IAM roles, service accounts, Secret Manager secrets, and networking components."
      }
    ],
    "prompt_templates": [
      {
        "title": "List VM instances",
        "scenario": "View running compute instances",
        "prompt": "List all Compute Engine instances in my GCP project using gcloud CLI."
      },
      {
        "title": "Deploy Cloud Run",
        "scenario": "Deploy container to serverless",
        "prompt": "Deploy a container image to Cloud Run with public access enabled using gcloud CLI."
      },
      {
        "title": "Create GKE cluster",
        "scenario": "Set up Kubernetes cluster",
        "prompt": "Create a new GKE cluster with 3 nodes in us-central1-a zone using gcloud CLI."
      },
      {
        "title": "Query BigQuery",
        "scenario": "Run analytics query",
        "prompt": "Run a query to select 10 rows from my dataset.table in BigQuery using gcloud CLI."
      }
    ],
    "output_examples": [
      {
        "input": "List all Compute Engine instances in my GCP project using gcloud CLI",
        "output": [
          "NAME: my-vm, ZONE: us-central1-a, STATUS: RUNNING",
          "NAME: test-vm, ZONE: us-central1-b, STATUS: STOPPED",
          "Use 'gcloud compute instances describe NAME --zone=ZONE' for details"
        ]
      },
      {
        "input": "Deploy a container to Cloud Run with gcloud",
        "output": [
          "Service my-service deployed successfully",
          "URL: https://my-service-abc123-uc.a.run.app",
          "Use 'gcloud run services list' to verify"
        ]
      },
      {
        "input": "Create a storage bucket on GCP",
        "output": [
          "Bucket gs://my-bucket created",
          "Location: us-central1",
          "Use 'gcloud storage ls' to list objects"
        ]
      }
    ],
    "best_practices": [
      "Always specify region and zone explicitly to avoid configuration confusion",
      "Use --quiet flag for automated scripts to prevent confirmation prompts",
      "Format output as JSON or table for easier parsing in scripts"
    ],
    "anti_patterns": [
      "Running destructive commands without --quiet in automation",
      "Using default project without verifying the correct project is set",
      "Hardcoding credentials in scripts instead of using application default credentials"
    ],
    "faq": [
      {
        "question": "Is gcloud CLI required to use this skill?",
        "answer": "Yes, the gcloud CLI must be installed and configured on your system. Install from the official Google Cloud documentation."
      },
      {
        "question": "What GCP services are supported?",
        "answer": "Compute Engine, Cloud Run, GKE, Cloud Functions, Cloud Storage, BigQuery, Cloud SQL, Pub/Sub, Secret Manager, IAM, and more."
      },
      {
        "question": "How do I authenticate?",
        "answer": "Use 'gcloud auth login' for user accounts or 'gcloud auth activate-service-account' with a key file for service accounts."
      },
      {
        "question": "Is my data safe when using these commands?",
        "answer": "Commands run locally using your credentials. No data is sent to third parties. Ensure proper IAM permissions are configured."
      },
      {
        "question": "Why did my command fail?",
        "answer": "Common issues include: missing gcloud installation, incorrect project, insufficient permissions, or invalid resource names. Check each requirement."
      },
      {
        "question": "How is this different from Google Cloud Console?",
        "answer": "The gcloud CLI provides scriptable, repeatable operations. It is ideal for automation, CI/CD pipelines, and reproducible infrastructure."
      }
    ]
