  "content": {
    "user_title": "Manage Supabase projects and databases",
    "value_statement": "This skill provides documented commands for the Supabase CLI. You can manage Supabase projects, databases, migrations, Edge Functions, and storage directly from your AI assistant without switching context.",
    "seo_keywords": [
      "Supabase",
      "Claude",
      "Codex",
      "Claude Code",
      "database management",
      "PostgreSQL",
      "Edge Functions",
      "database migrations",
      "backend development",
      "supabase CLI"
    ],
    "actual_capabilities": [
      "List and link Supabase projects",
      "Start and stop local Supabase development stack",
      "Execute SQL queries and manage database migrations",
      "Deploy and manage Edge Functions",
      "Manage storage buckets and file operations",
      "Generate TypeScript types from database schema"
    ],
    "limitations": [
      "Requires Supabase CLI to be installed on your system",
      "Requires authentication with Supabase account for remote operations",
      "Local development requires Docker to run Supabase services",
      "Does not provide direct database connections - uses CLI only"
    ],
    "use_cases": [
      {
        "target_user": "Backend developers",
        "title": "Manage database schema",
        "description": "Create and apply migrations, sync local and remote schemas, and generate TypeScript types automatically"
      },
      {
        "target_user": "Full-stack developers",
        "title": "Develop Edge Functions",
        "description": "Create, deploy, and monitor serverless Edge Functions with local testing support"
      },
      {
        "target_user": "DevOps engineers",
        "title": "Control local environments",
        "description": "Start, stop, and reset local Supabase stacks for consistent development environments"
      }
    ],
    "prompt_templates": [
      {
        "title": "Start local stack",
        "scenario": "Initialize local Supabase",
        "prompt": "Start a local Supabase development environment with supabase start"
      },
      {
        "title": "List projects",
        "scenario": "View Supabase projects",
        "prompt": "List all my Supabase projects using supabase projects list"
      },
      {
        "title": "Create migration",
        "scenario": "Database schema changes",
        "prompt": "Create a new migration called add_user_profile_table using supabase migration new"
      },
      {
        "title": "Deploy Edge Function",
        "scenario": "Deploy serverless function",
        "prompt": "Deploy my Edge Function called process_data to production using supabase functions deploy"
      }
    ],
    "output_examples": [
      {
