  "content": {
    "user_title": "Automate Chrome Browser with Claude",
    "value_statement": "Manual browser tasks consume too much time. This skill enables Claude to control your Chrome browser, automating repetitive form filling, data extraction, and workflow tasks using your existing login sessions.",
    "seo_keywords": [
      "Claude Chrome automation",
      "browser automation",
      "Chrome extension Claude",
      "form filling automation",
      "web data extraction",
      "workflow automation",
      "Claude Code browser control",
      "MCP chrome-devtools",
      "browser testing automation",
      "Codex browser"
    ],
    "actual_capabilities": [
      "Navigate to URLs and manage multiple browser tabs",
      "Click buttons, type text, and fill out web forms automatically",
      "Extract structured data from web pages and save as CSV files",
      "Access authenticated pages in Gmail, Google Docs, Notion, and CRMs",
      "Monitor console logs and network requests for debugging",
      "Record browser workflows as GIFs and run them on schedules"
    ],
    "limitations": [
      "Requires Google Chrome specifically. Other Chromium browsers are not supported.",
      "Requires a paid Claude plan such as Pro, Team, Enterprise, or Max.",
      "Cannot operate in headless mode with the extension. DevTools MCP supports headless.",
      "Does not work with WSL or mobile devices."
    ],
    "use_cases": [
      {
        "target_user": "QA Engineers",
        "title": "Automated Form and UI Testing",
        "description": "Test web forms with invalid data and verify error messages appear correctly. Check console for errors during page loads."
      },
      {
        "target_user": "Data Analysts",
        "title": "Extract Data from Authenticated Pages",
        "description": "Pull product listings, prices, and availability from web pages you are already logged into. Save results as CSV."
      },
      {
        "target_user": "Business Users",
        "title": "Automate Repetitive Data Entry",
        "description": "Fill CRM contact records or application forms automatically from CSV files. Schedule recurring tasks like inbox cleanup."
      }
    ],
    "prompt_templates": [
      {
        "title": "Basic Navigation",
        "scenario": "Visit a website and click an element",
        "prompt": "Go to github.com/anthropics and click on the Code tab"
      },
      {
        "title": "Form Testing",
        "scenario": "Test form validation behavior",
        "prompt": "Open localhost:3000, try submitting the login form with invalid data, and check if error messages appear correctly"
      },
      {
        "title": "Data Extraction",
        "scenario": "Extract structured data from a page",
        "prompt": "Go to the product listings page and extract the name, price, and availability for each item. Save as CSV."
      },
      {
        "title": "Authenticated Workflow",
        "scenario": "Work with logged-in services",
        "prompt": "Open my Google Analytics, get this week's traffic summary, and save it to a file"
      }
    ],
    "output_examples": [
      {
        "input": "Go to github.com/anthropics and click on the Code tab",
        "output": [
          "Navigated to github.com/anthropics",
          "Found the Code tab element on the page",
          "Clicked the Code tab successfully",
          "Page now shows the repository listing"
        ]
      },
      {
        "input": "Open Gmail and archive emails from newsletters",
        "output": [
          "Opened Gmail in a new tab",
          "Found 12 emails matching newsletter patterns",
          "Selected and archived all 12 emails",
          "Inbox cleanup complete"
        ]
      }
    ],
    "best_practices": [
      "Use fresh tabs when pages become unresponsive to avoid getting stuck on errors",
      "Be specific about elements and actions to produce consistent automation results",
      "Add verification steps for long tasks to confirm all items were completed correctly"
    ],
    "anti_patterns": [
      "Using browser automation for simple one-click tasks that are faster to do manually",
      "Leaving CAPTCHAs or modal dialogs unhandled during automation runs",
      "Running automation on dynamic pages without waiting for elements to fully load"
    ],
    "faq": [
      {
        "question": "Which browsers are supported?",
        "answer": "Only Google Chrome is supported. Brave, Arc, Edge, and other Chromium browsers do not work with this skill."
      },
      {
        "question": "Do I need a paid Claude plan?",
        "answer": "Yes. This skill requires a Pro, Team, Enterprise, or Max Claude plan to function."
      },
      {
        "question": "Can I use this with other MCP servers?",
        "answer": "Yes. Chrome DevTools MCP can run alongside other MCP servers in your Claude Code configuration."
      },
      {
        "question": "Is my browsing data safe?",
        "answer": "Yes. The extension uses Chrome Native Messaging API locally. Site permissions control access. High-risk actions require confirmation."
      },
      {
        "question": "Why is my browser not responding?",
        "answer": "Check for blocking modal dialogs such as alerts or prompts. Dismiss them manually or restart the Chrome extension."
      },
      {
        "question": "How is this different from Puppeteer or Playwright?",
        "answer": "This uses your real Chrome browser with existing login sessions. No re-authentication needed. More convenient but less isolated."
      }
    ]
