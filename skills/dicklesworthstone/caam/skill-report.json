  "content": {
    "user_title": "Switch AI accounts instantly",
    "value_statement": "Managing multiple AI coding subscriptions is frustrating when you hit rate limits. CAAM switches accounts in sub-100ms by backing up and restoring OAuth tokens locally. No browser OAuth friction required.",
    "seo_keywords": [
      "CAAM",
      "Claude Code account switcher",
      "AI CLI account manager",
      "Claude Max",
      "GPT Pro",
      "OAuth token management",
      "rate limit handling",
      "Codex CLI",
      "Gemini CLI",
      "account switching"
    ],
    "actual_capabilities": [
      "Back up and restore OAuth authentication files for Claude Code, Codex, and Gemini CLI",
      "Switch between accounts in under 100 milliseconds",
      "Smart rotation with health scoring and cooldown tracking",
      "Isolated profiles for running multiple accounts simultaneously",
      "Interactive TUI dashboard for visual profile management",
      "Automatic failover when rate limits are hit"
    ],
    "limitations": [
      "Does not work with API key authentication",
      "OAuth tokens may contain machine-specific identifiers that prevent syncing across machines",
      "Rate limits still apply per account; this only speeds up switching"
    ],
    "use_cases": [
      {
        "target_user": "Developers with multiple AI subscriptions",
        "title": "Avoid rate limit interruptions",
        "description": "Hit a rate limit mid-flow and need to continue immediately without browser OAuth friction"
      },
      {
        "target_user": "Teams sharing accounts",
        "title": "Manage work and personal accounts",
        "description": "Switch between work and personal AI coding subscriptions instantly"
      },
      {
        "target_user": "Power users",
        "title": "Run parallel AI sessions",
        "description": "Run multiple AI accounts simultaneously in different terminals with full directory isolation"
      }
    ],
    "prompt_templates": [
      {
        "title": "Check active accounts",
        "scenario": "See which account is currently active",
        "prompt": "caam status"
      },
      {
        "title": "Switch accounts",
        "scenario": "Switch to a different account instantly",
        "prompt": "caam activate claude bob@gmail.com"
      },
      {
        "title": "Auto-select best account",
        "scenario": "Let the system pick the healthiest account",
        "prompt": "caam activate claude --auto"
      },
