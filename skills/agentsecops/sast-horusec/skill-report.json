  "content": {
    "user_title": "Scan code for security vulnerabilities",
    "value_statement": "Manual security reviews miss common vulnerabilities. This skill provides Horusec-based SAST workflows to automatically scan code across 18+ languages, detect secrets in git history, and integrate security scanning into CI/CD pipelines for continuous protection.",
    "seo_keywords": [
      "sast scanner",
      "horusec",
      "static analysis",
      "secrets detection",
      "code security",
      "vulnerability scanning",
      "ci cd security",
      "secure sdlc",
      "Claude",
      "Claude Code"
    ],
    "actual_capabilities": [
      "Run static application security tests across 18+ programming languages",
      "Detect exposed secrets and credentials in git history",
      "Generate vulnerability reports with severity classification",
      "Integrate security scanning into CI/CD pipelines",
      "Configure false positive suppression and risk acceptance workflows",
      "Map findings to OWASP Top 10 and CWE classifications"
    ],
    "limitations": [
      "Does not execute scans directly - requires Horusec CLI installation",
      "Scanner runs outside the skill context after installation",
      "No automatic remediation of found vulnerabilities",
      "Requires Docker or manual tool installation for full functionality"
    ],
    "use_cases": [
      {
        "target_user": "Security Engineers",
        "title": "Configure CI/CD security scanning",
        "description": "Set up automated security scanning in GitHub Actions, GitLab CI, or Jenkins pipelines with severity thresholds and artifact reporting."
      },
      {
        "target_user": "Developers",
        "title": "Scan local code for vulnerabilities",
        "description": "Run pre-commit security scans on local projects to identify and fix vulnerabilities before committing code."
      },
      {
        "target_user": "Auditors",
        "title": "Detect secrets in git history",
        "description": "Scan entire git history for exposed credentials, API keys, and tokens that may have been accidentally committed."
      }
    ],
    "prompt_templates": [
      {
        "title": "Quick security scan",
        "scenario": "Run a basic Horusec scan on the current project",
        "prompt": "Help me run Horusec to scan this codebase for security vulnerabilities. Show me the commands to start a scan and explain how to interpret the severity levels in the output."
      },
      {
        "title": "Configure CI pipeline",
        "scenario": "Add Horusec to GitHub Actions workflow",
        "prompt": "Help me add Horusec security scanning to a GitHub Actions workflow. I need to configure it to fail builds on high-severity findings and upload results as artifacts."
      },
      {
        "title": "Scan for secrets",
        "scenario": "Check git history for exposed credentials",
        "prompt": "Show me how to use Horusec to scan git history for secrets and exposed credentials. Include commands to enable git history analysis and handle any found secrets."
      },
      {
