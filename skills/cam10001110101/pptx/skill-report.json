  "content": {
    "user_title": "Create and edit PowerPoint presentations",
    "value_statement": "Create, edit, and analyze PowerPoint files with comprehensive tools for template-based design, HTML-to-PPTX conversion, and visual thumbnail preview generation.",
    "seo_keywords": [
      "powerpoint",
      "pptx",
      "presentation",
      "slides",
      "claude",
      "codex",
      "claude-code",
      "office documents",
      "powerpoint automation",
      "presentation creation"
    ],
    "actual_capabilities": [
      "Create new presentations from HTML using the html2pptx library",
      "Edit existing presentations by manipulating OOXML structure",
      "Generate visual thumbnail grids for slide analysis",
      "Apply template designs by rearranging and replacing content",
      "Extract text inventory with formatting preserved",
      "Convert presentations to PDF and images for visual review"
    ],
    "limitations": [
      "Cannot edit embedded media or complex animations directly",
      "Requires LibreOffice and Poppler utilities installed for conversions",
      "Template-based creation requires analyzing existing template structure first"
    ],
    "use_cases": [
      {
        "target_user": "Business professionals",
        "title": "Create presentations from templates",
        "description": "Duplicate and customize professional templates by rearranging slides and replacing placeholder content with your own text."
      },
      {
        "target_user": "Content creators",
        "title": "Build presentations from HTML",
        "description": "Convert HTML slides with CSS styling to PowerPoint format using the html2pptx workflow for pixel-perfect design control."
      },
      {
        "target_user": "Presenters",
        "title": "Edit existing presentations",
        "description": "Modify presentation content by unpacking the OOXML structure, editing XML directly, and repacking the file."
      }
    ],
    "prompt_templates": [
      {
        "title": "Create new presentation",
        "scenario": "Start a presentation from scratch",
        "prompt": "Create a new PowerPoint presentation with 5 slides about [TOPIC]. Use the html2pptx workflow with professional styling."
      },
      {
        "title": "Use template",
        "scenario": "Apply a design template",
        "prompt": "Create a presentation using [TEMPLATE_FILE.pptx] as a template. Rearrange slides to show [SLIDE_ORDER]. Replace text with [NEW_CONTENT]."
      },
      {
        "title": "Extract content",
        "scenario": "Analyze existing presentation",
        "prompt": "Extract the text inventory from [PRESENTATION.pptx] and save it to a JSON file. Read the full content to understand the structure."
      },
      {
        "title": "Visual validation",
        "scenario": "Review slide layout",
        "prompt": "Generate a thumbnail grid for [PRESENTATION.pptx] and check for any text cutoff, overlap, or positioning issues across all slides."
      }
    ],
    "output_examples": [
      {
        "input": "Create a 3-slide presentation about climate change using the html2pptx workflow",
        "output": [
          "Created slide1.html with title slide styling and theme colors",
          "Created slide2.html with two-column layout for climate impacts",
          "Generated presentation.pptx with all 3 slides converted",
          "Thumbnail grid shows slides render correctly"
        ]
      },
      {
        "input": "Use template.pptx to create a new presentation with slides 0, 2, 5, 5 in that order",
        "output": [
          "Extracted template inventory with 20 shapes across 10 slides",
          "Generated working.pptx with slides 0, 2, 5, 5",
          "Created replacement-text.json with all placeholder content cleared",
          "Applied new content and saved to output.pptx"
        ]
      }
    ],
    "best_practices": [
      "Always validate presentations after editing OOXML structure using the validate.py script",
      "Generate thumbnail grids to visually verify slide layouts before finalizing",
      "Read full documentation files before using html2pptx or ooxml editing workflows"
    ],
    "anti_patterns": [
      "Avoid skipping the validation step after editing XML files",
      "Do not use html2pptx for simple presentations that would be faster with template duplication",
      "Do not forget to check thumbnail grids for layout issues before finalizing"
    ],
    "faq": [
      {
        "question": "Can I edit presentations without Microsoft PowerPoint?",
        "answer": "Yes, this skill manipulates the OOXML format directly. You can unpack, edit XML, and repack presentations on any system."
      },
      {
        "question": "What dependencies are required?",
        "answer": "LibreOffice (soffice), Poppler (pdftoppm), markitdown, pptxgenjs, and playwright are required for full functionality."
      },
      {
        "question": "How do I fix layout issues?",
        "answer": "Generate thumbnail grids to identify issues, then adjust HTML margins and spacing for html2pptx or modify XML directly."
      },
      {
        "question": "Can I create charts and tables?",
        "answer": "Yes, use PptxGenJS API within html2pptx for charts. Tables can be created by replacing placeholder shapes with formatted text."
      },
      {
        "question": "How do I preserve template styling?",
        "answer": "Analyze the template inventory first, then use the same placeholder positions and paragraph formatting properties in replacements."
      },
      {
        "question": "What file operations are safe?",
        "answer": "All documented operations (unpack, validate, pack, inventory, replace, thumbnail) are designed for safe document manipulation."
      }
    ]
  },
