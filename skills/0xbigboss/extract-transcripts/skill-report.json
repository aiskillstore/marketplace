  "content": {
    "user_title": "Convert session logs to readable transcripts",
    "value_statement": "Claude Code and Codex CLI store conversation history as JSONL files that are difficult to read manually. This skill extracts those sessions into clean markdown format with metadata, timestamps, and optional tool call details.",
    "seo_keywords": [
      "extract transcripts",
      "claude code session export",
      "codex cli history",
      "claude transcript converter",
      "ai session logs",
      "claude conversation export",
      "codex transcript tool",
      "claude code jsonl to markdown",
      "session transcript extractor"
    ],
    "actual_capabilities": [
      "Extract Claude Code session JSONL files to readable markdown",
      "Extract Codex CLI session rollout files to readable format",
      "Index and search across multiple sessions using DuckDB",
      "Filter sessions by date, project, or keyword",
      "Include or exclude tool calls and thinking blocks from output",
      "Batch process all sessions in a directory"
    ],
    "limitations": [
      "Requires Python 3 with optional DuckDB dependency for indexing",
      "Does not modify or delete original session files",
      "Only reads from local session paths (no remote fetch)",
      "Does not extract token usage or cost data"
    ],
    "use_cases": [
      {
        "target_user": "Developer",
        "title": "Session Review",
        "description": "Review past AI assistance sessions to recall decisions, context, or code patterns"
      },
      {
        "target_user": "Documentation Writer",
        "title": "Generate Guides",
        "description": "Convert session logs into tutorial content or process documentation"
      },
      {
        "target_user": "Team Lead",
        "title": "Audit Usage",
        "description": "Search across team sessions to identify patterns or verify implementation approaches"
      }
    ],
