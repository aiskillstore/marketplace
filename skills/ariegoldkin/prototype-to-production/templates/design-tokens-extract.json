{
  "$schema": "https://design-tokens.github.io/community-group/format/",
  "_comment": "Design tokens extracted from prototype. Replace placeholder values with actual extracted values.",

  "color": {
    "primitive": {
      "_description": "Raw color values from prototype CSS/styles",
      "gray": {
        "50": { "$value": "#f9fafb" },
        "100": { "$value": "#f3f4f6" },
        "200": { "$value": "#e5e7eb" },
        "300": { "$value": "#d1d5db" },
        "400": { "$value": "#9ca3af" },
        "500": { "$value": "#6b7280" },
        "600": { "$value": "#4b5563" },
        "700": { "$value": "#374151" },
        "800": { "$value": "#1f2937" },
        "900": { "$value": "#111827" }
      },
      "primary": {
        "_extract_from": "primary buttons, links, active states",
        "50": { "$value": "#EXTRACT" },
        "500": { "$value": "#EXTRACT" },
        "600": { "$value": "#EXTRACT" },
        "700": { "$value": "#EXTRACT" }
      }
    },
    "semantic": {
      "_description": "Contextual color assignments",
      "brand": {
        "primary": { "$value": "{color.primitive.primary.600}" },
        "primary-hover": { "$value": "{color.primitive.primary.700}" }
      },
      "text": {
        "primary": { "$value": "{color.primitive.gray.900}" },
        "secondary": { "$value": "{color.primitive.gray.600}" },
        "muted": { "$value": "{color.primitive.gray.400}" }
      },
      "background": {
        "primary": { "$value": "#ffffff" },
        "secondary": { "$value": "{color.primitive.gray.50}" },
        "tertiary": { "$value": "{color.primitive.gray.100}" }
      },
      "border": {
        "default": { "$value": "{color.primitive.gray.200}" },
        "strong": { "$value": "{color.primitive.gray.300}" }
      },
      "feedback": {
        "success": { "$value": "#10b981" },
        "warning": { "$value": "#f59e0b" },
        "error": { "$value": "#ef4444" },
        "info": { "$value": "#3b82f6" }
      }
    }
  },

  "typography": {
    "_description": "Extract from body, headings, and text elements",
    "fontFamily": {
      "sans": { "$value": "'Inter', system-ui, -apple-system, sans-serif" },
      "mono": { "$value": "'Fira Code', 'JetBrains Mono', monospace" }
    },
    "fontSize": {
      "xs": { "$value": "0.75rem" },
      "sm": { "$value": "0.875rem" },
      "base": { "$value": "1rem" },
      "lg": { "$value": "1.125rem" },
      "xl": { "$value": "1.25rem" },
      "2xl": { "$value": "1.5rem" },
      "3xl": { "$value": "1.875rem" },
      "4xl": { "$value": "2.25rem" }
    },
    "fontWeight": {
      "normal": { "$value": "400" },
      "medium": { "$value": "500" },
      "semibold": { "$value": "600" },
      "bold": { "$value": "700" }
    },
    "lineHeight": {
      "tight": { "$value": "1.25" },
      "normal": { "$value": "1.5" },
      "relaxed": { "$value": "1.75" }
    }
  },

  "spacing": {
    "_description": "Derive from padding/margin patterns in prototype",
    "0": { "$value": "0" },
    "1": { "$value": "0.25rem" },
    "2": { "$value": "0.5rem" },
    "3": { "$value": "0.75rem" },
    "4": { "$value": "1rem" },
    "5": { "$value": "1.25rem" },
    "6": { "$value": "1.5rem" },
    "8": { "$value": "2rem" },
    "10": { "$value": "2.5rem" },
    "12": { "$value": "3rem" },
    "16": { "$value": "4rem" }
  },

  "borderRadius": {
    "_description": "Extract from rounded elements",
    "none": { "$value": "0" },
    "sm": { "$value": "0.125rem" },
    "base": { "$value": "0.25rem" },
    "md": { "$value": "0.375rem" },
    "lg": { "$value": "0.5rem" },
    "xl": { "$value": "0.75rem" },
    "2xl": { "$value": "1rem" },
    "full": { "$value": "9999px" }
  },

  "shadow": {
    "_description": "Extract from box-shadow CSS properties",
    "sm": { "$value": "0 1px 2px 0 rgba(0, 0, 0, 0.05)" },
    "base": { "$value": "0 1px 3px 0 rgba(0, 0, 0, 0.1), 0 1px 2px -1px rgba(0, 0, 0, 0.1)" },
    "md": { "$value": "0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -2px rgba(0, 0, 0, 0.1)" },
    "lg": { "$value": "0 10px 15px -3px rgba(0, 0, 0, 0.1), 0 4px 6px -4px rgba(0, 0, 0, 0.1)" },
    "xl": { "$value": "0 20px 25px -5px rgba(0, 0, 0, 0.1), 0 8px 10px -6px rgba(0, 0, 0, 0.1)" }
  },

  "animation": {
    "_description": "Extract transition/animation values from prototype",
    "duration": {
      "fast": { "$value": "150ms" },
      "normal": { "$value": "200ms" },
      "slow": { "$value": "300ms" }
    },
    "easing": {
      "default": { "$value": "cubic-bezier(0.4, 0, 0.2, 1)" },
      "in": { "$value": "cubic-bezier(0.4, 0, 1, 1)" },
      "out": { "$value": "cubic-bezier(0, 0, 0.2, 1)" }
    }
  }
}
