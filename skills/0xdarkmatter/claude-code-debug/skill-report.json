    "user_title": "Debug Claude Code Issues",
    "value_statement": "Claude Code extensions, hooks, and agents sometimes stop working. This skill provides step-by-step troubleshooting guides and diagnostic commands to identify and fix common problems quickly. Use it when skills fail to load, hooks do not execute, or agents are not being used.",
    "seo_keywords": [
      "Claude Code debugging",
      "Claude Code troubleshooting",
      "Claude Code hooks",
      "Claude Code skills",
      "Claude Code agents",
      "MCP server debugging",
      "Claude CLI debugging",
      "Claude Code extensions",
      "Claude Code configuration",
      "Claude Code problems"
    ],
    "actual_capabilities": [
      "Diagnose skill activation and loading issues",
      "Troubleshoot hook execution failures",
      "Identify agent delegation problems",
      "Debug MCP server connection issues",
      "Validate configuration files and permissions",
      "Provide decision tree troubleshooting flows"
    ],
    "limitations": [
      "Cannot execute commands directly on user system",
      "Cannot modify configuration files automatically",
      "Does not install or update Claude Code",
      "Limited to documented troubleshooting scenarios"
    ],
    "use_cases": [
      {
        "target_user": "New Claude Code users",
        "title": "Skill Not Loading",
        "description": "When a newly installed skill does not activate despite correct file structure and YAML frontmatter"
      },
      {
        "target_user": "Hook developers",
        "title": "Hook Not Executing",
        "description": "When hooks configured in settings.json fail to run or do not block tools as expected"
      },
      {
        "target_user": "Power users",
        "title": "MCP Server Issues",
        "description": "When Model Context Protocol servers fail to connect or tools are not available"
      }
    ],
    "prompt_templates": [
      {
        "title": "Basic Debug Request",
        "scenario": "Skill not working",
        "prompt": "My Claude Code skill is not activating. How do I troubleshoot this?"
      },
      {
        "title": "Hook Problem",
        "scenario": "Hook not running",
        "prompt": "My hook script is not executing when tools are used. What diagnostic steps should I follow?"
      },
      {
        "title": "Agent Issue",
        "scenario": "Agent not used",
        "prompt": "Claude Code is not using my custom agent. How do I verify it is configured correctly?"
      },
      {
        "title": "MCP Debug",
        "scenario": "MCP connection failed",
        "prompt": "My MCP server is not connecting. Walk me through the debugging flow to identify the issue."
      }
    ],
    "output_examples": [
      {
        "input": "My skill is not activating",
        "output": [
          "Check if the skill directory exists at .claude/skills/skill-name/",
          "Verify SKILL.md exists with exact capitalization",
          "Confirm YAML frontmatter starts and ends with ---",
          "Ensure name: field matches directory name exactly",
          "Add trigger keywords: Triggers on: keyword1, keyword2",
          "Run: claude --debug and look for skill loading errors"
        ]
      },
      {
        "input": "My hook is not running",
        "output": [
          "Check if script is executable: chmod +x script.sh",
          "Validate settings.json with: jq '.' ~/.claude/settings.json",
          "Verify matcher is case-sensitive: use Bash not bash",
          "Test script manually: echo '{\"tool_name\":\"Test\"}' | ./hook.sh",
          "Run /hooks to see registered hooks"
        ]
      }
    ],
    "best_practices": [
      "Run 'claude --debug' to see detailed loading and permission information",
      "Validate all JSON files with 'jq .' before saving",
      "Test hook scripts manually before relying on them"
    ],
    "anti_patterns": [
      "Skipping the YAML frontmatter validation step",
      "Using relative paths in hook configurations without $CLAUDE_PROJECT_DIR",
      "Assuming a skill activates without adding specific trigger keywords"
    ],
