  "content": {
    "user_title": "Decompile Android APKs for security analysis",
    "value_statement": "Android apps hide their code in DEX bytecode. This skill provides complete guidance for using jadx to convert APK files into readable Java source code. Analyze app logic, find vulnerabilities, and extract hardcoded credentials with expert techniques.",
    "seo_keywords": [
      "jadx",
      "android decompiler",
      "apk decompile",
      "dex to java",
      "android reverse engineering",
      "security analysis",
      "vulnerability research",
      "claude code",
      "codex",
      "claude"
    ],
    "actual_capabilities": [
      "Convert APK DEX bytecode to readable Java source code",
      "Apply deobfuscation to restore meaningful class and method names",
      "Search decompiled code for hardcoded credentials and API keys",
      "Identify security vulnerabilities like SQL injection and weak crypto",
      "Extract API endpoints and authentication mechanisms from apps",
      "Export decompiled code as Gradle projects for further analysis"
    ],
    "limitations": [
      "Requires jadx to be installed on your system",
      "Cannot analyze heavily obfuscated or protected apps",
      "Decompiled code is approximate, not exact original source",
      "Java Runtime Environment (JRE) is required"
    ],
    "use_cases": [
      {
        "target_user": "Security researchers",
        "title": "Find vulnerabilities in apps",
        "description": "Search for insecure crypto, SQL injection points, and hardcoded secrets in Android applications."
      },
      {
        "target_user": "IoT security analysts",
        "title": "Analyze companion app APIs",
        "description": "Extract API endpoints and authentication logic from IoT device companion applications."
      },
      {
        "target_user": "App developers",
        "title": "Understand competitor apps",
        "description": "Study how other apps implement features and handle security in your codebase."
      }
    ],
    "prompt_templates": [
      {
        "title": "Basic decompile",
        "scenario": "Decompile an APK file",
        "prompt": "Decompile [app-name].apk using jadx with deobfuscation to [output-directory]"
      },
      {
        "title": "Search secrets",
        "scenario": "Find hardcoded credentials",
        "prompt": "Search the decompiled code for API keys, passwords, and hardcoded credentials. Report all findings."
      },
      {
        "title": "Vulnerability scan",
        "scenario": "Find security issues",
        "prompt": "Analyze the decompiled code for common vulnerabilities including: weak encryption (MD5, DES), SQL injection risks, insecure storage (MODE_WORLD_READABLE), and WebView issues."
      },
      {
        "title": "API discovery",
        "scenario": "Extract API endpoints",
        "prompt": "Extract all API endpoints, base URLs, and authentication mechanisms from the decompiled Android app. List each endpoint with its HTTP method."
      }
    ],
    "output_examples": [
