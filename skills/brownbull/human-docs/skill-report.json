  "content": {
    "user_title": "Transform AI documentation into human-friendly guides",
    "value_statement": "Complex AI documentation is difficult for humans to process quickly. This skill extracts essential information and creates three accessible formats: executive summaries, step-by-step quick starts, and visual Mermaid diagrams that make technical concepts easy to understand.",
    "seo_keywords": [
      "Claude documentation skill",
      "Mermaid diagram generator",
      "AI to human documentation",
      "documentation transformation",
      "technical summary generator",
      "quick start guide creator",
      "visual documentation",
      "markdown documentation tool",
      "Claude Code",
      "Codex"
    ],
    "actual_capabilities": [
      "Creates executive summaries from detailed AI documentation",
      "Generates step-by-step quick start guides with code examples",
      "Produces Mermaid.js visual diagrams including flowcharts and sequence diagrams",
      "Maintains parallel folder structure between ai/ and docs/ folders",
      "Supports batch processing of entire documentation folders",
      "Updates existing human docs when source AI documentation changes"
    ],
    "limitations": [
      "Only transforms existing documentation, does not create new documentation from scratch",
      "Does not execute code or perform technical validation",
      "Limited to markdown-based documentation formats",
      "Does not support real-time collaboration or multi-user workflows"
    ],
    "use_cases": [
      {
        "target_user": "New Team Members",
        "title": "Onboard developers quickly",
        "description": "Generate summaries and diagrams from technical specs so new team members understand architecture without reading thousands of lines of documentation."
      },
      {
        "target_user": "Project Managers",
        "title": "Create executive reports",
        "description": "Transform detailed technical documentation into one-page executive summaries with visual diagrams that communicate system architecture to stakeholders."
      },
      {
        "target_user": "Technical Writers",
        "title": "Build visual documentation",
        "description": "Extract and enhance existing documentation with Mermaid diagrams, flowcharts, and architecture visuals for improved readability."
      }
    ],
