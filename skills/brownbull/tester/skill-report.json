  "content": {
    "user_title": "Test React and Django Applications",
    "value_statement": "Ensure code quality through systematic testing. This skill designs test strategies, creates Playwright E2E tests for React frontends and pytest tests for Django backends, executes test suites, and generates detailed reports.",
    "seo_keywords": [
      "testing skill",
      "Playwright testing",
      "pytest framework",
      "E2E testing",
      "unit testing",
      "integration testing",
      "Claude Code testing",
      "software testing",
      "test automation",
      "quality assurance"
    ],
    "actual_capabilities": [
      "Design test strategies including UAT, integration, smoke, and unit tests",
      "Create E2E tests for React frontends using Playwright",
      "Create backend tests for Django APIs using pytest",
      "Execute test suites and collect results automatically",
      "Generate detailed test reports with findings and recommendations"
    ],
    "limitations": [
      "Designed specifically for React and Django applications",
      "Requires Playwright and pytest to be installed in the project",
      "Test execution requires project-specific setup and configuration",
      "Does not perform security testing or vulnerability scanning"
    ],
    "use_cases": [
      {
        "target_user": "Full-stack developers",
        "title": "Create comprehensive test suites",
        "description": "Design and implement tests for new features across frontend and backend components"
      },
      {
        "target_user": "QA engineers",
        "title": "Run smoke tests before deployment",
        "description": "Execute critical path tests to validate application stability before release"
      },
      {
        "target_user": "Product managers",
        "title": "Design UAT test scenarios",
        "description": "Create user acceptance test strategies for business validation of new features"
      }
    ],
    "prompt_templates": [
      {
        "title": "Create unit test",
        "scenario": "Write a test for a new function",
        "prompt": "Create a unit test for the validate_rut_field function in the gabeda_backend codebase using pytest with the AAA pattern"
      },
      {
        "title": "Create E2E test",
        "scenario": "Write an end-to-end test",
        "prompt": "Create a Playwright E2E test that verifies user registration flow including form validation and successful account creation"
      },
      {
        "title": "Run test suite",
        "scenario": "Execute existing tests",
        "prompt": "Run the backend pytest test suite and generate a coverage report showing areas that need additional test coverage"
      },
      {
        "title": "Design test strategy",
        "scenario": "Plan testing approach",
        "prompt": "Design a comprehensive test strategy for the file upload feature including unit, integration, and E2E test cases"
      }
    ],
    "output_examples": [
      {
        "input": "Create a Playwright test that logs in a test user and verifies the dashboard loads correctly",
        "output": [
          "Test file created at gabeda_frontend/tests/e2e/login-dashboard.spec.ts",
          "Test uses authenticated fixture with test credentials",
          "Verifies dashboard h1 title and navigation elements",
          "Reports generated at ai/testing/reports/"
        ]
      },
      {
        "input": "Run the smoke test suite for both frontend and backend",
        "output": [
          "Frontend smoke tests: 5 passed, 0 failed",
          "Backend smoke tests: 8 passed, 0 failed",
          "Total execution time: 2 minutes 34 seconds",
          "Deployment readiness: READY"
        ]
      },
      {
        "input": "Design UAT test cases for the company creation feature",
        "output": [
          "Strategy document created at ai/testing/strategies/uat_company_creation.md",
          "10 test scenarios covering valid input, edge cases, and error conditions",
          "Acceptance criteria defined for each scenario",
          "Test data samples provided for QA execution"
        ]
      }
    ],
    "best_practices": [
      "Use the AAA pattern (Arrange-Act-Assert) for all test cases to ensure clarity and maintainability",
      "Keep tests independent and isolated - no shared state between test runs",
      "Mock external services and APIs in unit tests to ensure fast, deterministic execution"
    ],
    "anti_patterns": [
      "Avoid brittle tests that depend on exact CSS selectors - use test IDs and role-based locators instead",
      "Do not test multiple assertions in a single test - split into separate test cases for clearer failures",
      "Avoid hardcoding test credentials in multiple test files - use fixtures and environment variables"
    ],
    "faq": [
      {
        "question": "What test frameworks does this skill support?",
        "answer": "The skill supports Playwright for frontend E2E and component testing, and pytest for backend Django API and unit testing."
      },
      {
        "question": "Can this skill run tests in parallel?",
        "answer": "Yes, the skill can configure parallel test execution for independent tests to reduce overall test suite runtime."
      },
      {
        "question": "Does this skill generate code coverage reports?",
        "answer": "Yes, the skill can run pytest with coverage flags and generate HTML coverage reports showing untested code paths."
      },
      {
        "question": "Where are test reports stored?",
        "answer": "Reports are stored in ai/testing/reports/ with timestamps. Strategies go to ai/testing/strategies/ and findings to ai/testing/findings/."
      },
      {
        "question": "Can this skill create tests for existing features?",
        "answer": "Yes, the skill can analyze existing code and create appropriate tests following project conventions and patterns."
      },
      {
        "question": "What test naming conventions does this skill use?",
        "answer": "Tests follow the pattern test_<feature>_<scenario>_<expected_result> for backend and Playwright's '<feature> - <scenario> - <expected result>' format."
      }
    ]
