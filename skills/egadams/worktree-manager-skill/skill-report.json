  "content": {
    "user_title": "Manage Git Worktrees for Parallel Development",
    "value_statement": "Managing multiple development branches simultaneously is complex and error-prone. This skill provides slash commands to safely create, list, and remove git worktrees with automatic port allocation and service management.",
    "seo_keywords": [
      "git worktree",
      "Claude Code skill",
      "parallel development",
      "multiple branches",
      "isolated environments",
      "git",
      "development workflow",
      "branch management",
      "Claude",
      "Codex"
    ],
    "actual_capabilities": [
      "Create git worktrees with automatic port allocation",
      "List all worktrees with status and service information",
      "Remove worktrees with automatic cleanup of services",
      "Handle port conflicts by calculating unique offsets",
      "Manage background processes for server and client",
      "Set up isolated configuration for each worktree"
    ],
    "limitations": [
      "Only works with git repositories that have worktree support enabled",
      "Requires existing branches to create worktrees from",
      "Cannot manage worktrees outside the trees directory convention",
      "Depends on Claude Code slash commands being available"
    ],
    "use_cases": [
      {
        "target_user": "Feature Developers",
        "title": "Parallel Feature Work",
        "description": "Work on multiple features simultaneously without stashing or switching branches"
      },
      {
        "target_user": "Code Reviewers",
        "title": "Review PRs in Isolation",
        "description": "Test pull requests in isolated environments before merging"
      },
      {
        "target_user": "Release Engineers",
        "title": "Hotfix While Developing",
        "description": "Apply production fixes while continuing feature development"
      }
    ],
    "prompt_templates": [
      {
        "title": "Create Worktree",
        "scenario": "Start new feature work",
        "prompt": "Create a worktree for the [branch-name] branch"
      },
      {
        "title": "List Worktrees",
        "scenario": "Check existing environments",
        "prompt": "Show me my current worktrees and their status"
      },
      {
        "title": "Remove Worktree",
        "scenario": "Clean up completed work",
        "prompt": "Remove the [branch-name] worktree, I am done with it"
      },
      {
        "title": "Parallel Setup",
        "scenario": "Multiple features at once",
        "prompt": "Create worktrees for feature-a, feature-b, and feature-c so I can work on all three"
      }
    ],
    "output_examples": [
      {
        "input": "Create a worktree for the feature-authentication branch",
        "output": [
          "Created worktree for feature-authentication:",
          "- Server: http://localhost:4010",
