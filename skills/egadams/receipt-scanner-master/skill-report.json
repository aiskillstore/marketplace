  "content": {
    "user_title": "Scan receipts with AI OCR",
    "value_statement": "Receipt scanning requires accurate OCR extraction and database integration. This skill provides complete guidance for parsing receipt images, debugging extraction errors, and building reliable expense tracking workflows with Google Gemini AI.",
    "seo_keywords": [
      "receipt scanner",
      "OCR extraction",
      "Claude Code",
      "Claude",
      "Codex",
      "expense tracking",
      "Google Gemini",
      "receipt parsing",
      "image to data",
      "financial records"
    ],
    "actual_capabilities": [
      "Parse receipt images (JPG, PNG, WebP, PDF) into structured data",
      "Debug OCR accuracy issues and extraction errors",
      "Enhance receipt parsing engine and AI prompts",
      "Troubleshoot database integration issues",
      "Test receipt uploads through web interface",
      "Validate extracted data against actual receipts"
    ],
    "limitations": [
      "Requires Gemini API key for OCR processing",
      "Needs local API server running for full functionality",
      "Only processes image and PDF receipt files",
      "Categorized items must be saved manually after parsing"
    ],
    "use_cases": [
      {
        "target_user": "Small business owners",
        "title": "Track business expenses",
        "description": "Scan and categorize receipts from business purchases for tax and accounting purposes."
      },
      {
        "target_user": "Developers",
        "title": "Build receipt integration",
        "description": "Integrate OCR receipt parsing into expense tracking applications with database storage."
      },
      {
        "target_user": "Bookkeepers",
        "title": "Automate data entry",
        "description": "Convert paper receipts to digital expense records with automated categorization."
      }
    ],
    "prompt_templates": [
      {
        "title": "Parse a receipt",
        "scenario": "Extract data from image",
        "prompt": "Use the Receipt Scanner Master skill to parse the receipt image at [file_path]. Extract all line items, prices, merchant name, and totals. Report the total amount spent."
      },
      {
        "title": "Debug OCR errors",
        "scenario": "Fix price misreading",
        "prompt": "The receipt parser keeps reading prices incorrectly. Use the skill to enhance the Gemini prompt with digit recognition rules for 4 vs 9 confusion."
      },
      {
        "title": "Integrate with database",
        "scenario": "Save parsed receipts",
        "prompt": "Show me how to modify the receipt endpoints to validate parsed data before saving to the expenses database. Include math validation for line totals."
      },
      {
        "title": "Build complete workflow",
        "scenario": "Create end-to-end system",
        "prompt": "Create a complete receipt scanning workflow. Include image preprocessing, Gemini API integration, data validation, database storage, and error handling."
      }
    ],
    "output_examples": [
