    "user_title": "Create TikZ Diagrams in Obsidian",
    "value_statement": "Generate precise technical drawings in Obsidian that exceed Mermaid limitations. Create geometric shapes, coordinate systems, circuit diagrams, chemical structures, and 3D plots using LaTeX TikZ syntax rendered by the TikZJax plugin.",
    "seo_keywords": [
      "tikzjax",
      "obsidian diagrams",
      "latex diagrams",
      "tikz diagram",
      "obsidian plugin",
      "circuit diagrams",
      "technical drawing",
      "claude code",
      "obsidian",
      "latex visualization"
    ],
    "actual_capabilities": [
      "Generate TikZ code blocks for geometric shapes and coordinate systems",
      "Create circuit diagrams with circuitikz package syntax",
      "Draw chemical structures using chemfig notation",
      "Build commutative diagrams with tikz-cd syntax",
      "Render 3D plots and data visualizations with pgfplots",
      "Use dark mode color adaptation for theme compatibility"
    ],
    "limitations": [
      "Requires TikZJax Obsidian plugin to be installed",
      "Color mixing syntax like blue!30 is not supported",
      "Korean text and special characters are not supported",
      "Custom RGB/HTML colors are not inverted in dark mode"
    ],
    "use_cases": [
      {
        "target_user": "Technical Writers",
        "title": "Technical Documentation",
        "description": "Create precise diagrams for technical documentation that require exact positioning and mathematical accuracy"
      },
      {
        "target_user": "Educators",
        "title": "Educational Materials",
        "description": "Build circuit diagrams, mathematical visualizations, and chemistry structures for teaching materials"
      },
      {
        "target_user": "Researchers",
        "title": "Research Figures",
        "description": "Generate publication-quality diagrams including coordinate systems, 3D plots, and data visualizations"
      }
    ],
    "prompt_templates": [
      {
        "title": "Basic Rectangle",
        "scenario": "Create a simple TikZ diagram",
        "prompt": "Create a TikZ diagram with a simple rectangle containing text using tikzjax-diagramming syntax"
      },
      {
        "title": "Coordinate System",
        "scenario": "Add axes and points",
        "prompt": "Generate TikZ code for a 2D coordinate system with labeled axes and a marked point using tikzjax-diagramming"
      },
      {
        "title": "Circuit Diagram",
        "scenario": "Draw electronic circuit",
        "prompt": "Create a circuit diagram with a resistor, capacitor, and voltage source using circuitikz syntax from tikzjax-diagramming"
      },
      {
        "title": "Commutative Diagram",
        "scenario": "Build mathematical diagram",
        "prompt": "Generate a commutative diagram with four nodes and labeled arrows using tikz-cd from tikzjax-diagramming"
      }
    ],
    "output_examples": [
      {
        "input": "Create a simple TikZ rectangle diagram",
        "output": [
          "Use a tikz code block with \\begin{document} and \\end{document} wrapper",
          "Place drawing commands between \\begin{tikzpicture} and \\end{tikzpicture}",
          "Add text labels using \\node at (x,y) {text}; syntax",
          "For dark mode compatibility, omit explicit color specifications"
        ]
      },
      {
        "input": "Generate a circuit with resistor and capacitor",
        "output": [
          "Load circuitikz package with \\usepackage{circuitikz}",
          "Draw components using to[R=$R$] and to[C=$C$] syntax",
          "Connect components with to[short] and to[battery1]",
          "Close the circuit loop for proper rendering"
        ]
      }
    ],
    "best_practices": [
      "Always include \\begin{document} and \\end{document} in your tikz code blocks",
      "Use scale=1 for optimal text readability in diagrams",
      "Omit explicit color in \\node commands to enable automatic dark mode adaptation",
      "Load additional packages like circuitikz or chemfig with \\usepackage{} before \\begin{document}"
    ],
    "anti_patterns": [
      "Using color mixing syntax like blue!30 which is not supported",
      "Including Korean or non-ASCII text which breaks rendering",
      "Setting scale less than 1 which reduces text readability",
      "Forgetting the \\begin{document} wrapper which causes rendering failure"
    ],
    "faq": [
      {
        "question": "Does TikZJax work with Obsidian mobile apps?",
        "answer": "TikZJax diagrams render on mobile but complex diagrams may have performance limitations."
      },
      {
        "question": "What is the maximum diagram complexity supported?",
        "answer": "Complexity limits depend on device resources. Simple to moderate diagrams render reliably."
      },
      {
        "question": "How do I install TikZJax in Obsidian?",
        "answer": "Install via Obsidian Community Plugins browser and enable the plugin in settings."
      },
      {
        "question": "Is my diagram data stored externally?",
        "answer": "No. TikZJax renders diagrams locally using browser JavaScript. No data leaves your device."
      },
      {
        "question": "Why is my diagram not rendering?",
        "answer": "Common causes: missing \\begin{document}, syntax errors, unsupported color mixing, or missing package declarations."
      },
      {
        "question": "How does TikZJax compare to Mermaid?",
        "answer": "TikZJax offers superior precision for geometry and mathematics. Mermaid is better for flowcharts and simple diagrams."
      }
    ]
