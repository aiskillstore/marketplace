  "content": {
    "user_title": "Extract Structured Data with Cohere v2",
    "value_statement": "Building data extraction pipelines with LLMs requires consistent, validated JSON outputs. This skill provides expert guidance on using Cohere v2 JSON Schema mode for reliable entity extraction, classification, and structured data pipelines.",
    "seo_keywords": [
      "cohere",
      "entity extraction",
      "json schema",
      "structured outputs",
      "llm data extraction",
      "ner system",
      "claude code",
      "claude",
      "codex",
      "python"
    ],
    "actual_capabilities": [
      "Initialize Cohere v2 Chat Client with Python SDK",
      "Configure JSON Schema mode for validated structured outputs",
      "Extract entities (people, organizations, locations, dates) from text",
      "Build classification systems using enum constraints",
      "Design nested schemas with $ref for reusable structures",
      "Create production pipelines for batch document processing"
    ],
    "limitations": [
      "RAG not supported in JSON Schema mode",
      "Maximum 200 fields in tools mode",
      "Schema mode adds initial latency overhead",
      "Numeric ranges and array length constraints not supported"
    ],
    "use_cases": [
      {
        "target_user": "Data Engineers",
        "title": "Build Extraction Pipelines",
        "description": "Create automated document processing pipelines that extract structured data from unstructured text sources."
      },
      {
        "target_user": "ML Engineers",
        "title": "Implement NER Systems",
        "description": "Deploy named entity recognition systems with guaranteed JSON output formats for downstream applications."
      },
      {
        "target_user": "Application Developers",
        "title": "Integrate LLM Data Extraction",
        "description": "Add reliable data extraction capabilities to applications with validated schema enforcement."
      }
    ],
    "prompt_templates": [
      {
        "title": "Basic Entity Extraction",
