  "content": {
    "user_title": "Apply SMC Trading Methodology to BTC/USDT Analysis",
    "value_statement": "This skill provides a complete ICT/SMC trading framework for autonomous agents operating in backtesting simulations. It enables systematic market analysis, trade decision-making, and risk management following institutional trading principles.",
    "seo_keywords": [
      "SMC trading",
      "ICT methodology",
      "Claude Code skill",
      "BTC/USDT trading",
      "smart money concepts",
      "liquidity analysis",
      "order blocks",
      "fair value gaps",
      "trading automation",
      "backtesting harness"
    ],
    "actual_capabilities": [
      "Analyze multi-timeframe market structure (4H bias, 15m entry signals)",
      "Identify liquidity pools, order blocks, and fair value gaps",
      "Evaluate trade setups with structured decision framework (TRADE/WATCH/NOTE)",
      "Calculate position sizes with 2% risk management rules",
      "Record trade reasoning and market observations for audit",
      "Set price alarms for monitoring key levels during simulation"
    ],
    "limitations": [
      "Operates only in Alpha Harness backtesting environment",
      "Limited to BTC/USDT symbol analysis",
      "Uses only closed candles (no real-time data)",
      "Requires setup creation before placing orders"
    ],
    "use_cases": [
      {
        "target_user": "Trading system developers",
        "title": "Backtesting framework",
        "description": "Deploy autonomous trading agents that follow consistent methodology in simulated markets"
      },
      {
        "target_user": "SMC educators",
        "title": "Methodology documentation",
        "description": "Reference comprehensive ICT/SMC concepts and terminology for training purposes"
      },
      {
        "target_user": "Strategy testers",
        "title": "Trade simulation",
        "description": "Test systematic trading decisions against historical BTC/USDT price data"
      }
    ],
    "prompt_templates": [
      {
        "title": "Analyze market",
        "scenario": "Get current structure",
        "prompt": "analyze BTC/USDT"
      },
      {
        "title": "Check state",
        "scenario": "View current position",
        "prompt": "my-state"
      },
      {
        "title": "Record setup",
        "scenario": "Document identified pattern",
        "prompt": "create-setup --symbol BTC/USDT --type choch-fvg --decision TRADE --confidence HIGH --reasoning [your analysis]"
      },
      {
        "title": "Place trade",
        "scenario": "Execute trade with setup",
        "prompt": "place-order --setup-id [id] --side long --entry [price] --stop [price] --target [price] --size [amount]"
      }
    ],
    "output_examples": [
      {
        "input": "analyze BTC/USDT",
        "output": [
          "4H Bias: Bullish (HH at 98500, HL at 95200)",
          "Price Zone: Discount (below equilibrium at 96850)",
          "Key Liquidity: SSL at 95500 (swept), BSL at 98500 (unswept)",
          "15m Structure: ChoCH confirmed at 95700",
          "Entry Zone: Bullish FVG 95550-95700 (unfilled)",
          "Recommendation: WATCH for pullback entry"
        ]
      },
      {
        "input": "my-state",
        "output": [
          "Active Orders: 0",
          "Balance: $10,000.00",
          "P&L: $0.00",
          "Alarms: price_below 94000, price_above 98000",
          "Recent Setups: 2 (1 WATCH, 1 TRADE completed)"
        ]
      }
    ],
    "best_practices": [
      "Always start analysis with my-state to check active orders first",
      "Require 2:1 minimum risk-to-reward before placing any trade",
      "Wait for liquidity sweep and ChoCH confirmation before entering"
    ],
    "anti_patterns": [
      "Trading against HTF bias without strong confirmation",
      "Entering without waiting for liquidity sweep",
      "Placing orders without creating a setup with documented reasoning"
    ],
    "faq": [
      {
        "question": "What timeframes does this skill use?",
        "answer": "4H for bias determination and 15m for entry timing."
      },
      {
        "question": "What is the maximum risk per trade?",
        "answer": "The harness enforces a maximum 2% risk per trade based on account balance."
      },
      {
        "question": "Can I trade other symbols besides BTC/USDT?",
        "answer": "No, this skill is designed exclusively for BTC/USDT trading."
      },
      {
        "question": "Does this skill execute real trades?",
        "answer": "No, it operates within Alpha Harness backtesting simulation only."
      },
      {
        "question": "Why is only closed candle data used?",
        "answer": "Closed candles ensure no future information leak during backtesting accuracy."
      },
      {
        "question": "How does this compare to discretionary trading?",
        "answer": "It provides systematic rules for consistent decision-making across sessions."
      }
    ]
