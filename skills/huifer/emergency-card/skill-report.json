  "content": {
    "user_title": "Generate Emergency Medical Information Cards",
    "value_statement": "First responders need quick access to critical health data during emergencies. This skill extracts allergies, medications, implants, and medical conditions from health records to generate printable emergency cards in multiple formats including JSON, text, HTML, and QR code.",
    "seo_keywords": [
      "emergency medical card",
      "health information card",
      "allergy card",
      "Claude skill",
      "medical emergency",
      "first responder",
      "health summary",
      "Clude Code",
      "Claude Code",
      "medical card generator"
    ],
    "actual_capabilities": [
      "Extract critical allergies with severity levels from health records",
      "List current active medications with dosage and frequency details",
      "Identify medical implants that affect treatment procedures",
      "Generate cards in JSON, text, HTML, and QR code formats",
      "Prioritize information by medical emergency severity levels",
      "Create specialized cards for children, elderly, and severe allergy patients"
    ],
    "limitations": [
      "Requires pre-existing health data files in the data directory",
      "Does not provide medical advice or treatment recommendations",
      "Does not verify or validate medical information accuracy",
      "PDF generation requires additional tools not included in this skill"
    ],
    "use_cases": [
      {
        "target_user": "Frequent travelers",
        "title": "Travel Medical Safety",
        "description": "Generate portable medical cards for international or domestic travel emergencies"
      },
      {
        "target_user": "Parents of children",
        "title": "School Health Documentation",
        "description": "Create pediatric emergency cards with school contacts and allergy information"
      },
      {
        "target_user": "Elderly and caregivers",
        "title": "Senior Emergency Ready",
        "description": "Generate large-print cards for elderly individuals with multiple medications"
      }
    ],
    "prompt_templates": [
      {
        "title": "Basic Emergency Card",
        "scenario": "Generate a standard emergency medical card",
        "prompt": "Generate my emergency medical information card with all available health data"
      },
      {
        "title": "Travel Preparation",
        "scenario": "Prepare medical info for an upcoming trip",
        "prompt": "Create an emergency card for my upcoming trip to [destination]. Include all allergies and current medications"
      },
      {
        "title": "Child Safety Card",
        "scenario": "Generate pediatric emergency information",
        "prompt": "Generate a pediatric emergency card for my child. Include school contacts and all known allergies"
      },
      {
        "title": "Severe Allergy Card",
        "scenario": "Create detailed allergy warning card",
        "prompt": "Create a detailed severe allergy emergency card with cross-reactivity warnings and first aid instructions"
      }
    ],
    "output_examples": [
      {
        "input": "Generate my emergency medical card",
        "output": [
          "Emergency Medical Card generated successfully",
          "Patient: Zhang San, Male, Age 35",
          "Blood Type: A+",
          "Critical Allergies: Penicillin (Anaphylaxis, Level 4)",
          "Current Medications: Aspirin 100mg once daily, Amlodipine 5mg twice daily",
          "Medical Conditions: Hypertension (controlled), Type 2 Diabetes (monitored)",
          "Implants: Pacillator (2022-06-10)",
          "Emergency Contact: Li Si (Spouse) - 138****1234"
        ]
      },
      {
        "input": "Create an emergency card for my child with school contacts",
        "output": [
          "Pediatric Emergency Card generated",
          "Student: Wang Xiaoming, Age 8, Grade 2",
          "Blood Type: O+",
          "Allergies: Eggs (Level 2), Milk (Level 1)",
          "Current Medication: Montelukast 5mg nightly for asthma prevention",
          "Medical Condition: Bronchial Asthma (under control)",
          "School Contacts: Teacher Zhang (138****1234), School Nurse (010-****1234)",
          "School Instructions: Avoid eggs and dairy, stop exercise if breathing difficulty"
        ]
      }
    ],
    "best_practices": [
      "Update emergency cards every 3 months or after any health information changes",
      "Store digital versions in cloud storage and keep printed copies in wallet or phone case",
      "Always include emergency contact information with partially masked phone numbers for privacy"
    ],
    "anti_patterns": [
      "Using the skill without first having health data files properly organized",
      "Relying on emergency cards as the sole source of medical information during emergencies",
      "Including unverified or self-diagnosed conditions that have not been confirmed by healthcare providers"
    ],
    "faq": [
      {
        "question": "What data sources does this skill use?",
        "answer": "It reads profile.json, allergies.json, medications.json, radiation records, surgery records, and discharge summaries from the data directory."
      },
      {
        "question": "What formats can the card be generated in?",
        "answer": "Supported formats include JSON (structured data), Text (readable with Unicode borders), HTML (printable with styling), and QR code (for phone scanning)."
      },
      {
        "question": "Is my health data sent to external servers?",
        "answer": "No. All processing happens locally through Claude. Data is only saved to your local device when you choose to export it."
      },
      {
        "question": "How often should I update my emergency card?",
        "answer": "Update your card whenever health information changes, at minimum every 3 months, and before any travel or medical procedures."
      },
      {
        "question": "Can this skill replace my medical records?",
        "answer": "No. Emergency cards are summary documents for first responders. Always maintain complete medical records with healthcare providers."
      },
      {
        "question": "What makes this different from medical alert bracelets?",
        "answer": "This card provides more complete information including all medications, multiple allergies, implants, and emergency contacts in formats that are easy to update and share digitally."
      }
    ]
