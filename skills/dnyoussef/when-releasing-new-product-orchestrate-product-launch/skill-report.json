    "user_title": "Orchestrate Complete Product Launches with AI Swarms",
    "value_statement": "Coordinating product launches across multiple teams is complex and error-prone. This skill orchestrates 15+ specialized AI agents through a proven 10-week workflow covering research, development, marketing, launch, and optimization phases.",
    "seo_keywords": [
      "product launch",
      "AI orchestration",
      "Claude Code",
      "multi-agent coordination",
      "go-to-market",
      "product development",
      "marketing campaigns",
      "launch strategy",
      "Claude",
      "Codex"
    ],
    "actual_capabilities": [
      "Coordinates 15+ specialized agents across 5 workflow phases",
      "Manages sequential and parallel execution patterns",
      "Stores phase deliverables in hierarchical memory structure",
      "Provides pre-built scripts for workflow initialization",
      "Includes comprehensive success metrics and KPIs",
      "Supports adaptive optimization in post-launch phase"
    ],
    "limitations": [
      "Requires 10-week timeline commitment for full workflow",
      "Needs access to Claude Flow swarm coordination tools",
      "Memory system required for cross-phase data persistence",
      "Complexity may be overkill for simple product launches"
    ],
    "use_cases": [
      {
        "target_user": "Product managers launching new SaaS products",
        "title": "SaaS Product Launch Coordination",
        "description": "Orchestrate development, marketing, and sales teams for a comprehensive SaaS launch with proper sequencing and dependencies."
      },
      {
        "target_user": "Startup founders with limited team resources",
        "title": "Resource-Efficient Launch Management",
        "description": "Leverage AI agents to fill expertise gaps across development, marketing, and sales functions for lean teams."
      },
      {
        "target_user": "Enterprise product teams",
        "title": "Multi-Stakeholder Launch Alignment",
        "description": "Coordinate complex launches across engineering, marketing, sales, support, and executive stakeholders with clear accountability."
      }
    ],
    "prompt_templates": [
      {
        "title": "Initialize New Product Launch",
        "scenario": "Starting a new product launch workflow",
        "prompt": "I need to launch a new product called [PRODUCT_NAME]. Please initialize the complete 10-week product launch workflow with all 15 agents coordinated across the 5 phases from research to post-launch optimization."
      },
      {
        "title": "Phase 2 Development Kickoff",
        "scenario": "Beginning product development phase",
        "prompt": "Based on the market research and product strategy from Phase 1, spawn the 6 development agents (backend, frontend, mobile, database, security, QA) to build the product in parallel over 6 weeks."
      },
      {
        "title": "Launch Day Execution",
        "scenario": "Executing the final launch phase",
        "prompt": "We are ready to launch. Please execute Phase 4 with production validation, deployment, marketing blitz, and real-time monitoring setup for launch week."
      },
      {
        "title": "Post-Launch Optimization",
        "scenario": "Analyzing and optimizing after launch",
        "prompt": "Generate the weekly performance report for week [N] post-launch and identify optimization opportunities for conversion rates, support efficiency, and marketing ROI."
      }
    ],
    "output_examples": [
      {
