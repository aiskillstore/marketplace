  "content": {
    "user_title": "Analyze ambiguous user intent",
    "value_statement": "Unclear requests waste time and create frustration. This skill uses cognitive science and probabilistic reasoning to understand what users truly mean, ask clarifying questions, and create actionable plans with confidence scores.",
    "seo_keywords": [
      "intent analyzer",
      "user intent analysis",
      "clarify ambiguous requests",
      "cognitive science",
      "Claude Code",
      "Claude",
      "Codex",
      "disambiguation",
      "intent mapping"
    ],
    "actual_capabilities": [
      "Analyzes user requests using probabilistic intent mapping with confidence scores",
      "Breaks complex requests into components using first principles decomposition",
      "Generates clarifying questions to resolve ambiguity through Socratic questioning",
      "Creates actionable execution briefs with phased plans and agent assignments",
      "Adapts questioning complexity based on detected user expertise level"
    ],
    "limitations": [
      "Requires user participation for clarifying questions",
      "Does not access external systems or APIs",
      "Cannot execute code or modify files",
      "Confidence threshold may require multiple clarification rounds"
    ],
    "use_cases": [
      {
        "target_user": "Product managers",
        "title": "Clarify requirements",
        "description": "Transform vague stakeholder requests into precise technical specifications"
      },
      {
        "target_user": "Developers",
        "title": "Prevent miscommunication",
        "description": "Ensure accurate understanding before starting complex feature development"
      },
      {
        "target_user": "AI agents",
        "title": "Improve task accuracy",
        "description": "Reduce errors by verifying understanding through probabilistic reasoning"
      }
    ],
    "prompt_templates": [
