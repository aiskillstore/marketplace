    "user_title": "Review code with AI swarm agents",
    "value_statement": "Code reviews require manual effort and may miss security issues or performance problems. This skill coordinates multiple AI agents to review pull requests across security, performance, style, and architecture dimensions simultaneously.",
    "seo_keywords": [
      "github code review",
      "ai code review",
      "claude code review",
      "automated pr review",
      "security analysis",
      "performance review",
      "claude code",
      "codex",
      "pr automation",
      "code quality"
    ],
    "actual_capabilities": [
      "Deploys specialized AI agents for security, performance, style, and architecture reviews",
      "Analyzes pull requests using GitHub CLI integration",
      "Generates contextual review comments with severity levels",
      "Supports custom review agents and quality gate configuration",
      "Integrates with GitHub Actions for automated review workflows"
    ],
    "limitations": [
      "Requires GitHub CLI (gh) and ruv-swarm tools to be installed",
      "Cannot execute code or run tests directly",
      "Review quality depends on AI model capabilities and context provided",
      "Labels and comments require appropriate GitHub permissions"
    ],
    "use_cases": [
      {
        "target_user": "Development teams",
        "title": "Multi-agent security review",
        "description": "Deploy security-focused AI agents to analyze pull requests for vulnerabilities before merging"
      },
      {
        "target_user": "Open source maintainers",
        "title": "Automated PR quality gates",
        "description": "Set up automated review workflows that check code quality, style, and documentation standards"
      },
      {
        "target_user": "DevOps engineers",
        "title": "CI/CD review integration",
        "description": "Integrate AI-powered code review into GitHub Actions pipelines for continuous quality assurance"
      }
    ],
    "prompt_templates": [
      {
