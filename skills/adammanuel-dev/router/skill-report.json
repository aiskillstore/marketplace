  "content": {
    "user_title": "Route Requests to Optimal Tools",
    "value_statement": "Complex development tasks require choosing the right tool from many options. The Router skill intelligently analyzes your request and routes it to the best available Skill, Agent, or Command with transparent explanations.",
    "seo_keywords": [
      "Claude Code router",
      "tool routing",
      "request orchestration",
      "workflow automation",
      "intelligent routing",
      "Claude routing skill",
      "AI assistant routing",
      "multi-step workflows",
      "tool selection",
      "command routing"
    ],
    "actual_capabilities": [
      "Analyzes user intent from natural language requests",
      "Routes to optimal Skills, Agents, or Commands",
      "Coordinates multi-step sequential and parallel workflows",
      "Calculates confidence scores for routing decisions",
      "Handles ambiguous requests with clarification prompts",
      "Manages emergency fast-path routing for critical issues"
    ],
    "limitations": [
      "Does not execute code or modify files directly",
      "Depends on available tools being installed",
      "Cannot override user-specified tool preferences",
      "Routing accuracy depends on pattern matching rules"
    ],
    "use_cases": [
      {
        "target_user": "New Users",
        "title": "Learn the Ecosystem",
        "description": "New Claude Code users can describe what they want in natural language and get routed to the right tool automatically"
      },
      {
        "target_user": "Busy Developers",
        "title": "Save Time on Tool Selection",
        "description": "Experienced developers skip the discovery phase with intelligent routing that matches requests to specialized tools"
      },
      {
        "target_user": "Project Leads",
        "title": "Coordinate Complex Workflows",
        "description": "Multi-step workflows like feature development, security audits, and refactoring are orchestrated automatically"
      }
    ],
    "prompt_templates": [
      {
        "title": "Fix Issues",
        "scenario": "Code quality problems",
        "prompt": "fix my typescript errors in the auth module"
      },
      {
        "title": "Build Feature",
        "scenario": "New development work",
        "prompt": "build a dashboard component with user metrics"
      },
      {
        "title": "Review Code",
        "scenario": "Quality assurance",
        "prompt": "review my changes before I merge to main"
      },
      {
        "title": "Emergency Fix",
        "scenario": "Critical production issue",
        "prompt": "URGENT production is down with payment processing errors"
      }
    ],
    "output_examples": [
      {
        "input": "fix my typescript errors",
        "output": [
          "Routing to: /fix:types command",
          "Found 5 TypeScript errors across 2 files in your recent changes",
          "Executing fix now..."
        ]
      },
      {
        "input": "build a React dashboard",
        "output": [
          "Routing to: ui-engineer agent",
          "Based on your React component work, this agent is well-suited for building interactive UI",
