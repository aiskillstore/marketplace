  "content": {
    "user_title": "Generate Professional PR Descriptions",
    "value_statement": "Create comprehensive pull request descriptions that help reviewers understand changes quickly and improve code review quality. Generate structured templates for features, bug fixes, and refactors with checklists and context.",
    "seo_keywords": [
      "pull request templates",
      "PR description generator",
      "code review documentation",
      "git commit messages",
      "Claude Code",
      "Claude",
      "Codex",
      "developer documentation",
      "team collaboration",
      "software development"
    ],
    "actual_capabilities": [
      "Generate feature PR descriptions with structured templates",
      "Create bug fix documentation with root cause analysis",
      "Build refactoring PRs with before/after comparisons",
      "Add comprehensive checklists for reviewers",
      "Include breaking changes and migration guides",
      "Suggest related issues and reviewer assignments"
    ],
    "limitations": [
      "Does not access git repositories directly",
      "Does not execute git commands or analyze code",
      "User must provide change context manually",
      "Does not integrate with GitHub or GitLab APIs"
    ],
    "use_cases": [
      {
        "target_user": "Software developers",
        "title": "Document feature additions",
        "description": "Create detailed PR descriptions for new features with motivation, implementation details, and testing strategy."
      },
      {
        "target_user": "DevOps engineers",
        "title": "Track bug fix details",
        "description": "Document bug fixes with root cause analysis, impact assessment, and deployment considerations."
      },
      {
        "target_user": "Engineering leads",
        "title": "Review code refactors",
        "description": "Generate comprehensive refactor documentation with architecture changes and migration guides."
      }
    ],
    "prompt_templates": [
      {
        "title": "Simple PR",
        "scenario": "Quick feature PR",
        "prompt": "@pr-template-generator Generate a pull request description for a feature that adds user authentication with OAuth support."
      },
      {
        "title": "Bug Fix",
        "scenario": "Document bug resolution",
        "prompt": "@pr-template-generator --type bugfix Create a PR description for fixing a memory leak in the websocket handler."
      },
      {
        "title": "Refactor",
        "scenario": "Code restructuring",
        "prompt": "@pr-template-generator --type refactor Generate documentation for refactoring the payment processing module."
      },
      {
        "title": "Complete PR",
        "scenario": "Full documentation",
        "prompt": "@pr-template-generator --include-screenshots Create a comprehensive PR description for a UI overhaul."
      }
    ],
    "output_examples": [
      {
        "input": "Generate a PR description for a feature that adds email notifications",
        "output": [
          "Title: feat: Add email notification system for order updates",
          "Summary: Implements email notifications for order status changes",
          "Motivation: Users need email alerts when order status changes",
          "Changes: Added notification service, email templates, scheduled jobs",
          "Testing: Unit tests added, manual testing completed in staging",
          "Checklist: Code follows style guidelines, docs updated, no breaking changes"
        ]
      },
      {
        "input": "Create a bug fix PR for a login issue",
        "output": [
          "Title: fix: Resolve login redirect loop for OAuth users",
          "Summary: Fixes redirect issue when users login with Google OAuth",
          "Root Cause: Session cookie set before token validation completed",
          "Impact: 200+ users affected, cannot access the platform",
          "Testing: Verified fix with affected user accounts"
        ]
      },
      {
        "input": "Document a refactoring change",
        "output": [
          "Title: refactor: Extract payment processing to separate module",
          "Summary: Improves code organization by separating payment concerns",
          "Before: 1200 lines in single file with mixed concerns",
          "After: 12 files with clear separation of concerns",
          "Benefits: 87% test coverage, easier to maintain and extend"
        ]
      }
    ],
    "best_practices": [
      "Include the problem statement and solution approach in every description",
      "Add screenshots or diagrams for visual changes to the codebase",
      "Link related issues and explain breaking changes clearly to reviewers"
    ],
    "anti_patterns": [
      "Using vague titles like Update code or Fix stuff in PR descriptions",
      "Skipping testing information and checklist items for reviewers",
      "Assuming reviewers have full context of the changes without explanation"
    ],
    "faq": [
      {
        "question": "Which platforms support this skill?",
        "answer": "This skill works with Claude, Codex, and Claude Code. Use @pr-template-generator to invoke it."
      },
      {
        "question": "What types of PRs can this generate?",
        "answer": "It generates templates for features, bug fixes, refactoring, documentation updates, and performance improvements."
      },
      {
        "question": "Does it connect to GitHub or GitLab?",
        "answer": "No, this skill generates descriptions based on your input. It does not access repositories or APIs directly."
      },
      {
        "question": "Is my code or data shared anywhere?",
        "answer": "No. This skill runs entirely in your AI session. No data is transmitted to external servers."
      },
      {
        "question": "Can I customize the templates?",
        "answer": "Yes, you can extend the prompts with flags like --type, --include-screenshots, or --minimal to customize output."
      },
      {
        "question": "How is this different from GitHub PR templates?",
        "answer": "GitHub templates provide static forms. This skill generates dynamic, context-aware descriptions based on your specific changes."
      }
    ]
