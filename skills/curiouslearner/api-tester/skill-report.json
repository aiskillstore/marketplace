  "content": {
    "user_title": "Test APIs with comprehensive validation",
    "value_statement": "Manual API testing is time-consuming and error-prone. This skill provides complete API testing methodologies including security checks, performance testing, and automated test generation.",
    "seo_keywords": [
      "API testing",
      "REST API",
      "GraphQL testing",
      "automated testing",
      "Claude Code",
      "API validation",
      "Postman",
      "curl",
      "security testing",
      "performance testing"
    ],
    "actual_capabilities": [
      "Generate curl commands for any HTTP method",
      "Create Postman collections with test scripts",
      "Write Jest and pytest test suites",
      "Validate API responses against schemas",
      "Test authentication methods (JWT, OAuth, API keys)",
      "Generate load testing scripts with Artillery"
    ],
    "limitations": [
      "Does not execute actual API calls",
      "Requires manual implementation of generated tests",
      "No built-in test result reporting",
      "Cannot monitor API performance continuously"
    ],
    "use_cases": [
      {
        "target_user": "Backend developers",
        "title": "API endpoint validation",
        "description": "Generate comprehensive test suites for new API endpoints including edge cases and error scenarios."
      },
      {
        "target_user": "QA engineers",
        "title": "Automated testing setup",
        "description": "Create automated test scripts for regression testing and CI/CD integration."
      },
      {
        "target_user": "DevOps teams",
        "title": "Performance benchmarking",
        "description": "Generate load testing configurations to validate API performance under stress."
      }
    ],
    "prompt_templates": [
      {
        "title": "Basic API testing",
        "scenario": "Test a simple REST endpoint",
        "prompt": "Generate curl commands and test cases for GET /api/users endpoint with pagination parameters"
      },
      {
        "title": "Authentication testing",
        "scenario": "Test JWT authentication flow",
        "prompt": "Create test scripts for login endpoint that returns JWT token and subsequent authenticated requests"
      },
      {
        "title": "GraphQL validation",
        "scenario": "Test GraphQL queries and mutations",
        "prompt": "Generate GraphQL test cases for user queries and createUser mutation with proper error handling"
      },
      {
        "title": "Load testing",
        "scenario": "Performance test under load",
        "prompt": "Create Artillery configuration to test POST /api/orders with 100 concurrent users over 5 minutes"
      }
    ],
    "output_examples": [
      {
        "input": "Generate test cases for a user registration API with email validation",
        "output": [
          "Valid registration with proper email format",
          "Rejects duplicate email addresses",
          "Validates email format (missing @ symbol)",
          "Requires password minimum 8 characters",
          "Returns 201 status with user ID on success",
          "Returns 400 for missing required fields"
        ]
      },
      {
        "input": "Create Jest test suite for a login endpoint",
        "output": [
          "Successful login returns 200 with JWT token",
          "Invalid credentials return 401 unauthorized",
          "Missing password returns 400 bad request",
          "Rate limiting triggers after 5 failed attempts"
        ]
      },
      {
        "input": "Generate load test for health check endpoint",
        "output": [
          "1000 requests with 50 concurrent users",
          "Average response time under 200ms",
          "Success rate above 99.5 percent",
          "No timeout errors during 5 minute test"
        ]
      }
    ],
    "best_practices": [
      "Always test both success and error scenarios for complete coverage",
      "Use environment variables for API URLs and authentication tokens",
      "Implement proper test data cleanup after test execution"
    ],
    "anti_patterns": [
      "Never commit API keys or tokens to version control",
      "Avoid testing only happy path scenarios without error cases",
      "Do not skip authentication testing in public endpoints"
    ],
    "faq": [
      {
        "question": "Which authentication methods are supported?",
        "answer": "Bearer tokens, API keys, Basic auth, and OAuth 2.0 with examples for each method."
      },
      {
        "question": "Can I test GraphQL APIs?",
        "answer": "Yes, includes GraphQL query and mutation examples with variables and error handling."
      },
      {
        "question": "How do I integrate with CI/CD?",
        "answer": "Generates Jest and pytest test suites that can run in any CI/CD pipeline automatically."
      },
      {
        "question": "Is my API data safe?",
        "answer": "This skill only generates test code. It does not execute tests or access your actual APIs."
      },
      {
        "question": "What about rate limiting?",
        "answer": "Includes rate limit testing strategies and proper delay configurations for load tests."
      },
      {
        "question": "How does this compare to Postman?",
        "answer": "Generates Postman collections while also providing code-based tests for automation."
      }
