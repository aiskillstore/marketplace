  "content": {
    "user_title": "Find Code Faster with Intelligent Search Patterns",
    "value_statement": "Finding code in large projects wastes valuable development time. This skill teaches you advanced search techniques with grep and ripgrep for instant code discovery.",
    "seo_keywords": [
      "code search",
      "grep",
      "ripgrep",
      "pattern matching",
      "regex search",
      "Claude Code",
      "Codex",
      "semantic search",
      "developer productivity",
      "codebase navigation"
    ],
    "actual_capabilities": [
      "Translates natural language queries into search commands",
      "Provides regex patterns for JavaScript, Python, Go, and TypeScript",
      "Suggests filtering by file type and directory scope",
      "Teaches cross-reference and dependency tracking searches",
      "Identifies security and performance anti-patterns via search",
      "Optimizes search performance for large codebases"
    ],
    "limitations": [
      "Requires grep or ripgrep to be installed on your system",
      "Cannot search inside binary or compiled files",
      "Regex patterns may need adjustment for edge cases",
      "Does not replace IDE-integrated semantic analysis tools"
    ],
    "use_cases": [
      {
        "target_user": "Backend Developer",
        "title": "Find API Endpoints Quickly",
        "description": "Locate all Express, Flask, or Go HTTP handlers using smart regex patterns across your codebase"
      },
      {
        "target_user": "Security Engineer",
        "title": "Audit for Hardcoded Secrets",
        "description": "Search for API keys, passwords, and tokens that should not be in source code"
      },
      {
        "target_user": "Tech Lead",
        "title": "Track Technical Debt",
        "description": "Find all TODO comments, deprecated APIs, and code quality issues for sprint planning"
      }
    ],
    "prompt_templates": [
      {
        "title": "Find Functions",
        "scenario": "Locate specific function definitions",
        "prompt": "Search for all async functions in the src directory"
      },
      {
        "title": "Security Audit",
        "scenario": "Find potential security issues",
        "prompt": "Find all places where SQL queries use string concatenation"
      },
      {
        "title": "React Hook Usage",
        "scenario": "Track React patterns",
        "prompt": "Find all components using useState and useEffect hooks"
      },
      {
        "title": "Cross-Reference Analysis",
        "scenario": "Track dependencies",
        "prompt": "Find all files that import and use the AuthService class"
      }
    ],
    "output_examples": [
      {
        "input": "Find all TODO comments in the project",
        "output": [
          "Found 15 TODO comments across 7 files",
          "src/auth/login.ts:42 - TODO: Add rate limiting",
          "src/api/users.ts:18 - TODO: Validate email format",
          "src/utils/cache.ts:91 - FIXME: Handle cache expiry"
        ]
      },
      {
        "input": "Search for hardcoded API keys",
        "output": [
          "Potential secrets found in 2 files:",
          "config/dev.js:12 - Contains 'api_key' assignment",
          "src/services/payment.ts:5 - Contains 'secret' variable",
          "Recommendation: Move these to environment variables"
        ]
      }
    ],
    "best_practices": [
      "Always exclude node_modules, dist, and build directories from searches",
      "Use ripgrep for speed and automatic gitignore respect",
      "Start with broad searches and add filters progressively"
    ],
    "anti_patterns": [
      "Searching entire codebase without file type or directory filters",
      "Using case-sensitive search when matching identifiers of any case",
      "Including generated files and dependencies in search results"
    ],
    "faq": [
      {
        "question": "Which search tool is recommended?",
        "answer": "Ripgrep (rg) is fastest and respects .gitignore automatically. Use grep when ripgrep is not available."
      },
      {
        "question": "How do I search across multiple lines?",
        "answer": "Use ripgrep with -U flag for multiline mode: rg -U 'pattern.*\\n.*continuation'"
      },
      {
        "question": "Can I search for patterns in specific file types?",
        "answer": "Yes, use -t flag with ripgrep: rg 'pattern' -t js -t ts or --glob with grep."
      },
      {
        "question": "How do I exclude test files from results?",
        "answer": "Use glob exclusion: rg 'pattern' --glob '!**/*.test.*' --glob '!**/__tests__/**'"
      },
      {
        "question": "What languages does this skill support?",
        "answer": "The skill provides patterns for JavaScript, TypeScript, Python, Go, and any text-based language."
      },
      {
        "question": "How can I count matches instead of showing them?",
        "answer": "Use --count flag with ripgrep: rg 'TODO' --count shows match counts per file."
      }
    ]
