  "content": {
    "user_title": "Manage Python Virtual Environments",
    "value_statement": "Struggling with Python dependency conflicts and virtual environment setup? This skill provides expert guidance on creating, managing, and troubleshooting Python virtual environments using modern tools like Poetry, pipenv, and pyenv.",
    "seo_keywords": [
      "python virtual environment",
      "poetry python",
      "pipenv",
      "pyenv",
      "python dependencies",
      "venv",
      "virtualenv",
      "Claude Code",
      "Codex",
      "python package management"
    ],
    "actual_capabilities": [
      "Create and configure Python virtual environments",
      "Manage dependencies with Poetry, pip-tools, and pipenv",
      "Set up Python version management with pyenv",
      "Generate requirements.txt and pyproject.toml files",
      "Troubleshoot dependency conflicts and installation issues",
      "Configure development environments with testing and linting"
    ],
    "limitations": [
      "Requires existing Python installation on system",
      "Some tools (Poetry, pyenv) need separate installation",
      "Platform-specific commands may vary (Windows vs Linux/Mac)",
      "Cannot directly execute commands - provides instructions only"
    ],
    "use_cases": [
      {
        "target_user": "Python developers",
        "title": "New Project Setup",
        "description": "Create properly structured Python projects with virtual environments and dependency management from scratch."
      },
      {
        "target_user": "Development teams",
        "title": "Dependency Management",
        "description": "Standardize dependency management across team with lock files and reproducible environments."
      },
      {
        "target_user": "DevOps engineers",
        "title": "Environment Troubleshooting",
        "description": "Diagnose and fix Python environment issues, version conflicts, and broken dependencies."
      }
    ],
    "prompt_templates": [
      {
        "title": "Create New Project",
        "scenario": "Starting a Python project with Poetry",
        "prompt": "Help me set up a new Python project using Poetry with Django, pytest, and black formatter. Include proper project structure and configuration."
      },
      {
        "title": "Fix Dependencies",
        "scenario": "Resolving package conflicts",
        "prompt": "I'm getting dependency conflicts with requests and urllib3 versions. How do I resolve this using Poetry?"
      },
      {
        "title": "Migrate Project",
        "scenario": "Moving from requirements.txt to Poetry",
        "prompt": "I have an existing project using requirements.txt. How do I migrate it to Poetry while keeping all dependencies?"
      },
      {
        "title": "Multi-Python Setup",
        "scenario": "Managing multiple Python versions",
        "prompt": "I need to work with Python 3.9, 3.10, and 3.11 on different projects. Set up pyenv and configure each project with the right version."
      }
    ],
