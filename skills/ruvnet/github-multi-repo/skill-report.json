  "content": {
    "user_title": "Coordinate multi-repository GitHub workflows",
    "value_statement": "Managing multiple GitHub repositories requires coordinating changes, dependencies, and releases across repositories. This skill uses AI swarm orchestration to synchronize packages, standardize architecture, and coordinate cross-repository operations automatically.",
    "seo_keywords": [
      "GitHub multi-repo",
      "Claude Code skill",
      "repository coordination",
      "cross-repo synchronization",
      "package management",
      "swarm orchestration",
      "claude-flow",
      "AI agents",
      "Claude Code",
      "GitHub automation"
    ],
    "actual_capabilities": [
      "Coordinate changes across multiple GitHub repositories using AI swarm orchestration",
      "Synchronize package versions and dependencies across repositories",
      "Analyze and optimize repository architecture with automated templates",
      "Create standardized project templates for new repositories",
      "Deploy security patches and dependency updates across all repositories",
      "Generate cross-repository dependency graphs and health reports"
    ],
    "limitations": [
      "Requires GitHub CLI (gh) and claude-flow MCP server installation",
      "Cannot access private repositories without proper gh authentication",
      "Operates on repositories you have explicit access to via gh CLI",
      "Swarm operations depend on available MCP server resources"
    ],
    "use_cases": [
      {
        "target_user": "Platform engineers",
        "title": "Synchronize microservices",
        "description": "Keep shared libraries and API contracts consistent across all microservices in your organization"
      },
      {
        "target_user": "Release managers",
        "title": "Coordinate cross-repo releases",
        "description": "Deploy coordinated releases across multiple repositories with synchronized version bumps"
      },
      {
        "target_user": "Security teams",
        "title": "Deploy security patches",
        "description": "Apply security updates to all repositories and create automated pull requests"
      }
    ],
    "prompt_templates": [
      {
        "title": "Initialize coordination",
