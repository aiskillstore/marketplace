      "error handling automation",
      "workflow orchestration",
      "unite-hub"
    ],
    "actual_capabilities": [
      "Routes tasks through Truth Layer validation before execution",
      "Coordinates email-agent and content-agent workflows",
      "Maintains persistent memory for workflow state across runs",
      "Generates comprehensive system health and performance reports",
      "Handles errors at three levels: recoverable, significant, and critical",
      "Creates audit trails for all automated actions"
    ],
    "limitations": [
      "Requires other specialist agents (email-agent, content-agent) to function",
      "Designed specifically for Unite-Hub platform integration",
      "Cannot operate without database connectivity to Convex",
      "Does not execute actions outside defined workflow patterns"
    ],
    "use_cases": [
      {
        "target_user": "Marketing Managers",
        "title": "Automated Lead Nurturing",
        "description": "Process incoming emails, score leads, and generate personalized follow-up content automatically."
      },
      {
        "target_user": "Agency Owners",
        "title": "Content Approval Pipeline",
        "description": "Review and approve generated content drafts while maintaining compliance audit trails."
      },
