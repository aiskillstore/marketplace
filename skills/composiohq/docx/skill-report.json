  "content": {
    "user_title": "Create and edit Word documents with tracked changes",
    "value_statement": "This skill enables Claude to create, edit, and analyze Word documents. It handles tracked changes, comments, and formatting preservation for professional document workflows. Perfect for legal document review, academic editing, and business document automation.",
    "seo_keywords": [
      "docx skill",
      "Word document",
      "Claude Code",
      "tracked changes",
      "document editing",
      "OOXML",
      "document analysis",
      "create docx",
      "edit Word",
      "Pandoc"
    ],
    "actual_capabilities": [
      "Create new Word documents using docx-js library",
      "Edit existing documents with tracked changes (insertions and deletions)",
      "Add and manage comments in documents",
      "Extract text and raw XML content from documents",
      "Validate document structure against OOXML schemas",
      "Convert documents to markdown for analysis"
    ],
    "limitations": [
      "Cannot edit password-protected or encrypted documents",
      "Limited support for complex embedded objects and OLE elements",
      "Requires system dependencies (pandoc, LibreOffice) for full functionality"
    ],
    "use_cases": [
      {
        "target_user": "Legal professionals",
        "title": "Review contracts with tracked changes",
        "description": "Edit legal documents while preserving revision history for compliance and audit trails"
      },
      {
        "target_user": "Academic researchers",
        "title": "Collaborate on papers with edits",
        "description": "Review and edit research papers with clear change tracking for collaborative writing"
      },
      {
        "target_user": "Business analysts",
        "title": "Automate document updates",
        "description": "Batch update business documents with consistent formatting and change documentation"
      }
    ],
    "prompt_templates": [
      {
        "title": "Create new document",
        "scenario": "Starting a new Word doc",
        "prompt": "Create a new Word document with a title, three paragraphs of introduction text, and a numbered list with three items"
      },
      {
        "title": "Edit with tracked changes",
        "scenario": "Reviewing a contract",
        "prompt": "Review the contract in contract.docx and change all occurrences of '30 days' to '60 days' using tracked changes"
      },
      {
        "title": "Extract document content",
        "scenario": "Analyzing a document",
        "prompt": "Extract all text from quarterly_report.docx and convert it to markdown format"
      },
      {
        "title": "Complex multi-batch edits",
        "scenario": "Large document review",
        "prompt": "Review and edit the entire policy document using the redlining workflow. Make date corrections in batch 1, party name updates in batch 2, and terminology changes in batch 3"
      }
    ],
    "output_examples": [
      {
        "input": "Create a new Word document with a title 'Project Proposal' and three bullet points about main deliverables",
        "output": [
          "Created document with title formatting",
          "Added three paragraphs with bullet points",
          "Document saved as output.docx"
        ]
      },
      {
        "input": "Change 'client' to 'customer' throughout proposal.docx using tracked changes",
        "output": [
          "Found 12 occurrences of 'client'",
          "Replaced all with 'customer' using tracked changes",
          "Added author and timestamp to all revisions"
        ]
      },
      {
        "input": "Extract text from the document and convert to markdown",
        "output": [
          "Extracted 1500 words from document",
          "Preserved heading structure",
          "Generated markdown file with --track-changes=reject"
        ]
      }
    ],
    "best_practices": [
      "Always use the redlining workflow for documents you did not create to ensure proper revision tracking",
      "Group related changes into batches of 3-10 edits for easier debugging and review",
      "Extract RSID values from unpacked documents and use them in your edits to maintain document integrity"
    ],
    "anti_patterns": [
      "Do not use markdown line numbers to find text - they do not map to XML structure",
      "Avoid marking unchanged text as deleted and re-inserted - this creates unnecessary revision history",
      "Do not skip the unpacking step - always work with unpacked XML for reliable edits"
    ],
    "faq": [
      {
        "question": "What file formats are supported?",
        "answer": "DOCX, PPTX, and XLSX files are supported for reading, editing, and validation."
      },
      {
        "question": "How are tracked changes implemented?",
        "answer": "Tracked changes use OOXML w:ins (insertions) and w:del (deletions) tags with RSID values for tracking."
      },
      {
        "question": "What dependencies are required?",
        "answer": "Pandoc for text extraction, LibreOffice for validation, and defusedxml for secure XML parsing."
      },
      {
        "question": "Can I edit encrypted documents?",
        "answer": "No, password-protected documents cannot be edited due to encryption constraints."
      },
      {
        "question": "How does the redlining workflow work?",
        "answer": "Convert to markdown, plan changes in batches, unpack document, implement edits in Python, then repack."
      },
      {
        "question": "What RSID values should I use?",
        "answer": "Use the RSID suggested by the unpack script, or copy RSIDs from existing elements in the document."
      }
    ]
  },
