  "content": {
    "user_title": "Manage Jira Issues from Command Line",
    "value_statement": "Manage Jira issues without leaving your terminal. This skill provides complete command-line documentation for creating, editing, tracking, and transitioning Jira issues, sprints, and epics.",
    "seo_keywords": [
      "jira cli",
      "jira command line",
      "jira automation",
      "issue tracking cli",
      "jira issue management",
      "project management",
      "Claude",
      "Claude Code",
      "developer tools",
      "sprint management"
    ],
    "actual_capabilities": [
      "Create, list, view, edit, and delete Jira issues",
      "Transition issues through workflow states",
      "Manage sprints, epics, and boards",
      "Assign issues and add comments",
      "Log work time on issues",
      "Generate plain text, JSON, or CSV output"
    ],
    "limitations": [
      "Requires separate installation of jira-cli tool",
      "Needs JIRA_API_TOKEN environment variable configured",
      "Requires initial configuration with jira init command",
      "Some features vary between Jira Cloud and Jira Server"
    ],
    "use_cases": [
      {
        "target_user": "Software developers",
        "title": "Quick issue updates",
        "description": "Create and update Jira issues without switching to a browser or leaving your IDE"
      },
      {
        "target_user": "Project managers",
        "title": "Sprint tracking",
        "description": "View sprint progress, list assigned issues, and manage team workloads from terminal"
      },
      {
        "target_user": "DevOps engineers",
        "title": "CI/CD integration",
        "description": "Update Jira issues automatically from deployment scripts and pipeline workflows"
      }
    ],
    "prompt_templates": [
      {
        "title": "Create a bug",
        "scenario": "Report new bug",
        "prompt": "Create a high priority bug titled 'Login fails on mobile' with description 'Users cannot authenticate on iOS devices'"
      },
      {
        "title": "List my tasks",
        "scenario": "Daily standup prep",
        "prompt": "Show all Jira issues assigned to me that are in progress or to do status"
      },
      {
        "title": "Complete an issue",
        "scenario": "Mark work done",
        "prompt": "Move issue PROJ-123 to Done status and log 2 hours of work with comment 'Fixed authentication bug'"
      },
      {
        "title": "Sprint overview",
        "scenario": "Team sync",
        "prompt": "List all issues in the current sprint and show their status in table format"
      }
    ],
    "output_examples": [
      {
        "input": "List my high priority issues in plain text",
        "output": [
          "PROJ-101 Fix payment processing bug",
          "PROJ-105 Security vulnerability in auth module",
          "PROJ-108 Database connection timeout errors"
        ]
      },
      {
        "input": "Create a bug and start working on it",
        "output": [
          "Created issue BUG-456: App crashes on startup",
          "Assigned BUG-456 to current user",
          "Moved BUG-456 to In Progress status"
        ]
      },
      {
        "input": "Show current sprint issues",
        "output": [
          "SPRINT-42 Current Sprint Issues:",
          "PROJ-100 [In Progress] User authentication flow",
          "PROJ-101 [To Do] Payment integration",
          "PROJ-102 [Done] Database migration"
        ]
      }
    ],
    "best_practices": [
      "Run jira init first to configure project settings and avoid repeated prompts",
      "Use --plain flag for scripting to get predictable, machine-parseable output",
      "Store JIRA_API_TOKEN securely in your shell profile or environment manager"
    ],
    "anti_patterns": [
      "Never share or commit your JIRA_API_TOKEN to version control systems",
      "Avoid interactive mode in automated scripts - use explicit flags instead",
      "Do not rely on implicit project context - specify project explicitly in commands"
    ],
    "faq": [
      {
        "question": "What Jira versions are supported?",
        "answer": "Works with Jira Cloud and most Jira Server instances. Some advanced features may vary between versions."
      },
      {
        "question": "How do I install jira-cli?",
        "answer": "Use brew install ankitpokhrel/jira-cli/jira-cli on macOS, or download binaries from the GitHub releases page."
      },
      {
        "question": "Can I use this in CI/CD pipelines?",
        "answer": "Yes, use --plain or --raw flags for script-friendly output that integrates with automation tools."
      },
      {
        "question": "Is my Jira data secure with this tool?",
        "answer": "Yes, jira-cli uses HTTPS for all API calls and your API token for authentication. This skill only provides documentation."
      },
      {
        "question": "What output formats are available?",
        "answer": "Default interactive UI, --plain for text, --raw for JSON, and --csv for spreadsheet-compatible output."
      },
      {
        "question": "How do I manage sprints from command line?",
        "answer": "Use jira sprint list --current to view active sprint, then jira sprint add to add issues to the sprint."
      }
    ]
