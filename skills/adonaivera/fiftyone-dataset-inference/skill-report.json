  "content": {
    "user_title": "Create FiftyOne Datasets with Inference",
    "value_statement": "Loading local media files into FiftyOne and running ML model inference requires understanding dataset creation, label imports, and model application. This skill provides step-by-step guidance for creating datasets, importing labels in standard formats, and applying zoo models for detection, classification, and segmentation tasks.",
    "seo_keywords": [
      "fiftyone",
      "dataset creation",
      "model inference",
      "computer vision",
      "COCO",
      "YOLO",
      "VOC",
      "object detection",
      "image classification",
      "Claude",
      "Codex",
      "Claude Code"
    ],
    "actual_capabilities": [
      "Create FiftyOne datasets from media directories (images, videos, point clouds)",
      "Import labeled datasets in COCO, YOLO, VOC, and other formats",
      "Apply zoo models for detection, classification, segmentation, and embeddings",
      "Build end-to-end ML inference pipelines with validation steps",
      "Launch FiftyOne App for interactive visualization",
      "Use directory structure for image classification datasets"
    ],
    "limitations": [
      "Requires FiftyOne MCP server to be installed and running",
      "Requires specific plugins (@voxel51/io, @voxel51/zoo, @voxel51/utils)",
      "Does not include model weights or training capabilities",
      "Cannot modify existing datasets without delete and recreate"
    ],
    "use_cases": [
      {
        "target_user": "ML Engineers",
        "title": "Load Images and Detect Objects",
        "description": "Create dataset from image directory and apply YOLOv8 or Faster R-CNN for object detection"
      },
      {
        "target_user": "Data Scientists",
        "title": "Import COCO Dataset",
        "description": "Import existing COCO-formatted dataset and add model predictions for comparison with ground truth"
      },
      {
        "target_user": "CV Researchers",
        "title": "YOLO Dataset Evaluation",
        "description": "Import YOLOv5 dataset format and evaluate model performance with FiftyOne visualization"
      }
    ],
    "prompt_templates": [
      {
        "title": "Load Images Only",
        "scenario": "Create dataset without labels",
        "prompt": "Use the fiftyone-dataset-inference skill to create a dataset from /path/to/images. Load only media files, no labels. Name the dataset my-images. Then apply faster-rcnn-resnet50-fpn-coco-torch model for object detection."
      },
      {
        "title": "Import with Labels",
        "scenario": "Import COCO format dataset",
        "prompt": "Use the fiftyone-dataset-inference skill to import a COCO dataset from /path/to/images and /path/to/annotations.json. Create dataset called coco-dataset with ground_truth field. Then apply yolov8n-coco-torch for predictions."
      },
      {
        "title": "Directory Classification",
        "scenario": "Folder-per-class structure",
        "prompt": "Use the fiftyone-dataset-inference skill to create an image classification dataset from /path/to/dataset. The structure is folder-per-class (cats/, dogs/). Name the dataset classification-dataset with ground_truth field."
      },
      {
        "title": "Custom Model",
        "scenario": "Apply specific zoo model",
        "prompt": "Use the fiftyone-dataset-inference skill to create a dataset called test-set from /path/to/images. Apply clip-vit-base32-torch model for embeddings. Launch the app to visualize results."
      }
    ],
    "output_examples": [
