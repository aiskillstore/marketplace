  "content": {
    "user_title": "Query NotebookLM notebooks from Claude Code",
    "value_statement": "Finding answers across many NotebookLM notebooks is slow and repetitive. This skill routes NotebookLM queries and manages notebook entries through a guided Chrome automation flow.",
    "seo_keywords": [
      "Claude",
      "Codex",
      "Claude Code",
      "NotebookLM",
      "notebook query",
      "Chrome automation",
      "knowledge base",
      "research assistant",
      "notebook management",
      "source citations"
    ],
    "actual_capabilities": [
      "Detect NotebookLM requests and extract notebook ID and question.",
      "Look up notebook URLs from a local registry file.",
      "Trigger a Chrome-connected subagent to submit NotebookLM queries.",
      "Ask once per notebook whether to clear chat history.",
      "Return responses with a structured answer and citation section."
    ],
    "limitations": [
      "Requires the Claude in Chrome extension and an active browser session.",
      "Cannot browse general websites or local files outside the notebook registry.",
      "Relies on NotebookLM availability and user authentication in Chrome.",
      "Does not validate or correct NotebookLM content beyond returned text."
    ],
    "use_cases": [
      {
        "title": "Research recall",
        "description": "Ask a specific NotebookLM notebook a targeted question and receive a cited answer.",
        "target_user": "Researcher"
      },
      {
        "title": "Product brief lookup",
        "description": "Query a product notes notebook for a feature detail before a meeting.",
        "target_user": "Product manager"
      },
      {
        "title": "Support knowledge check",
        "description": "Search a troubleshooting notebook for a known fix and confirm context.",
        "target_user": "Support lead"
      }
    ],
    "prompt_templates": [
      {
        "title": "List available notebooks",
        "prompt": "List my NotebookLM notebooks and show their IDs.",
        "scenario": "Beginner"
      },
      {
        "title": "Query a notebook by ID",
        "prompt": "Query notebook claude-docs about rate limits and summarize the answer with citations.",
        "scenario": "Intermediate"
      },
      {
        "title": "Add a new notebook URL",
        "prompt": "Add this NotebookLM URL to my registry and name it onboarding-notes: https://notebooklm.google.com/notebook/XXXX.",
        "scenario": "Intermediate"
      },
      {
        "title": "Fill coverage gaps",
        "prompt": "Ask my notebook about onboarding policies, then check for missing topics from my question and follow up if needed.",
        "scenario": "Advanced"
      }
    ],
    "output_examples": [
      {
        "input": "Query notebook onboarding-notes about vacation policy.",
        "output": "Notebook: Onboarding Notes (onboarding-notes). Answer: The policy allows 15 days per year with manager approval. Citations: Employee Handbook, section 4."
      },
      {
