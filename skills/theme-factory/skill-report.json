  "content": {
    "user_title": "Apply a professional theme to your artifact",
    "value_statement": "You need consistent visual styling across documents and presentations. This skill provides 10 curated themes and can generate custom themes based on your requirements. Apply matching colors and fonts to slides, docs, and reports.",
    "seo_keywords": [
      "theme palettes",
      "presentation styling",
      "color schemes",
      "font pairing",
      "Claude",
      "Codex",
      "Claude Code",
      "slide deck design",
      "branding themes",
      "visual identity"
    ],
    "actual_capabilities": [
      "Lists 10 preset themes with names, descriptions, and use cases",
      "Provides complete color palettes with hex codes for each theme",
      "Specifies header and body fonts per theme for typography consistency",
      "Shows a visual theme showcase PDF for comparing options",
      "Generates custom themes based on natural language descriptions"
    ],
    "limitations": [
      "Does not automatically edit files without a host tool",
      "Theme showcase is a static PDF that requires manual viewing",
      "Font availability depends on the user environment and installed fonts",
      "Themes are limited to the provided palettes and font pairings"
    ],
    "use_cases": [
      {
        "target_user": "Marketing manager",
        "title": "Brand consistent decks",
        "description": "Select a theme and apply matching colors and fonts across pitch decks for consistent branding."
      },
      {
        "target_user": "Product designer",
        "title": "Quick visual direction",
        "description": "Compare theme options and choose a palette that fits a new landing page or design project."
      },
      {
        "target_user": "Consultant",
        "title": "Polished client reports",
        "description": "Apply a professional theme to client reports for consistent and polished delivery."
      }
    ],
    "prompt_templates": [
      {
        "title": "Show available themes",
        "scenario": "Explore theme options",
        "prompt": "Show the theme showcase and list all available themes with brief descriptions of each."
      },
      {
        "title": "Apply preset theme",
        "scenario": "Use a known theme",
        "prompt": "Apply the Ocean Depths theme to my slide deck with its specified colors and fonts."
      },
      {
        "title": "Compare themes",
        "scenario": "Need recommendation",
        "prompt": "Compare Golden Hour and Modern Minimalist for a finance report and recommend the best option."
      },
      {
        "title": "Create custom theme",
        "scenario": "No preset fits",
        "prompt": "Create a custom theme for a wellness brand with calm colors and readable fonts, then show it for review."
      }
    ],
    "output_examples": [
      {
        "input": "Apply the Forest Canopy theme to my report.",
        "output": [
          "Selected theme: Forest Canopy",
          "Primary colors: Forest Green #2d4a2b, Sage #7d8471",
          "Background colors: Olive #a4ac86, Ivory #faf9f6",
          "Typography: Headers FreeSerif Bold, Body FreeSans"
        ]
      },
      {
        "input": "Create a custom theme for a tech startup.",
        "output": [
          "Generated theme: Tech Innovation (similar style)",
          "Primary colors: Navy #0f172a, Electric Blue #3b82f6",
          "Accent colors: Cyan #06b6d4, Silver #94a3b8",
          "Typography: Headers Roboto Bold, Body Inter",
          "Best for: Pitch decks, product launches, corporate presentations"
        ]
      }
    ],
    "best_practices": [
      "Ask the user to view the theme showcase before making a selection",
      "Confirm the selected theme name before applying it to ensure accuracy",
      "Check contrast and readability after applying colors to verify accessibility"
    ],
    "anti_patterns": [
      "Applying a theme without user confirmation or explicit selection",
      "Ignoring the specified font pairings that complement each theme",
      "Modifying the theme palette outside the defined hex colors"
    ],
    "faq": [
      {
        "question": "Is this compatible with any document type?",
        "answer": "It provides colors and fonts that can be applied to slides, docs, and reports in most editing tools."
      },
      {
        "question": "What are the limits of the preset themes?",
        "answer": "There are 10 curated preset themes. Custom themes can be generated on request using natural language."
      },
      {
        "question": "How does it integrate with my tools?",
        "answer": "It outputs color palettes and font specifications. Your editor or workflow applies them to artifacts."
      },
      {
        "question": "Does it store or send my data?",
        "answer": "No. The skill contains only static theme definitions and operates locally without data transmission."
      },
      {
        "question": "What if the fonts are missing?",
        "answer": "Install the listed fonts or substitute close alternatives with similar weight and style characteristics."
      },
      {
        "question": "How does it compare to generic theming?",
        "answer": "It provides curated, professional color palettes with intentional font pairings and use case guidance."
      }
    ]
