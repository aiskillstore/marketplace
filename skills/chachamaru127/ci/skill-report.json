    "user_title": "Diagnose CI/CD pipeline failures",
    "value_statement": "CI/CD pipelines fail frequently and debugging takes time. This skill analyzes failure logs, identifies root causes, and provides specific fix recommendations for build and test errors.",
    "seo_keywords": [
      "CI/CD debugging",
      "GitHub Actions troubleshooting",
      "pipeline failure fix",
      "CI failure analysis",
      "build error resolution",
      "test failure fix",
      "claude code ci",
      "claude ci/cd",
      "codex ci pipeline"
    ],
    "actual_capabilities": [
      "Analyzes CI/CD failure logs from GitHub Actions and GitLab CI",
      "Identifies error patterns including TypeScript, npm, and Jest errors",
      "Provides step-by-step fix instructions for failing tests",
      "Routes between analysis and fix workflows based on user intent",
      "Includes error pattern dictionaries for quick reference"
    ],
    "limitations": [
      "Cannot access private CI systems without gh CLI authentication",
      "Cannot modify CI configuration files directly",
      "Cannot guarantee fix success for complex multi-file issues",
      "Requires user approval for test file modifications"
    ],
    "use_cases": [
      {
        "target_user": "DevOps engineers",
        "title": "Analyze CI failure logs",
        "description": "Extract and interpret GitHub Actions logs to identify root causes of build failures"
      },
      {
        "target_user": "QA engineers",
        "title": "Fix failing tests",
        "description": "Diagnose why tests fail and receive specific guidance on correcting test or implementation code"
      },
      {
        "target_user": "Developers",
        "title": "Understand build errors",
        "description": "Get clear explanations of TypeScript, npm, and dependency errors with fix recommendations"
      }
    ],
    "prompt_templates": [
      {
        "title": "Check recent failures",
        "scenario": "View CI status",
        "prompt": "Show me the recent CI runs and why the latest one failed"
      },
      {
        "title": "Analyze error logs",
        "scenario": "Debug build failure",
        "prompt": "Look at the GitHub Actions log and tell me why the build is failing"
      },
      {
        "title": "Fix test failures",
        "scenario": "Debug test error",
        "prompt": "The CI test is failing. Find the failing test and suggest how to fix it"
      },
      {
        "title": "Full CI diagnosis",
        "scenario": "Complete troubleshooting",
        "prompt": "Analyze the CI failure, identify the root cause, and provide a specific fix for the issue"
      }
    ],
    "output_examples": [
      {
        "input": "The CI failed on the latest commit. Can you check why?",
        "output": [
          "CI FAILURE SUMMARY",
          "Run: #123 - build-and-test",
          "Failed step: unit-tests",
