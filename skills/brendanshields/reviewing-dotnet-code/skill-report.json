  "content": {
    "user_title": "Review .NET Code for Microsoft Conventions",
    "value_statement": "Manually reviewing .NET code for naming conventions and patterns is time-consuming and error-prone. This skill provides automated checks for PascalCase naming, modern C# patterns, async best practices, and common anti-patterns.",
    "seo_keywords": [
      ".NET code review",
      "C# conventions",
      "Microsoft coding standards",
      "Claude code review",
      "C# code analysis",
      ".NET patterns",
      "C# best practices",
      "code review automation",
      "ASP.NET review",
      "dotnet format"
    ],
    "actual_capabilities": [
      "Check naming conventions (PascalCase, camelCase, underscore prefixes)",
      "Identify outdated patterns (String vs string, List<T> vs new())",
      "Flag async anti-patterns (async void, blocking calls)",
      "Review exception handling patterns",
      "Suggest modern C# features (records, file-scoped namespaces)",
      "Apply CancellationToken and ConfigureAwait patterns"
    ],
    "limitations": [
      "Does not execute or test code",
      "Does not check for logic errors or bugs",
      "Does not validate XML documentation comments",
      "Does not analyze performance or security issues"
    ],
    "use_cases": [
      {
        "target_user": ".NET Developers",
        "title": "Code Review Assistant",
        "description": "Get automated feedback on C# code following Microsoft conventions before commits or PRs"
      },
      {
        "target_user": "Legacy Teams",
        "title": "Modernization Guide",
        "description": "Refactor older .NET codebases to modern C# 10+ patterns with clear before/after examples"
      },
      {
        "target_user": "New Developers",
        "title": "Learning Tool",
        "description": "Learn proper .NET naming and patterns while writing code with immediate feedback"
      }
    ],
    "prompt_templates": [
      {
        "title": "Quick Review",
        "scenario": "Review a C# file",
        "prompt": "Review this C# code for naming conventions and Microsoft patterns: [paste code]"
      },
      {
        "title": "Refactor Request",
        "scenario": "Modernize .NET code",
        "prompt": "Refactor this code to use modern C# 10+ features: [paste code]"
      },
      {
        "title": "Pattern Check",
        "scenario": "Check async patterns",
        "prompt": "Check this code for async/await anti-patterns: [paste code]"
      },
      {
        "title": "Best Practices",
        "scenario": "Generate service class",
        "prompt": "Generate a CustomerService class following Microsoft conventions with ILogger and repository pattern"
      }
    ],
    "output_examples": [
      {
        "input": "Review this code: public class customerService { private String strName; public async void GetData() { var result = GetAsync().Result; } }",
