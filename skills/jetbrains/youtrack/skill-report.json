    "user_title": "Manage YouTrack tickets for IdeaVim",
    "value_statement": "Tracking VIM tickets is slow when you forget YouTrack commands. This skill gives clear command guidance for comments, tags, status, and releases.",
    "seo_keywords": [
      "YouTrack",
      "IdeaVim",
      "issue tracking",
      "ticket comments",
      "workflow tags",
      "status updates",
      "release management",
      "Claude",
      "Codex",
      "Claude Code"
    ],
    "actual_capabilities": [
      "Lists commands to add public or private comments to VIM tickets",
      "Lists commands to add or remove tags by tag ID",
      "Lists commands to set ticket status values",
      "Lists commands to get ticket details",
      "Lists commands to set fix versions and manage release versions"
    ],
    "limitations": [
      "Requires a valid YOUTRACK_TOKEN environment variable",
      "Depends on external CLI scripts not included in this folder",
      "Applies to the IdeaVim YouTrack project and VIM ticket keys only",
      "Does not support GitHub Issues workflows"
    ],
    "use_cases": [
      {
        "target_user": "Maintainer",
        "title": "Update ticket status",
        "description": "Move VIM tickets to Ready To Release after verification"
      },
      {
        "target_user": "Release manager",
        "title": "Manage release versions",
        "description": "Create or delete YouTrack versions for IdeaVim releases"
      },
      {
        "target_user": "Contributor",
        "title": "Post review comments",
        "description": "Add public or private comments requesting review on VIM tickets"
      }
    ],
    "prompt_templates": [
      {
        "title": "Add a public comment",
        "scenario": "Beginner: comment on a ticket",
        "prompt": "Add a public comment to VIM-1234 that says the issue reproduces in 2.28.0"
      },
      {
        "title": "Tag a ticket",
        "scenario": "Intermediate: mark analysis status",
        "prompt": "Add the claude-analyzed tag to VIM-1234 and confirm the tag ID used"
      },
      {
        "title": "Set ticket status",
        "scenario": "Intermediate: move to release",
        "prompt": "Set the status of VIM-1234 to Ready To Release"
      },
      {
        "title": "Prepare a release",
        "scenario": "Advanced: manage versions",
        "prompt": "Create a YouTrack version 2.29.0 and set VIM-1234 fix version to 2.29.0"
      }
    ],
    "output_examples": [
      {
        "input": "Add a private comment to VIM-1234 asking Aleksei.Plate for clarification",
        "output": [
          "Use the add-comment command with the --private flag",
          "Target ticket VIM-1234",
          "Mention Aleksei.Plate in the comment text"
        ]
      },
      {
        "input": "Mark VIM-5678 as analyzed by Claude",
        "output": [
          "Use the add-tag command with tag ID 68-507461",
          "Run: npx tsx scripts-ts/src/youtrack-cli/add-tag.ts VIM-5678 68-507461"
        ]
      },
      {
        "input": "Set VIM-9999 fix version to 2.30.0",
        "output": [
          "Use the set-fix-version command",
          "Run: npx tsx scripts-ts/src/youtrack-cli/set-fix-version.ts VIM-9999 2.30.0"
        ]
      }
    ],
    "best_practices": [
      "Confirm the VIM ticket key before running any command",
      "Use private comments for internal team requests only",
      "Reuse documented tag IDs to avoid mistakes and typos"
    ],
