    "use_cases": [
      {
        "target_user": "Team leads",
        "title": "Standardize code review workflows",
        "description": "Create specialized reviewer agents that run automatically after code changes for consistent quality checks."
      },
      {
        "target_user": "Developers",
        "title": "Debug complex issues faster",
        "description": "Dispatch debugger agents with structured workflows for root cause analysis across multiple files."
      },
      {
        "target_user": "Architects",
        "title": "Parallel research and synthesis",
        "description": "Launch multiple research agents simultaneously and synthesize findings into comprehensive architectural reports."
      }
    ],
    "prompt_templates": [
      {
        "title": "Basic Agent Definition",
        "scenario": "Create a simple file-based agent",
        "prompt": "Help me create a file-based agent in .claude/agents/ for [task type]. Include name, description with PROACTIVELY trigger, and basic tool permissions."
      },
      {
        "title": "Task Dispatch",
        "scenario": "Dispatch an on-demand agent",
        "prompt": "Show me how to use the Task tool to dispatch a [model] agent for [task]. What parameters do I need?"
      },
      {
        "title": "Custom Agent Persona",
        "scenario": "Define agent with constraints",
        "prompt": "Create a complete agent definition for [role] including role definition, hard rules, workflow phases, and output format expectations."
      },
      {
        "title": "Parallel Execution",
        "scenario": "Run multiple agents at once",
        "prompt": "Set up parallel execution of [number] [model] agents to analyze [component]. Include synthesis step to combine findings."
      }
    ],
