    "use_cases": [
      {
        "target_user": "AI practitioners",
        "title": "Build Custom AI Assistants",
        "description": "Create specialized Claude Code agents for specific tasks like code review, debugging, or documentation generation."
      },
      {
        "target_user": "Development teams",
        "title": "Standardize Agent Workflows",
        "description": "Establish consistent agent patterns across your team with proper structure, tool access, and validation."
      },
      {
        "target_user": "Plugin developers",
        "title": "Create Publishable Agents",
        "description": "Build agents that conform to schema requirements and can be distributed through the skill marketplace."
      }
    ],
    "prompt_templates": [
      {
        "title": "Create New Agent",
        "scenario": "Need a new Claude Code agent",
        "prompt": "Create a Claude Code agent for code review that checks for security vulnerabilities and performance issues."
      },
      {
        "title": "Fix Validation Errors",
        "scenario": "Agent failing schema validation",
        "prompt": "My agent file is failing validation. Check the frontmatter and content format against the schema requirements."
      },
      {
        "title": "Configure Tools",
        "scenario": "Setting up agent permissions",
        "prompt": "Show me how to configure an agent with read-only access and git commands only using the allowed-tools field."
      },
      {
        "title": "Model Selection",
        "scenario": "Choosing the right model",
        "prompt": "Should I use sonnet, opus, or haiku for an agent that analyzes architecture decisions? Explain the trade-offs."
      }
    ],
    "output_examples": [
      {
        "input": "Create a Claude Code agent for testing TypeScript code",
        "output": [
          "Here is the agent structure for a TypeScript test generator:",
          "",
          "1. Frontmatter with required fields:",
          "   - name: test-generator",
          "   - description: Generates TypeScript tests",
          "   - allowed-tools: Read, Write, Grep, Bash",
          "   - model: sonnet",
          "",
          "2. Place the file at: .claude/agents/test-generator.md",
          "",
          "3. Key elements needed: H1 heading with emoji, 'You are...' persona, step-by-step process for test generation."
        ]
      },
      {
        "input": "How do I restrict an agent to only git commands?",
        "output": [
          "Use Bash tool restrictions in the allowed-tools field:",
          "",
          "allowed-tools: Bash(git *), Read",
          "",
          "This syntax limits the agent to git commands only. The pattern Bash(git *) allows any git subcommand."
        ]
      },
      {
        "input": "Why is my agent not activating?",
        "output": [
          "Check these common issues:",
          "",
          "1. File location: Must be in .claude/agents/ directory",
          "2. Frontmatter: Must have valid YAML with name and description",
          "3. Name pattern: Must use lowercase letters, numbers, and hyphens only",
          "4. Description: Must clearly explain when to use the agent",
          "5. H1 heading: Content must start with # heading",
          "6. Persona: Must include 'You are...' after the H1"
        ]
      }
    ],
    "best_practices": [
      "Define a clear, specific persona immediately after the H1 heading using 'You are...' format",
      "Grant minimal tool access following the principle of least privilege",
      "Include concrete examples showing both good and bad patterns for your agent's domain"
    ],
    "anti_patterns": [
      "Using underscores or uppercase letters in agent names violates schema validation",
      "Granting all tools to every agent creates unnecessary security risks",
      "Vague agent descriptions like 'A helpful agent' prevent proper activation"
    ],
    "faq": [
      {
        "question": "What file path do agents require?",
        "answer": "Agents must be placed in .claude/agents/ directory as markdown files with .md extension."
      },
      {
        "question": "What fields are required in frontmatter?",
        "answer": "The name and description fields are required. Name must use lowercase letters, numbers, and hyphens only."
      },
      {
        "question": "How do I restrict which tools an agent can use?",
        "answer": "Use the allowed-tools field with comma-separated tool names. For Bash, use patterns like Bash(git *) to restrict commands."
      },
      {
        "question": "Is my data safe when using agents?",
        "answer": "Agents only access files and tools you explicitly grant. This skill is read-only documentation that does not access your data."
      },
      {
        "question": "Why is my agent not activating?",
        "answer": "Check that the file is in .claude/agents/, frontmatter is valid YAML, and description specifically matches your use case."
      },
      {
        "question": "How is this different from skills or slash commands?",
        "answer": "Agents have persistent context and tool access for complex workflows. Skills provide context-aware documentation. Slash commands are for quick manual invocations."
      }
    ]
